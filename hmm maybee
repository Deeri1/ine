    [2] = "All"   
}

 
wait(1.5)
local args = {
    [1] = "[ ??? ] Evenly with your rejection.",
    [2] = "All"   
}

 
wait(2.8)
local args = {
    [1] = "[ CATASTROPHE ] THE TRUE CATASTROPHE APPEARED!",
    [2] = "All"   
}

 
		changeMode'CATASTROPHE'
	elseif(io.KeyCode == Enum.KeyCode.H and Mode=='CATASTROPHE')then
		changeMode'CATACALYSM'
	elseif(io.KeyCode == Enum.KeyCode.B and Mode=='Dark Matter')then 
		changeMode'MEMER'
local args = {
    [1] = "[ MEMER ] HELP MY DOG IS FLYING!!!",
    [2] = "All"   
}

 
	elseif(io.KeyCode == Enum.KeyCode.V and Mode=='RIDDLER')then 
		changeMode'THE WHEEL'
	elseif(io.KeyCode == Enum.KeyCode.Minus and Mode=='Dark Matter')then
		changeMode'OMEGA'
local args = {
    [1] = "[ OMEGA ] HOW DO YOU LIKE THIS!?",
    [2] = "All"   
}

 
	elseif(io.KeyCode == Enum.KeyCode.Minus and Mode=='OMEGA')then
		changeMode'OMEGA-X'
local args = {
    [1] = "[ OMEGA-X ] HAHAHAHAHAHA!! BEGONE!",
    [2] = "All"   
}

 
	elseif(io.KeyCode == Enum.KeyCode.Minus and Mode=='OMEGA-X')then
		changeMode'OMEGA'
local args = {
    [1] = "[ OMEGA ] HOW DO YOU LIKE THIS!?",
    [2] = "All"   
}

 
			elseif(io.KeyCode == Enum.KeyCode.H and Mode=='SANS')then 
		changeMode'GASTER'
local args = {
    [1] = "[ Gaster ] Dark, Darker, Yet Darker...",
    [2] = "All"   
}

 

	elseif(io.KeyCode == Enum.KeyCode.U and Mode=='HORROR')then
		changeMode'SUPERNATURAL'
	elseif(io.KeyCode == Enum.KeyCode.R and Mode=='HORROR')then 
		changeMode'Determined'
	elseif(io.KeyCode == Enum.KeyCode.R and Mode=='Dark Matter')then 
		changeMode'BETRAYAL'
	elseif(io.KeyCode == Enum.KeyCode.Zero and Mode=='Dark Matter')then 
		changeMode'UNIVERSAL'
local args = {
    [1] = "[ UNIVERSAL ] Universe's the name the height is my game!",
    [2] = "All"   
}

 
	elseif(io.KeyCode == Enum.KeyCode.Zero and Mode=='UNIVERSAL')then 
		changeMode'MULTIVERSAL'
local args = {
    [1] = "[ MULTIVERSAL ] TO INFINITY!",
    [2] = "All"   
}

 
	elseif(io.KeyCode == Enum.KeyCode.Zero and Mode=='MULTIVERSAL')then 
		changeMode'OMNIVERSAL'
local args = {
    [1] = "[ OMNIVERSAL ] THE ENERGY SURGING IS IMMENSE!",
    [2] = "All"   
}

 
	elseif(io.KeyCode == Enum.KeyCode.X and Mode=='JEVIL')then
local args = {
    [1] = "[ JEVIL ] OH!",
    [2] = "All"   
}

 
	elseif(io.KeyCode == Enum.KeyCode.C and Mode=='JEVIL')then 
local args = {
    [1] = "[ JEVIL ] I CAN DO ANYTHING!",
    [2] = "All"   
}

 
	elseif(io.KeyCode == Enum.KeyCode.V and Mode=='JEVIL')then 
local args = {
    [1] = "[ JEVIL ] BYE BYE!",
    [2] = "All"   
}

 
	elseif(io.KeyCode == Enum.KeyCode.B and Mode=='JEVIL')then 
local args = {
    [1] = "[ JEVIL ] CHAOS, CHAOS!",
    [2] = "All"   
}

 
	elseif(io.KeyCode == Enum.KeyCode.H and Mode=='=-XESTER-=')then 
		changeMode'JEVIL'
	elseif(io.KeyCode == Enum.KeyCode.U and Mode=='RIDDLER')then 
		changeMode'HYPNOSIS'
local args = {
    [1] = "[ Hypnosis ] I will control you.",
    [2] = "All"   
}

 
	elseif(io.KeyCode == Enum.KeyCode.R and Mode=='EUCLIDEAN')then 
		changeMode'S P A C E T I M E'
local args = {
    [1] = "[ S P A C E T I M E ] E m b r a c e   T h e   C h a n g e s . . .",
    [2] = "All"   
}

 
	elseif(io.KeyCode == Enum.KeyCode.U and Mode=='N U C L E A R - WASTE')then 
		changeMode'THE PLAGUEBRINGER'
local args = {
    [1] = "[ THE PLAGUEBRINGER ] The plague is already within you.",
    [2] = "All"   
}

 
	--TOGGLE MUSIC
	elseif(io.KeyCode == Enum.KeyCode.M and getMode(Mode))then 
		MusicMode=MusicMode+1
		if(MusicMode>3)then MusicMode=1 end
		if(MusicMode==1)then
			music:Pause()
			music.Volume=5
			music.Parent=Torso
			music:Resume()
		elseif(MusicMode==2)then
			music:Pause()
			music.Volume=5
			music.Parent=Char
			music:Resume()
		elseif(MusicMode==3)then
			music.Volume = 0
		end
	elseif(io.KeyCode==Enum.KeyCode.B)then
		--TAUNTS
		if(vaporwaveMode and Mode=='THE ESCHATON')then
			VaporTaunt()
		end
	end
	if(vaporwaveMode)then return end
	--ATTACKS
	if(io.UserInputType==Enum.UserInputType.MouseButton1)then
		ClickCombo()
	end
end)


player = game.Players.LocalPlayer
player.Character.Humanoid.Animator.Parent = nil
WingAnims.StarG=function()
