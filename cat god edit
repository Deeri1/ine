		Swait()
		spawnwave(Mouse.Hit.p)
	end
	for i=0,2,.5 / Animation_Speed do
		Swait()
		spawnwave(Mouse.Hit.p)
	end
	for i=0,2,.5 / Animation_Speed do
		Swait()
		spawnwave(Mouse.Hit.p)
	end
	for i=0,2,.5 / Animation_Speed do
		Swait()
		spawnwave(Mouse.Hit.p)
	end
