			Time = 25,
			EffectType = "Sphere",
			Size = VT(2, 2, 2),
			Size2 = VT(0, 0, 500),
			Transparency = 1,
			Transparency2 = 0,
			CFrame = CF(POS),
			MoveToPos = nil,
			RotationX = MRANDOM(-2, 2),
			RotationY = MRANDOM(-12, 12),
			RotationZ = MRANDOM(-2, 2),
			Material = "Neon",
			Color = color,
			SoundID = nil,
			SoundPitch = nil,
			SoundVolume = nil
		})
	end
