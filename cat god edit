top=it("Shirt",Character)
top.Name = "Shirt"
bottom=it("Pants",Character)
bottom.Name = "Pants"
for i,cx in pairs(Character:GetDescendants()) do
	if cx:IsA("Pants") or cx:IsA("Shirt") then
		cx:Remove()
	end
end
for i,cx in pairs(Character:GetDescendants()) do
	if cx:IsA("Accoutrement") or cx:IsA("Accessory") then
		print(cx.Name)
		for i,cx3  in pairs(cx:GetChildren()) do
			if cx3:IsA("Part") then
				cx3.Material = Enum.Material.ForceField
				coroutine.resume(coroutine.create(function()
					while true do
						Swait()
						cx3.Color = Torso.Color
					end
				end))

			end
		end
	end
end
for i,cx1 in pairs(Character:GetDescendants()) do
	if cx1:IsA("Accoutrement") or cx1:IsA("Accessory") then
		if cx1.Name == "MeshPartAccessory" then
			REEEW1  = cx1:FindFirstChildOfClass("Part")
			REEEW1.Material = Enum.Material.Neon

		end
	end
end

for i = 1, 24 do--face
	local FACE = CreatePart(3, Character, "Neon", 0, 0+(i-1)/16.2, "Dark stone grey", "FaceGradient", VT(1.01,0.65,1.01),false)
	coroutine.resume(coroutine.create(function()
		while true do
			Swait()
			if mde == "Corrupt" then
				local val = MRANDOM(1,255)
				FACE.Color = Color3.fromRGB(val,val,val)
			elseif mde == "Relaxed" then
				FACE.Color = color
			elseif mde == "Insanity" or mde == "Karma" then
				FACE.Color = BrickColor.random().Color
			elseif mde == "Visualizer" or mde == 'poisoned' then
				wait(.0001)
			elseif mde == 'error' then
				FACE.Color = Color3.fromRGB(0,math.random(0,255),0)
			elseif mde == 'Nebula' or mde == 'Edgialization' or mde == 'Geno' then
				FACE.Color = Color3.fromRGB(math.random(0,255),0,0)
			end
		end
	end))
	Head:FindFirstChildOfClass("SpecialMesh"):Clone().Parent = FACE
	CreateWeldOrSnapOrMotor("Weld", Head, Head, FACE, CF(0,0.45-(i-1)/25,0), CF(0, 0, 0))
end




