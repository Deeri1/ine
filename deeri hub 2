local function EKAHXIX_fake_script() -- rejoinbutton.rejoin button script 
	local script = Instance.new('LocalScript', rejoinbutton)

	
	
	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.Parent["extra buttons"]["silent rejoin lable"]["silent rejoin toggle"].BackgroundColor3 == Color3.fromHSV(0, 0, 1) then
			game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer('Rejoining', 'All')
		end 
		local ts = game:GetService("TeleportService")
		local p = game:GetService("Players").LocalPlayer
		ts:Teleport(game.PlaceId, p)
	end)
end
coroutine.wrap(EKAHXIX_fake_script)()
