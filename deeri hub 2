-- Scripts:

local function FVCEPV_fake_script() -- openup.openupbuttons 
	local script = Instance.new('LocalScript', openup)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.openupitems.Visible = true
	end)
end
coroutine.wrap(FVCEPV_fake_script)()
