local function VLPDJGH_fake_script() -- silentrejointoggle.LocalScript 
	local script = Instance.new('LocalScript', silentrejointoggle)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent["silent rejoin toggle"].BackgroundColor3 == Color3.fromHSV(0, 0, 1)== true then
			script.Parent.Parent["silent rejoin toggle"].BackgroundColor3 = Color3.fromHSV(0, 0, 0)
		elseif script.Parent.Parent["silent rejoin toggle"].BackgroundColor3 == Color3.fromHSV(0, 0, 0)== true then
			script.Parent.Parent["silent rejoin toggle"].BackgroundColor3 = Color3.fromHSV(0, 0, 1)
			end
	
	end)
end
coroutine.wrap(VLPDJGH_fake_script)()
local function LWLFU_fake_script() -- resetcolorbutton.resetcolorscript 
	local script = Instance.new('LocalScript', resetcolorbutton)

	mainui = script.Parent.Parent.Parent.Main
	
	
	
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.BackgroundColor3 =
			Color3.fromHSV(0, 0, 0.835294)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.opendownitems.BackgroundColor3 =
			Color3.fromHSV(0, 0, 1)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.openupitems.BackgroundColor3 =
			Color3.fromHSV(0, 0, 1)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.CMD.BackgroundColor3 =
			Color3.fromHSV(0, 0, 1)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.Run.BackgroundColor3 =
			Color3.fromHSV(0, 0, 1)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.Run.TextColor3 =
			Color3.fromHSV(0, 0, 0)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.CMD.TextColor3 =
			Color3.fromHSV(0, 0, 0)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.colorsettings.BackgroundColor3 =
			Color3.fromHSV(0, 0, 1)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.opendown.TextColor3 =
			Color3.fromHSV(0, 0, 0)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.openupitems.BackgroundColor3 =
			Color3.fromHSV(0, 0, 1)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.opendownitems.closedown.BackgroundColor3 =
			Color3.fromHSV(0, 0, 1)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.openupitems.closeup.BackgroundColor3 =
			Color3.fromHSV(0, 0, 1)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.colorsettings.BackgroundColor3 =
			Color3.fromHSV(0, 0, 1)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.opendown.BackgroundColor3 =
			Color3.fromHSV(0, 0, 1)
	end)
	
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.opendownitems.closedown.TextColor3 =
			Color3.fromHSV(0, 0, 0)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.openupitems.closeup.TextColor3 =
			Color3.fromHSV(0, 0, 0)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.openup.BackgroundColor3 =
			Color3.fromHSV(0, 0, 1)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.openup.TextColor3 =
			Color3.fromHSV(0, 0, 0)
	end)
	
end
coroutine.wrap(LWLFU_fake_script)()
