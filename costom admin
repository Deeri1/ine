hum2 = charv.HumanoidRootPart
print("yea")
else
hum2 = charv.Torso
print("naw")
print(errormessage)
end
hum.Position = Vector3.new(hum.Position.X,hum.Position.Y,hum.Position.Z)
	RunService.RenderStepped:Connect(function()
							if copy == true then
								local success, errormessage = pcall(function()
if charv:FindFirstChild(HumanoidRootPart) then
end
end)
if success then
hum2 = charv.HumanoidRootPart

else
hum2 = charv.Torso

end
							game:GetService("Workspace").CurrentCamera.CameraSubject = char.Head

char["Left Arm"].Position = Vector3.new(charv["Left Arm"].Position.X-hum2.Position.X+hum.Position.X,charv["Left Arm"].Position.Y+hum.Position.Y-3.45,charv["Left Arm"].Position.Z+hum.Position.Z-hum2.Position.Z)
char["Left Arm"].Rotation  = charv["Left Arm"].Rotation
		

			end
	end)

		
end
