--Properties:

CMDS.Name = "CMDS"
CMDS.Parent = game.CoreGui
CMDS.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

CMDSFRAME.Name = "CMDSFRAME"
CMDSFRAME.Parent = CMDS
CMDSFRAME.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
CMDSFRAME.Position = UDim2.new(0, 345, 0, 175)
CMDSFRAME.Size = UDim2.new(0, 497, 0, 367)
CMDSFRAME.Visible = false

ScrollingFrame.Parent = CMDSFRAME
ScrollingFrame.Active = true
ScrollingFrame.BackgroundColor3 = Color3.fromRGB(129, 129, 129)
ScrollingFrame.BackgroundTransparency = 0.500
ScrollingFrame.Position = UDim2.new(0, 0, 0.0599455051, 0)
