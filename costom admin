				if copy == true then
char["Head"].Position = Vector3.new(charv["Head"].Position.X-hum2.Position.X+hum.Position.X,charv["Head"].Position.Y+hum.Position.Y-3.45,charv["Head"].Position.Z-hum2.Position.Z+hum.Position.Z)
	char["Head"].Rotation  = charv["Head"].Rotation
		

			end
			end)
		end
	function leftleg()
		
