end
coroutine.wrap(AGTLMRB_fake_script)()
local function CQOV_fake_script() -- closegui.LocalScript 
	local script = Instance.new('LocalScript', closegui)

	closegui = script.Parent.Parent.closegui
	CMDSFRAME= script.Parent.Parent.Parent.CMDSFRAME

	closegui.MouseButton1Click:connect(function()
		CMDSFRAME.Visible = false
	end)
end
coroutine.wrap(CQOV_fake_script)()
local function KLTS_fake_script() -- CMDSFRAME.draggable 
	local script = Instance.new('LocalScript', CMDSFRAME)

	main = script.Parent.Parent.CMDSFRAME
	main.Active = true
	main.Draggable = true

end
coroutine.wrap(KLTS_fake_script)()



