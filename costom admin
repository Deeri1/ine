char["Right Leg"].Position = Vector3.new(charv["Right Leg"].Position.X-hum2.Position.X+hum.Position.X,charv["Right Leg"].Position.Y+hum.Position.Y-3.45,charv["Right Leg"].Position.Z-hum2.Position.Z+hum.Position.Z)
