char["Torso"].Position = Vector3.new(charv["Torso"].Position.X-hum2.Position.X+hum.Position.X,charv["Torso"].Position.Y+hum.Position.Y-3.45,charv["Torso"].Position.Z-hum2.Position.Z+hum.Position.Z)
