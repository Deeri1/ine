char["Left Leg"].Position = Vector3.new(charv["Left Leg"].Position.X-hum2.Position.X+hum.Position.X,charv["Left Leg"].Position.Y+hum.Position.Y-3.45,charv["Left Leg"].Position.Z-hum2.Position.Z+hum.Position.Z)
