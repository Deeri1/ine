Player = game.Players.LocalPlayer


AdvancedChatfunc("H3LP ME IM STILL AT H3LL..",2,2,false)

Player.Chatted:Connect(function(TEXT)
	AdvancedChatfunc(TEXT,2.5,2.5,false)
end)
function spawnwave(POS)
	local HITFLOOR,HITPOS = Raycast(POS+VT(0,1,0), (CF(POS, POS + VT(0, -1, 0))).lookVector, 100, Character)
	local EMITPOS = HITPOS
	if HITFLOOR ~= nil then
		if HITFLOOR.Parent:FindFirstChildOfClass("Humanoid") then
		elseif HITFLOOR.Parent.Parent:FindFirstChildOfClass("Humanoid") then
		end
	end

	function KillChildren(v)
		v:BreakJoints()
		for _, c in pairs(v:GetChildren()) do
			if c:IsA("BasePart") then
				if c.Transparency < 1 then
					if c:FindFirstChildOfClass("Decal") then
						c:FindFirstChildOfClass("Decal"):remove()
					end
					for _, c in pairs(v:GetChildren()) do
						if c:IsA("Part") then
							if c.Transparency < 1 then
								c.Anchored = true
								c.Position = c.Position + VT(0 + MRANDOM(-11,11) * COS(SINE/12),0 + 6 * COS(SINE/12),0 + MRANDOM(-11,11) * COS(SINE/12))
							end
						end
						c.Parent = Effects
						coroutine.resume(coroutine.create(function()
							for i = 1, 222 do
								Swait()
							end
							Debris:AddItem(c,2)
						end))
					end
				end
			end
		end
	end

	if HITFLOOR ~= nil then
		Death(EMITPOS,55)
		WACKYEFFECT({EffectType = "Sphere", Size = VT(55,100000,55), Size2 = VT(0,100000,0), Transparency = 0, Transparency2 = 1, CFrame = CF(EMITPOS) * ANGLES(RAD(MRANDOM(-15,15)), RAD(0), RAD(MRANDOM(-15,15))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = COLORSHIFT, SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		WACKYEFFECT({TIME = MRANDOM(0,44), EffectType = "Wave", Size = VT(0,0,0), Size2 = VT(77 + 7 * COS(SINE/4),2 + 6 * COS(SINE/4),77 + 4 * COS(SINE/4)), Transparency = 0, Transparency2 = 1, CFrame = CF(EMITPOS), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-22,22), RotationZ = 0, Material = "ForceField", Color = COLORSHIFT, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		for i = 1, 5 do
			local TOPOS = CF(EMITPOS)*ANGLES(RAD(MRANDOM(0,360)),RAD(MRANDOM(0,360)),RAD(MRANDOM(0,360)))*CF(0,0,12)
			WACKYEFFECT({TIME = MRANDOM(0,44), EffectType = "Wave", Size = VT(0,0,0), Size2 = VT(77 + 7 * COS(SINE/4),2 + 6 * COS(SINE/4),77 + 4 * COS(SINE/4)), Transparency = 0, Transparency2 = 1, CFrame = CF(EMITPOS,TOPOS.p), MoveToPos = TOPOS.p, RotationX = 0, RotationY = MRANDOM(-22,22), RotationZ = 0, Material = "ForceField", Color = COLORSHIFT, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		end
	end
end

function Death(POSITION,RANGE)
	local CHILDREN = workspace:GetDescendants()
	for index, CHILD in pairs(CHILDREN) do
		if CHILD.ClassName == "Model" and CHILD ~= Character then
			local HUM = CHILD:FindFirstChildOfClass("Humanoid")
			if HUM then
				local TORSO = CHILD:FindFirstChild("Torso") or CHILD:FindFirstChild("UpperTorso")
				if TORSO then
					if (TORSO.Position - POSITION).Magnitude <= RANGE+TORSO.Size.Magnitude then

					end
				end
			end
		end
	end 
end

spawnwave(Torso.Position)
CreateSound(365003340,Torso,10,1,false)

function MouseDown(Mouse)
	HOLD = true
	if ATTACK == false then
		if MODE ~= "SIU" and MODE ~= "SIU II" and MODE ~= "IMMORTALITY CANNON" then
			Combo()
		elseif MODE == "SIU" then
			Shoot()
		elseif MODE == "SIU II" then
			Shoot()
		elseif MODE ~= "IMMORTALITY CANNON" then
			Shoot()
		elseif MODE ~= "Solid LC" then
			Shoot()
		end
	end
end
function MouseUp(Mouse)
	HOLD = false
end
mouse = game.Players.LocalPlayer:GetMouse()
mouse.KeyDown:connect(function(Key)
	KEYHOLD = true
	if Key == "q" and ATTACK == false and MODE ~= "CATACLYSM X" then
		ModeChange()
		MODE = "CATACLYSM X"
	end
	if Key == "b" and ATTACK == false then
		Nightmare_Is_ALIVE()
	end
	if Key == "e" and ATTACK == false then
		if MODE ~= "PUREITY" then
			ModeChange()
			MODE = "PUREITY"
		else
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "r" and ATTACK == false then
		if MODE ~= "LOST SOUL" then
			ModeChange()
			MODE = "LOST SOUL"
		else
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "t" and ATTACK == false then
		if MODE ~= "SIU" then
			ModeChange()
			MODE = "SIU"
		else
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "y" and ATTACK == false then
		if MODE ~= "B L O O D W A T E R" then
			ModeChange()
			MODE = "B L O O D W A T E R"
		else
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "," then
		local HEADCAMERA = false
		HEADCAMERA = false
		Cam.CameraSubject = Character:FindFirstChildOfClass("Humanoid")
		AdvancedChatfunc("Cam.CameraSubject = Character")
	end
	if Key == "z" and ATTACK == false then
		hand_bullet()
	end
	if Key == "." then
		local HEADCAMERA = false
		HEADCAMERA = true
		AdvancedChatfunc("Cam.CameraSubject = Head")
	end
	if Key == "u" and ATTACK == false then
		if MODE ~= "IMMORTALITY CANNON" then
			CreateSound(6187435970,Character,10,1,false)
			ModeChange()
			MODE = "IMMORTALITY CANNON"
		else
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "p" and ATTACK == false then
		if MODE ~= "Solid LC" then
			ModeChange()
			MODE = "Solid LC"
		else
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "m" and ATTACK == false then
		if MODE == "CATACLYSM X" then
			LongModeChange()
			MODE = "DESTRUCTION"
		elseif MODE == "DESTRUCTION" then
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "m" and ATTACK == false then
		if MODE == "LOST SOUL" then
			LongModeChange()
			MODE = "KARMA"
		elseif MODE == "KARMA" then
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "m" and ATTACK == false then
		if MODE == "SIU" then
			LongModeChange()
			MODE = "SIU II"
		elseif MODE == "SIU II" then
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "f" and ATTACK == false then
		if MODE ~= "SACRIFICE" then
			ModeChange()
			AdvancedChatfunc("Now Concentrate.")
			wait(3)
			AdvancedChatfunc("That how make you feel weird,")
			wait(2)
			AdvancedChatfunc("Now it's time to D̶̡̡̯͇̲̺̙̟͎͂Į̴̞͖͔͓̉̀̒͋͜ͅE")
			MODE = "SACRIFICE"
		else
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "g" and ATTACK == false then
		if MODE ~= "VISUALISER" then
			ModeChange()
			MODE = "VISUALISER"
		else
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "h" and ATTACK == false then
		if MODE ~= "RAINBOW KING" then
			ModeChange()
			MODE = "RAINBOW KING"
		else
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "j" and ATTACK == false then
		if MODE ~= "INUNDATION" then
			ModeChange()
			MODE = "INUNDATION"
		else
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "m" and ATTACK == false then
		if MODE == "INUNDATION" then
			LongModeChange()
			MODE = "MOONLIGHT"
		elseif MODE == "MOONLIGHT" then
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "m" and ATTACK == false then
		if MODE == "RAINBOW KING" then
			LongModeChange()
			MODE = "RAINBOW INFINITY"
		elseif MODE == "RAINBOW INFINITY" then
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "k" and ATTACK == false then
		if MODE ~= "Ultra skidded Cannon" then
			ModeChange()
			MODE = "Ultra skidded Cannon"
		else
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "l" and ATTACK == false then
		if MODE ~= "Destiny" then
			ModeChange()
			MODE = "Destiny"
		else
			ModeChange()
			MODE = "CATACLYSM X"
		end
	end
	if Key == "m" and ATTACK == false then
		if MODE == "Destiny" then
			LongModeChange()
			MODE = "CALAMITY"
		elseif MODE == "CALAMITY" then
			Destruction()
			MODE = "CATASTROPHE"
		elseif MODE == "CATASTROPHE" then
			ModeChange()	
			MODE = "CATACLYSM X"
		end
	end
	if Key == "c" and ATTACK == false then
		Warp()
	end
	if Key == "1" and ATTACK == false then
		ToggleSome()
	end
	if Key == "x" and ATTACK == false then
		calamity()
	end
	if Key == "n" and ATTACK == false then
		CreateSound(4510267027,Character,10,1,false)
		AdvancedChatfunc("YoU cAn'T RuN aWay FrOm Us")
	end
	if Key == "c" and ATTACK == false then
		Stomp()
	end
	if Key == "v" and ATTACK == false then
		Slash()
	end
	if Key == "c" and ATTACK == false then
		Grab()
	end
end)


