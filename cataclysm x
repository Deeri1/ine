function otherrandomstring2(length,randmin,randmax)
	local array = {}
	for i = 1,length do
		array[i] = string.char(MRANDOM(randmin,randmax))
	end
	return table.concat(array)
end
function Serv(Name)
	return game:GetService(Name)
end
