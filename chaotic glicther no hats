		if v:IsA("WedgePart") then
			v.Transparency = 0
			v.Color = C3(0.2,0,0)
		end
	end
	for i, v in pairs(lwing3.Buttom:GetChildren()) do
		if v:IsA("WedgePart") then
			v.Transparency = 0
			v.Color = C3(0.2,0,0)
		end
	end
	for i, v in pairs(mw2:GetChildren()) do
		if v:IsA("Part") then
			v.Transparency = 0
			v.Color = C3(0.2,0,0)
		end
	end
	for i, v in pairs(mw4:GetChildren()) do
		if v:IsA("Part") then
			v.Transparency = 0
			v.Color = C3(0.2,0,0)
		end
	end
	for i, v in pairs(mw1:GetChildren()) do
		if v:IsA("Part") then
			v.Transparency = 0
			v.Color = C3(0.2,0,0)
		end
	end
	refec.Color = ColorSequence.new(C3(0.2,0,0))
	refec2.Color = ColorSequence.new(C3(0.2,0,0))
	refec3.Color = ColorSequence.new(C3(0.2,0,0))
	lwing1.MainWelder.Emitter1.Color = ColorSequence.new(C3(0.2,0,0))
	lwing1.MainWelder.Emitter2.Color = ColorSequence.new(C3(0.2,0,0))
	lwing2.MainWelder.Emitter1.Color = ColorSequence.new(C3(0.2,0,0))
	lwing2.MainWelder.Emitter2.Color = ColorSequence.new(C3(0.2,0,0))
	lwing3.MainWelder.Emitter1.Color = ColorSequence.new(C3(0.2,0,0))
	lwing3.MainWelder.Emitter2.Color = ColorSequence.new(C3(0.2,0,0))
	rwing1.MainWelder.Emitter1.Color = ColorSequence.new(C3(0.2,0,0))
	rwing1.MainWelder.Emitter2.Color = ColorSequence.new(C3(0.2,0,0))
	rwing2.MainWelder.Emitter1.Color = ColorSequence.new(C3(0.2,0,0))
	rwing2.MainWelder.Emitter2.Color = ColorSequence.new(C3(0.2,0,0))
	rwing3.MainWelder.Emitter1.Color = ColorSequence.new(C3(0.2,0,0))
	rwing3.MainWelder.Emitter2.Color = ColorSequence.new(C3(0.2,0,0))
	--ned.Text = "Anxiety"
	ATTACK = false
	Rooted = false
end

function PeculiarForm()
	ATTACK = true
	Rooted = true
	killcam=true
	BALL.Parent = Character
	XEYE.Parent = nil
	sick.TimePosition = 0
	PITCH = 1
	VOLUME = 3
	SONG = 3271126110
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(15 - 2.5 * SIN(SINE / 12)), RAD(0), RAD(0)), 0.4 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(0.75, 0.4, -1) * ANGLES(RAD(70), RAD(0), RAD(-70)) * ANGLES(RAD(20), RAD(25), RAD(-15)) * RIGHTSHOULDERC0, 0.4 / Animation_Speed)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0 + 0.05 * COS(SINE / 12)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.4 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1, 0.1, -0.5) * ANGLES(RAD(25), RAD(0), RAD(85)) * LEFTSHOULDERC0, 0.4 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1 - 0.025 * COS(SINE / 12), -0.01) * ANGLES(RAD(0), RAD(83), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1 - 0.05 * COS(SINE / 12), -0.01) * ANGLES(RAD(0), RAD(-83), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)				
	end
	WACKYEFFECT({Time = 60,EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(85,200000,85), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,0,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.new("Black").Color, SoundID = 2011915907, SoundPitch = 1, SoundVolume = 15})
	WACKYEFFECT({Time = 90,EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(70,200000,70), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,0,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.new("White").Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
	WACKYEFFECT({Time = 120,EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(65,2000000,65), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,0,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.new("Really black").Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
	symbolizeBlink(RootPart,0,144580273,BrickColor.new("Black").Color,25,0,0,0,RootPart,true,-5,2)
	symbolizeBlink(RootPart,0,144580273,BrickColor.new("White").Color,20,0,0,0,RootPart,true,-5,1)
	symbolizeBlink(RootPart,0,144580273,BrickColor.new("Really black").Color,22.5,0,0,0,RootPart,true,-5,1.5)
	symbolizeBlink(RootPart,0,348103573,BrickColor.new("Black").Color,22.5,0,0,0,RootPart,true,-5,1.5)
	symbolizeBlink(RootPart,0,348103573,BrickColor.new("White").Color,30,0,0,0,RootPart,true,-25,1)
	symbolizeBlink(RootPart,0,348103573,BrickColor.new("Really black").Color,40,0,0,0,RootPart,true,-25,1)
	FACE = 629933140
	Speed = 19
	text.Text = "Peculiarity"
	TEXTCOLOR = BrickColor.new("Dark indigo").Color
	PRT.Color = BrickColor.new("Dark indigo").Color
	EYE.Color = C3(1,1,1)
	EYE2.Color = C3(1,1,1)
	Hair.Color = BrickColor.new("Pastel Blue").Color
	Hair2.Color = BrickColor.new("Pastel Blue").Color
	MAINRUINCOLOR = BrickColor.new("Dark indigo")
	SECONDRUINCOLOR  = BrickColor.new("Pastel Blue")
	RecolorThing(MAINRUINCOLOR,SECONDRUINCOLOR,"Neon","Neon",true)
	MODE = "Peculiar"
	--ned.Text = "PVZ2"
	killcam=false
	ATTACK = false
	Rooted = false
end

function CrystalForm()
	ATTACK = true
	Rooted = true
	killcam=true
	BALL.Parent = Character
	XEYE.Parent = nil
	local sizeo = 0
	SONG = 789657160
	PITCH = 1.18
	VOLUME = 1
	sick.TimePosition = 0.0001
	sick:Play()
	local vel = Instance.new("BodyPosition", RootPart)
	vel.P = 25000
	vel.D = 1000
	vel.maxForce = Vector3.new(50000000000, 10e10, 50000000000)
	vel.position = RootPart.CFrame.p + vt(0,100,0)
	text.Text = ""
	for i=0, 2, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0 + 0.2*math.cos(SINE/32)) * ANGLES(RAD(-0 + 2.5*math.cos(SINE/24)), RAD(0), RAD(0)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-7.5), RAD(0), RAD(0)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(80)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(-80)) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1 - 0.05, -0.01) * ANGLES(RAD(-15), RAD(80), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0 - 7*math.cos(SINE/34))), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -0.5, -0.5) * ANGLES(RAD(-12.5), RAD(-80), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0 + 5*math.cos(SINE/26))), 1 / Animation_Speed)				
	end
	coroutine.resume(coroutine.create(function()
		WACKYEFFECT({Time = 60*4,EffectType = "Crystal", Size = VT(0,0,0), Size2 = VT(85,115,85), Transparency = 0, Transparency2 = 0.4, CFrame = RootPart.CFrame*CF(0,0,0), MoveToPos = nil, RotationX = 0, RotationY = 5, RotationZ = 0, Material = "Glass", Color = BrickColor.new("Pastel Blue").Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		Swait(60*4)
