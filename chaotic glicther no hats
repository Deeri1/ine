				LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(-25), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(5 + 50 * SIN(SINE / 6))), 1.5 / Animation_Speed)
			else
				RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(-25), RAD(70), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1.5 / Animation_Speed)
				LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(-25), RAD(-70), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1.5 / Animation_Speed)
			end
		end
		CreateSound(1489705211,LeftArm,MRANDOM(7,8),MRANDOM(8,13)/12,false)
		local hitb = Instance.new("Part", char)
		hitb.Anchored = true
		hitb.CanCollide = false
		hitb.FormFactor = 3
		hitb.Name = "Ring"
		hitb.Material = "Neon"
		hitb.Size = Vector3.new(1, 1, 1)
		hitb.Transparency = 1
		hitb.TopSurface = 0
		hitb.BottomSurface = 0
		hitb.CFrame = RootPart.CFrame*CF(0,0,-0.5)
		ManualAoE(hitb.Position,5.5,5,20,0,false)
		hitb:Destroy()
		for i=0, 0.26, 0.1 / Animation_Speed do
			Swait()
			WACKYEFFECT({EffectType = "Box", Size = VT(2,2,2), Size2 = VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = LeftArm.CFrame*CF(0,-1,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = SECONDRUINCOLOR.Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			WACKYEFFECT({EffectType = "Box", Size = VT(2,2,2), Size2 = VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = RightArm.CFrame*CF(0,-1,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(25), RAD(0), RAD(0)), 1.5 / Animation_Speed)
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, -0.3, -0.2) * ANGLES(RAD(30), RAD(0), RAD(0)), 1.5 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1, 0.5, -0.5) * ANGLES(RAD(90), RAD(0), RAD(-30)) * ANGLES(RAD(0), RAD(0), RAD(0)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1, 0.5, -0.5) * ANGLES(RAD(90), RAD(0), RAD(30)) * ANGLES(RAD(0), RAD(0), RAD(0)) * LEFTSHOULDERC0, 1.5 / Animation_Speed)
			if ANIM=="Walk" then
				RightHip.C0 = Clerp(RightHip.C0, CF(1 , -1, 0) * ANGLES(RAD(30), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(-5 + 50 * SIN(SINE / 6))), 1.5 / Animation_Speed)
				LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(30), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(5 + 50 * SIN(SINE / 6))), 1.5 / Animation_Speed)
			else
				RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(30), RAD(70), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1.5 / Animation_Speed)
				LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(30), RAD(-70), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1.5 / Animation_Speed)
			end
		end
		COMBO = 4
	elseif COMBO == 4 then
		for i=0, 0.26, 0.1 / Animation_Speed do
			Swait()
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(15), RAD(0), RAD(0)), 1.5 / Animation_Speed)
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, -0.3, -0.2) * ANGLES(RAD(30), RAD(0), RAD(0)), 1 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.2, 0.5, -0.5) * ANGLES(RAD(75), RAD(0), RAD(-70)) * ANGLES(RAD(0), RAD(0), RAD(0)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.2, 0.5, -0.5) * ANGLES(RAD(75), RAD(0), RAD(70)) * ANGLES(RAD(0), RAD(0), RAD(0)) * LEFTSHOULDERC0, 1.5 / Animation_Speed)
			if ANIM=="Walk" then
				RightHip.C0 = Clerp(RightHip.C0, CF(1 , -1, 0) * ANGLES(RAD(30), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(-5 + 50 * SIN(SINE / 6))), 1.5 / Animation_Speed)
				LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(30), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(5 + 50 * SIN(SINE / 6))), 1.5 / Animation_Speed)
			else
				RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(30), RAD(70), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1.5 / Animation_Speed)
				LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(30), RAD(-70), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1.5 / Animation_Speed)
			end
		end	
		WACKYEFFECT({Time = 40,EffectType = "Block", Size = VT(0,0,0), Size2 = VT(25,25,25), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,0,-0.5)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = SECONDRUINCOLOR.Color, SoundID = 260435136, SoundPitch = 0.9, SoundVolume = 10})
		WACKYEFFECT({Time = 80,EffectType = "Block", Size = VT(0,0,0), Size2 = VT(12.5,12.5,12.5), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,0,-0.5)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		CameraEnshaking(3,10)
		ManualAoE(RootPart.Position,17.5,10,30,20,false)
		for i=0, 0.26, 0.1 / Animation_Speed do
			Swait()
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-15), RAD(0), RAD(0)), 2 / Animation_Speed)
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0.25, -0.2) * ANGLES(RAD(-25), RAD(0), RAD(0)), 2 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(75), RAD(0), RAD(45)) * RIGHTSHOULDERC0, 2 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(75), RAD(0), RAD(-45)) * ANGLES(RAD(0), RAD(0), RAD(0)) * LEFTSHOULDERC0, 2 / Animation_Speed)
			if ANIM=="Walk" then
				RightHip.C0 = Clerp(RightHip.C0, CF(1 , -1, 0) * ANGLES(RAD(-25), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(-5 + 50 * SIN(SINE / 6))), 2 / Animation_Speed)
				LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(-25), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(5 + 50 * SIN(SINE / 6))), 2 / Animation_Speed)
			else
				RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(-25), RAD(70), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 2 / Animation_Speed)
				LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(-25), RAD(-70), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 2 / Animation_Speed)
			end
		end	
		COMBO = 1
	end
	coroutine.resume(coroutine.create(function()
		for i = 1, 50 do
			Swait()
			if ATTACK == true then
				break
			end
		end
		if ATTACK == false then
			COMBO = 1
		end
	end))
	ATTACK = false
	Rooted = false
end

--[[                 ])
 //--------==================--------\\
 \\                                  //
 ||        SPECIAL MOVE THING        ||
 //                                  \\
 \\--------==================--------//
--([                 ]]
function turnto(position)
	RootPart.CFrame=CFrame.new(RootPart.CFrame.p,VT(position.X,RootPart.Position.Y,position.Z)) * CFrame.new(0, 0, 0)
end

function turnto2(position)
	RootPart.CFrame=CFrame.new(RootPart.CFrame.p,VT(position.X,position.Y,position.Z)) * CFrame.new(0, 0, 0)
end

function CharacterFade(COLOR,TIMER)
	coroutine.resume(coroutine.create(function()
		local FADE = IT("Model",Effects)
		FADE.Name = "FadingEffect"
		for _, c in pairs(Character:GetChildren()) do
			if c.ClassName == "Part" and c ~= RootPart then
				local FADER = c:Clone()
				FADER.Color = COLOR
				FADER.CFrame = c.CFrame
				FADER.Parent = FADE
				FADER.Anchored = true
				FADER.Transparency = 0.25+c.Transparency
				FADER:BreakJoints()
				FADER.Material = "ForceField"
				if FADER.Name == "Head" then
					FADER:ClearAllChildren()
					FADER.Size = VT(1,1,1)
				end
				FADER.CanCollide = false
			end
		end
		local TRANS = 0.75/TIMER
		for i = 1, TIMER do
			Swait()
			for _, c in pairs(FADE:GetChildren()) do
				if c.ClassName == "Part" then
					c.Transparency = c.Transparency + TRANS
				end
			end
		end
		FADE:remove()
	end))
end

function Chunks(PART)
	for i = 1, MRANDOM(3,5) do
		coroutine.resume(coroutine.create(function()
			local CHUNK = CreatePart(3, workspace, PART.Material, 0, PART.Transparency, PART.BrickColor, "Chunk", VT(0.3,0.3,0.3)*MRANDOM(7,13)/10, false)
			CHUNK.CFrame = PART.CFrame*ANGLES(RAD(MRANDOM(0,360)),RAD(MRANDOM(0,360)),RAD(MRANDOM(0,360)))
			local CFRAME = PART.CFrame*CF(MRANDOM(-4,4)/2,MRANDOM(-4,4)/2,-6)
			CHUNK.Velocity = CF(PART.Position,CFRAME.p).lookVector*MRANDOM(15,65)
			wait(0.1)
			CHUNK.CanCollide = true
			wait(MRANDOM(15,25)/5)
			for i = 1, 25 do
				Swait()
				CHUNK.Transparency = CHUNK.Transparency + 1/25
			end
			CHUNK:remove()
		end))
	end
end

function SuperStomp()
	ATTACK = true
	Rooted = true
	local HITFLOOR,HITPOS = Raycast(RootPart.Position, (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 25*SIZE, Character)
	coroutine.resume(coroutine.create(function()
		for i = 0, 10 do
			WACKYEFFECT({
				Time = 15,
				EffectType = "Sphere",
				Size = VT(90, 1, 90),
				Size2 = VT(0, 0, 0),
				Transparency = 1,
				Transparency2 = 0.2,
				CFrame = RootPart.CFrame*CF(0,-3,0),
				MoveToPos = nil,
				RotationX = 0,
				RotationY = 0,
				RotationZ = 0,
				Material = "ForceField",
				Color = C3(155/255, 0/255, 0/255 ),
				SoundID = nil,
				SoundPitch = nil,
				SoundVolume = nil
			})
			WACKYEFFECT({
				Time = 15,
				EffectType = "Sphere",
				Size = VT(90, 1, 90),
				Size2 = VT(0, 0, 0),
				Transparency = 1,
				Transparency2 = 0.2,
				CFrame = RootPart.CFrame*CF(0,-3,0),
				MoveToPos = nil,
				RotationX = 0,
				RotationY = 0,
				RotationZ = 0,
				Material = "ForceField",
				Color = C3(0/255, 0/255, 0/255 ),
				SoundID = nil,
				SoundPitch = nil,
				SoundVolume = nil
			})
			Swait(7.5)
		end
	end))
	coroutine.resume(coroutine.create(function()
		Swait(65)
		CameraEnshaking(3,25)
		CreateSound(262562442,RightLeg,2,1.35,false)
		CreateDebreeRing(HITFLOOR,HITPOS,90,VT(10,10,10),20)
		for i = 0,50 do
			WACKYEFFECT({Time = math.random(10,120), EffectType = "Sphere", Size = VT(0,0.55,0)*SIZE, Size2 = VT(10,0,10)*math.random(1,600)/25, Transparency = 0, Transparency2 = 1, CFrame = CF(HITPOS), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = C3(159/255, 0/255, 0/255), SoundID = nil, SoundPitch = 0.96, SoundVolume = 10})
		end      
		ApplyAoE2(HITPOS,80,50,100,75,false)
	end))
	CreateSound(163619849,RightLeg,10,1,false)
	for i=0, 2, 0.1 / Animation_Speed do
		WACKYEFFECT({EffectType = "Block", Size = VT(2,2,2), Size2 = VT(1,1,1)/3, Transparency = 0.5, Transparency2 = 1, CFrame = RightLeg.CFrame*CF(0,-1.3,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(1,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0 + 0.05 * COS(SINE / 12)) * ANGLES(RAD(-15), RAD(0), RAD(40)), 0.5 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1.25*SIZE) - 1)) * ANGLES(RAD(35), RAD(0), RAD(-40)), 0.5 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.2*SIZE, 0.5*SIZE, 0) * ANGLES(RAD(90), RAD(-45), RAD(-55)) * RIGHTSHOULDERC0, 0.5 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.2*SIZE, 0.5*SIZE, 0) * ANGLES(RAD(0), RAD(30), RAD(85)) * LEFTSHOULDERC0, 0.5 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1*SIZE, -0.4*SIZE - 0.05 * COS(SINE / 12), -0.5) * ANGLES(RAD(0), RAD(75), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.5 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1*SIZE, -1*SIZE - 0.05 * COS(SINE / 12), -0.01) * ANGLES(RAD(-15), RAD(-75), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.5 / Animation_Speed)
	end
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, -0.5 + 0.05 * COS(SINE / 12)) * ANGLES(RAD(10), RAD(0), RAD(0)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1.25*SIZE) - 1)) * ANGLES(RAD(35), RAD(0), RAD(0)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.2*SIZE, 0.5*SIZE, 0) * ANGLES(RAD(0), RAD(105), RAD(-85)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.2*SIZE, 0.5*SIZE, 0) * ANGLES(RAD(0), RAD(-105), RAD(85)) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1*SIZE, -0.7*SIZE - 0.05 * COS(SINE / 12), -0.4) * ANGLES(RAD(10), RAD(75), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(20)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1*SIZE, -1*SIZE - 0.05 * COS(SINE / 12), 0.13) * ANGLES(RAD(0), RAD(-75), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(35)), 1 / Animation_Speed)
	end
	ATTACK = false
	Rooted = false
end

function DirtQuake()
	local HITFLOOR,HITPOS = Raycast(RootPart.Position, (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 12, Character)
	if HITFLOOR then
		ATTACK = true
		Rooted = true
		local GYRO = IT("BodyGyro",RootPart)
		GYRO.D = 2
		GYRO.P = 2000
		GYRO.MaxTorque = VT(0,4000000,0)
		coroutine.resume(coroutine.create(function()
			repeat
				Swait()
				GYRO.CFrame = CF(RootPart.Position,Mouse.Hit.p)
			until ATTACK == false
			GYRO:Remove()
		end))
		for i=0, 0.75, 0.1 / Animation_Speed do
			Swait()
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(-15), RAD(0), RAD(0)), 1 / Animation_Speed)
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(25), RAD(0), RAD(0)), 1 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(20), RAD(0), RAD(-5)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(20), RAD(0), RAD(5)) * LEFTSHOULDERC0, 1 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -0.35, -0.75) * ANGLES(RAD(30), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(-30), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
		coroutine.resume(coroutine.create(function()
			wait(0.15)
			CameraEnshaking(5,7.5)
			CreateSound(165970126, RightLeg, 1, MRANDOM(13,15)/10, false)
		end))
		coroutine.resume(coroutine.create(function()
			for i = 1, 5 do
				WaveParticles(RightLeg.CFrame*CF(0,-1,0).p,60,C3(1,1,1))
				wait(0.1)
			end
		end))
		coroutine.resume(coroutine.create(function()
			local CFRAME = RootPart.CFrame*CF(0,-3,0)
			for i = 1, 20 do
				local SPIKE = Getblocky(HITFLOOR)
				EffectsAoE(CFRAME*CF(0,0,-i*8).p,7,10,35,75,"Up",false)
				SPIKE.CFrame = CFRAME*CF(0,-11,-i*8)
				coroutine.resume(coroutine.create(function()
					local POS = SPIKE.CFrame*CF(0,12.5,0)
					for i = 1, 10 do
						Swait()
						SPIKE.CFrame = Clerp(SPIKE.CFrame, POS, 1 / Animation_Speed)
					end
					for i = 1, 45 do
						Swait()
						SPIKE.Transparency = SPIKE.Transparency + 1/45
						SPIKE.CFrame = SPIKE.CFrame * CF(0,-2/15,0)
					end
					SPIKE:remove()
				end))
				Swait(8)
			end
		end))
		for i=0, 0.5, 0.1 / Animation_Speed do
			Swait()
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, -0.4, -0.1) * ANGLES(RAD(35), RAD(0), RAD(0)), 1 / Animation_Speed)
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(25), RAD(0), RAD(0)), 1 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(35), RAD(0), RAD(12)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(35), RAD(0), RAD(-12)) * LEFTSHOULDERC0, 1 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -0.9, -0.6) * ANGLES(RAD(35), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1.1, 0) * ANGLES(RAD(35), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
		wait(0.3)
		ATTACK = false
		Rooted = false
	end
end

function Execute()
	ATTACK = true
	Rooted = true
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		WACKYEFFECT({EffectType = "Block", Size = VT(1,1,1), Size2 = VT(1,1,1), Transparency = 0.5, Transparency2 = 1, CFrame = RightArm.CFrame*CF(0,-1.3,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		WACKYEFFECT({EffectType = "Block", Size = VT(1,1,1), Size2 = VT(1,1,1), Transparency = 0.5, Transparency2 = 1, CFrame = LeftArm.CFrame*CF(0,-1.3,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, -0.35, -0.1) * ANGLES(RAD(35), RAD(0), RAD(0)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(25), RAD(0), RAD(0)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(35), RAD(0), RAD(12)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(35), RAD(0), RAD(-12)) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1.1, 0) * ANGLES(RAD(35), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1.1, 0) * ANGLES(RAD(35), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
	end
	coroutine.resume(coroutine.create(function()
		CreateSound(240429466,RootPart,20,1,false)
		CreateSound(240517975,RootPart,2,1.3,false)
		ApplyAoE2(Mouse.Hit.Position,19,1,100,13,false)
		EffectsAoE(Mouse.Hit.Position,40,3,6.5,90,"UpKnock",false)
		ApplyAoE2(RootPart.Position,19,1,100,13,false)
		CameraEnshaking(3,8)
		WACKYEFFECT({EffectType = "Wave", Size = VT(1,10,1), Size2 = VT(60,10,60), Transparency = 0.5, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position)*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-800/100,800/100), RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		WACKYEFFECT({EffectType = "Crystal", Size = VT(0,0,0), Size2 = VT(10,100,10), Transparency = 0.5, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position)*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-100/100,100/100), RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = 240429466, SoundPitch = 1, SoundVolume = 8})
		WACKYEFFECT({EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(60,10,60), Transparency = 0.5, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position)*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-800/100,800/100), RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
	end))
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		WACKYEFFECT({EffectType = "Block", Size = VT(1,1,1), Size2 = VT(1,1,1), Transparency = 0.5, Transparency2 = 1, CFrame = RightArm.CFrame*CF(0,-1.3,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		WACKYEFFECT({EffectType = "Block", Size = VT(1,1,1), Size2 = VT(1,1,1), Transparency = 0.5, Transparency2 = 1, CFrame = LeftArm.CFrame*CF(0,-1.3,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		WACKYEFFECT({EffectType = "Wave", Size = VT(1,5,1), Size2 = VT(60,0,60), Transparency = 0.5, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,-3,0), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-100/10,100/10), RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0.35, -0.1) * ANGLES(RAD(-35), RAD(0), RAD(0)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-25), RAD(0), RAD(0)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(135), RAD(0), RAD(12)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(135), RAD(0), RAD(-12)) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1.1, 0) * ANGLES(RAD(-35), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1.1, 0) * ANGLES(RAD(-35), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
	end
	ATTACK = false
	Rooted = false
end

function SenseThePain()
	ATTACK = true
	Rooted = true
	local sizey = 1
	CreateSound(163619849,RightArm,3,0.8,false)
	for i=0, 4, 0.1 / Animation_Speed do
		WACKYEFFECT({Time = 5,EffectType = "Sphere", Size = VT(1,0.05,1)*sizey, Size2 = VT(1,0.05,1)*sizey, Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		Swait()
		sizey = sizey + 0.5
		turnto(Mouse.Hit.p)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0 - 0.04 * SIN(SINE / 24)*SIZE, 0 + 0.04 * SIN(SINE / 12)*SIZE, 0 + 0.05*SIZE * COS(SINE / 12)) * ANGLES(RAD(0 - 2.5 * SIN(SINE / 12)), RAD(0), RAD(25)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1.15) - 1)) * ANGLES(RAD(-10 - 4 * SIN(SINE / 12)), RAD(0), RAD(-25)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.4, 0.6, -0.2) * ANGLES(RAD(135), RAD(0 + 2.5 * SIN(SINE / 25)), RAD(35 + 2.5 * SIN(SINE / 12))) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0.1) * ANGLES(RAD(-30), RAD(0 - 2.5 * SIN(SINE / 12)), RAD(-40 - 2.5 * SIN(SINE / 12))) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1*SIZE, -1*SIZE - 0.05*SIZE * COS(SINE / 12), -0.01*SIZE) * ANGLES(RAD(0 - 2.5 * SIN(SINE / 12)), RAD(75), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1*SIZE, -1*SIZE - 0.05*SIZE * COS(SINE / 12), -0.01*SIZE) * ANGLES(RAD(0 - 2.5 * SIN(SINE / 12)), RAD(-75), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 1 / Animation_Speed)
		if MRANDOM(1,10) == 1 then
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(MRANDOM(-5,5)), RAD(MRANDOM(-10,10)), RAD(MRANDOM(-10,10))), 1)
		end
	end
	local dis = CreateParta(char,1,1,"Neon",BrickColor.new("Really red"))
	dis.CFrame = CF(Mouse.Hit.Position)
	CFuncs["EchoSound"].Create("rbxassetid://763718160", dis, 3, 1.1,0,10,0.15,0.5,1)
	CFuncs["EchoSound"].Create("rbxassetid://782200047", dis, 7, 1.1,0,10,0.15,0.5,1)
	CameraEnshaking(5,17.5)
	WACKYEFFECT({Time = 5,EffectType = "Sphere", Size = VT(1,0.05,1)*sizey, Size2 = VT(1,0.05,1)*sizey, Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = 1, SoundVolume = 5})
	WACKYEFFECT({Time = 30,EffectType = "Sphere", Size = VT(1,0.05,1), Size2 = VT(3,0.05,3)*sizey, Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = 1, SoundVolume = 5})
	WACKYEFFECT({Time = 110,EffectType = "Sphere", Size = VT(1,0.05,1)*sizey, Size2 = VT(2,0.05,2)*sizey, Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = 1, SoundVolume = 5})
	WACKYEFFECT({Time = 70,EffectType = "Sphere", Size = VT(1,0.05,1), Size2 = VT(2.5,0.05,2.5)*sizey, Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = 1, SoundVolume = 5})
	coroutine.resume(coroutine.create(function()
		for i=0, 4, 0.1 / Animation_Speed do
			Swait()
			EffectsAoE(dis.Position,40,3,6.5,90,"UpKnock",false)
		end
	end))
	WACKYEFFECT({Time = 5,EffectType = "Sphere", Size = VT(0,0,0)*sizey, Size2 = VT(1,1,1)*sizey, Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = 1, SoundVolume = 5})
	WACKYEFFECT({Time = 30,EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(3,3,3)*sizey, Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = 1, SoundVolume = 5})
	WACKYEFFECT({Time = 110,EffectType = "Sphere", Size = VT(0,0,0)*sizey, Size2 = VT(2,2,2)*sizey, Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = 1, SoundVolume = 5})
	WACKYEFFECT({Time = 70,EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(2.5,2.5,2.5)*sizey, Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = 1, SoundVolume = 5})
	for i = 0, 17 do
		WACKYEFFECT({Time = MRANDOM(30,190),EffectType = "Wave", Size = VT(0,0,0)*sizey, Size2 = VT(1.6,1.2,1.6)*sizey, Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(15,80), RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = 1, SoundVolume = 5})
	end
	for i = 0, 124 do
		WACKYEFFECT({Time = MRANDOM(20,200),EffectType = "Crystal", Size = VT(4,0,4), Size2 = VT(0,5+MRANDOM(5,150),0), Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position)*CFrame.new(MRANDOM(-35,35),0,MRANDOM(-35,35)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = MAINRUINCOLOR.Color, SoundID = nil, SoundPitch = 1, SoundVolume = 5})
	end
	coroutine.resume(coroutine.create(function()
		local eff = Instance.new("ParticleEmitter",dis)
		eff.Texture = "rbxassetid://2344870656"
		eff.LightEmission = 1
		eff.Color = ColorSequence.new(MAINRUINCOLOR.Color)
		eff.Rate = 10000000
		eff.Enabled = true
		eff.EmissionDirection = "Front"
		eff.Lifetime = NumberRange.new(1)
		eff.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,75,0),NumberSequenceKeypoint.new(0.1,20,0),NumberSequenceKeypoint.new(0.8,40,0),NumberSequenceKeypoint.new(1,60,0)})
		eff.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,0.8,0),NumberSequenceKeypoint.new(0.5,0,0),NumberSequenceKeypoint.new(1,1,0)})
		eff.Speed = NumberRange.new(150)
		eff.Drag = 5
		eff.Rotation = NumberRange.new(-500,500)
		eff.SpreadAngle = Vector2.new(0,900)
		eff.RotSpeed = NumberRange.new(-500,500)
		wait(0.2)
		eff.Enabled = false
	end))
	coroutine.resume(coroutine.create(function()
		local eff = Instance.new("ParticleEmitter",dis)
		eff.Texture = "rbxassetid://2092248396"
		eff.LightEmission = 1
		eff.Color = ColorSequence.new(MAINRUINCOLOR.Color)
		eff.Rate = 500000
		eff.Lifetime = NumberRange.new(0.5,2)
		eff.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,60,0),NumberSequenceKeypoint.new(0.2,3,0),NumberSequenceKeypoint.new(0.8,24,0),NumberSequenceKeypoint.new(1,0,0)})
		eff.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,1,0),NumberSequenceKeypoint.new(0.1,0,0),NumberSequenceKeypoint.new(0.8,0,0),NumberSequenceKeypoint.new(1,1,0)})
		eff.Speed = NumberRange.new(100,600)
		eff.Drag = 5
		eff.Rotation = NumberRange.new(-500,500)
		eff.VelocitySpread = 9000
		eff.RotSpeed = NumberRange.new(-50,50)
		wait(0.25)
		eff.Enabled = false
	end))
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0 - 0.04 * SIN(SINE / 24)*SIZE, 0 + 0.04 * SIN(SINE / 12)*SIZE, -0.65 + 0.05*SIZE * COS(SINE / 12)) * ANGLES(RAD(30 - 2.5 * SIN(SINE / 12)), RAD(-15), RAD(25)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1.15) - 1)) * ANGLES(RAD(-10 - 4 * SIN(SINE / 12)), RAD(0), RAD(-25)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.4, 0.6, -0.2) * ANGLES(RAD(60), RAD(0 + 2.5 * SIN(SINE / 25)), RAD(35 + 2.5 * SIN(SINE / 12))) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0.1) * ANGLES(RAD(-50), RAD(0 - 2.5 * SIN(SINE / 12)), RAD(-40 - 2.5 * SIN(SINE / 12))) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1.2*SIZE, 0.35*SIZE - 0.05*SIZE * COS(SINE / 12), -0.75*SIZE) * ANGLES(RAD(30 - 2.5 * SIN(SINE / 12)), RAD(79), RAD(0)) * ANGLES(RAD(-25), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1*SIZE, -1*SIZE - 0.05*SIZE * COS(SINE / 12), 0*SIZE) * ANGLES(RAD(-40 - 2.5 * SIN(SINE / 12)), RAD(-70), RAD(0)) * ANGLES(RAD(25), RAD(0), RAD(0)), 1 / Animation_Speed)
		if MRANDOM(1,10) == 1 then
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(MRANDOM(-5,5)), RAD(MRANDOM(-10,10)), RAD(MRANDOM(-10,10))), 1)
		end
	end
	ATTACK = false
	Rooted = false
end

function FairySwarm()
	ATTACK = true
	Rooted = true
	local eff1 = Instance.new("ParticleEmitter",sorb)
	eff1.Texture = "rbxasset://textures/particles/explosion01_implosion_main.dds"
	eff1.LightEmission = 1
	eff1.Color = ColorSequence.new(TEXTCOLOR)
	eff1.Rate = 1500
	eff1.Lifetime = NumberRange.new(0.5,1)
	eff1.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,2,0),NumberSequenceKeypoint.new(0.2,1,0),NumberSequenceKeypoint.new(1,0,0)})
	eff1.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,1,0),NumberSequenceKeypoint.new(0.2,0,0),NumberSequenceKeypoint.new(1,1,0)})
	eff1.Speed = NumberRange.new(0,7.5)
	eff1.Drag = 5
	eff1.Rotation = NumberRange.new(-500,500)
	eff1.VelocitySpread = 9000
	eff1.RotSpeed = NumberRange.new(-500,500)
	local eff2 = Instance.new("ParticleEmitter",sorb2)
	eff2.Texture = "rbxasset://textures/particles/explosion01_implosion_main.dds"
	eff2.LightEmission = 1
	eff2.Color = ColorSequence.new(TEXTCOLOR)
	eff2.Rate = 1500
	eff2.Lifetime = NumberRange.new(0.5,1)
	eff2.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,2,0),NumberSequenceKeypoint.new(0.2,1,0),NumberSequenceKeypoint.new(1,0,0)})
	eff2.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,1,0),NumberSequenceKeypoint.new(0.2,0,0),NumberSequenceKeypoint.new(1,1,0)})
	eff2.Speed = NumberRange.new(0,7.5)
	eff2.Drag = 5
	eff2.Rotation = NumberRange.new(-500,500)
	eff2.VelocitySpread = 9000
	eff2.RotSpeed = NumberRange.new(-500,500)
	coroutine.resume(coroutine.create(function()
		local BulletHolder = IT("Folder",Character)
		BulletHolder.Name = "Bullets"
		local ran=MRANDOM(1,3)
		local Effects2 = {}
		if ran==1 then
			bosschatfunc("Swarms!",TEXTCOLOR,3)
		elseif ran==2 then
			bosschatfunc("Magical Projectiles!!",TEXTCOLOR,3)
		elseif ran==3 then
			bosschatfunc("Try and dodge these!!",TEXTCOLOR,3)
		end
		game:GetService("Debris"):AddItem(BulletHolder, 800)
		for i = 1, 16 do
			Swait()
			coroutine.resume(coroutine.create(function()
				local ORB = IT('Part',BulletHolder)
				CreateSound(235097661,ORB,1.5,1.15,false) 
				ORB.Size = VT(0.5,0.5,0.5)
				ORB.CFrame = Torso.CFrame * CF(math.random(-20,20),math.random(10,30),math.random(-20,20))
				ORB.Anchored = true
				ORB.Color = TEXTCOLOR
				ORB.CanCollide = false
				ORB.Material = "Neon"
				local eff = Instance.new("ParticleEmitter",ORB)
				eff.Texture = "rbxasset://textures/particles/explosion01_implosion_main.dds"
				eff.LightEmission = 1
				eff.Color = ColorSequence.new(TEXTCOLOR)
				eff.Rate = 1500
				eff.Lifetime = NumberRange.new(0.5,1)
				eff.Size = NumberSequence.new({NumberSequenceKeypoint.new(0,4,0),NumberSequenceKeypoint.new(0.2,1,0),NumberSequenceKeypoint.new(1,0,0)})
				eff.Transparency = NumberSequence.new({NumberSequenceKeypoint.new(0,1,0),NumberSequenceKeypoint.new(0.2,0,0),NumberSequenceKeypoint.new(1,1,0)})
				eff.Speed = NumberRange.new(10,30)
				eff.Drag = 5
				eff.Rotation = NumberRange.new(-500,500)
				eff.VelocitySpread = 9000
				eff.RotSpeed = NumberRange.new(-500,500)
				coroutine.resume(coroutine.create(function()
					wait(0.25)
					eff.Enabled = false
				end))
				local BALLMESH = Instance.new("SpecialMesh", ORB)
				BALLMESH.MeshType = "Sphere"
				BALLMESH.Scale = Vector3.new(2,2,2)
				local AT1 = IT("Attachment",ORB)
				AT1.Position = CF(0, -0.08, 0).p
				local AT2 = IT("Attachment",ORB)
				AT2.Position = CF(0, 0.08, 0).p
				local TRAIL = script.Things.OrbTrail:Clone()
				TRAIL.Parent = ORB
				TRAIL.Attachment0 = AT1
				TRAIL.Attachment1 = AT2
			end))
		end
		local FINISH=false
		local BULLETHOLDERCHILDREN = BulletHolder:GetChildren()
		for i = 1, #BULLETHOLDERCHILDREN do
			Swait(5)
			local bullet = BULLETHOLDERCHILDREN[i]
			CreateSound(231917758,bullet,6,1.15,false) 
			local bv = Instance.new("BodyVelocity") 
			bullet.Mesh.Scale = VT(2,2,2)
			bullet.CFrame = CF(bullet.Position,Mouse.Hit.p)
			bv.maxForce = Vector3.new(1e9, 1e9, 1e9)
			bv.velocity = CF(bullet.Position,Mouse.Hit.p).lookVector*125
			bv.Parent = bullet
			bv.Name = "BodyVelocity"
			bullet.Anchored = false
			local hitted = false
			--[[local BULLETTOUCHED = bullet.Touched:Connect(function(hit)
				if bullet.Anchored == false then
					local cframe = bullet.CFrame
					bullet.Mesh.Scale = Vector3.new(2,2,2)
					CreateSound(588695064,bullet,6,0.85,false) 
					CreateSound(234365587,bullet,2,1.15,false) 
					CreateSound(233856106,bullet,5,0.9,false) 
					ManualAoE(bullet.Position,5,2,4,10,false)
                    WACKYEFFECT({EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(6,2,6), Transparency = 0.3, Transparency2 = 0, CFrame = cframe*ANGLES(RAD(MRANDOM(0,360)),RAD(MRANDOM(0,360)),(RAD(MRANDOM(0,360)))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 50})
       			end
			end)]]
			bullet.Touched:connect(function(hit) 
				if hitted == false and hit.Parent ~= char then
					hitted = true
					BULLETHOLDERCHILDREN[i].Anchored=true
					BULLETHOLDERCHILDREN[i].Transparency = 1
					local cframe = bullet.CFrame
					BULLETHOLDERCHILDREN[i].Mesh.Scale = Vector3.new(2,2,2)
					CreateSound(588695064,bullet,6,0.85,false) 
					CreateSound(234365587,bullet,1,1.15,false) 
					CreateSound(233856106,bullet,1.5,0.9,false) 
					ManualAoE(bullet.Position,9,5,15,1,false)
					for i = 1,5 do
						WACKYEFFECT({EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(1,12,1), Transparency = 0.3, Transparency2 = 0, CFrame = cframe*ANGLES(RAD(MRANDOM(0,360)),RAD(MRANDOM(0,360)),(RAD(MRANDOM(0,360)))), MoveToPos = nil, RotationX = MRANDOM(-10000/10000,10000/10000), RotationY = MRANDOM(-10000/10000,10000/10000), RotationZ = MRANDOM(-10000/10000,10000/10000), Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 50})
					end
					WACKYEFFECT({EffectType = "Box", Size = VT(0,0,0), Size2 = VT(5,5,5), Transparency = 0.6, Transparency2 = 0, CFrame = cframe*ANGLES(RAD(MRANDOM(0,360)),RAD(MRANDOM(0,360)),(RAD(MRANDOM(0,360)))), MoveToPos = nil, RotationX = MRANDOM(-10000/10000,10000/10000), RotationY = MRANDOM(-10000/10000,10000/10000), RotationZ = MRANDOM(-10000/10000,10000/10000), Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 50})
					wait(5)
					bullet:Destroy()
				end
			end)
			hitted = false
		end
		wait(5)
		for i = 1, 25 do
			wait()
			for _, c in pairs(BulletHolder:GetChildren()) do
				if c:IsA("Part") then
					c.Transparency = c.Transparency + 1/15
				end
			end
		end
		BulletHolder:Remove()
	end))
	for i=0, 0.6, 0.1 / Animation_Speed do
		Swait()
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-25 - 2* SIN(SINE / 40)), RAD(0), RAD(0)), 0.8 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.8, 0) * ANGLES(RAD(150-10* SIN(SINE / 54)), RAD(0), RAD(35)) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.8, 0) * ANGLES(RAD(150+10* SIN(SINE / 61)), RAD(0), RAD(-35)) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0+0.5* SIN(SINE / 71), 0+0.25* SIN(SINE / 68), 3+0.3* SIN(SINE / 30)) * ANGLES(RAD(5), RAD(0), RAD(0+2.5 * SIN(SINE / 45))), 0.8 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1 , -1, 0) * ANGLES(RAD(0), RAD(80+10* SIN(SINE / 78)), RAD(0)) * ANGLES(RAD(-3+1* SIN(SINE / 58)), RAD(0), RAD(2.5+8 * SIN(SINE / 43))), 0.8 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -0.5, -0.5) * ANGLES(RAD(0), RAD(-80-10* SIN(SINE / 81)), RAD(0)) * ANGLES(RAD(-3+1* SIN(SINE / 63)), RAD(0), RAD(0+15 * SIN(SINE / 49))), 0.8 / Animation_Speed)				
	end
	for i=0, 2.75, 0.1 / Animation_Speed do
		Swait()
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(5 - 2* SIN(SINE / 40)), RAD(0), RAD(-60)), 0.8 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(85-10* SIN(SINE / 54)), RAD(0), RAD(60)) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(0+10* SIN(SINE / 61)), RAD(0), RAD(-15)) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0+0.5* SIN(SINE / 71), 0+0.25* SIN(SINE / 68), 3+0.3* SIN(SINE / 30)) * ANGLES(RAD(-5), RAD(0), RAD(60+2.5 * SIN(SINE / 45))), 0.8 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1 , -1, 0) * ANGLES(RAD(0), RAD(80+10* SIN(SINE / 78)), RAD(0)) * ANGLES(RAD(-3+1* SIN(SINE / 58)), RAD(0), RAD(2.5+8 * SIN(SINE / 43))), 0.8 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -0.5, -0.5) * ANGLES(RAD(0), RAD(-80-10* SIN(SINE / 81)), RAD(0)) * ANGLES(RAD(-3+1* SIN(SINE / 63)), RAD(0), RAD(0+15 * SIN(SINE / 49))), 0.8 / Animation_Speed)				
	end
	eff1.Enabled = false
	eff2.Enabled = false
	ATTACK = false
	Rooted = false
	wait(1.2)
	eff1:Destroy()
	eff2:Destroy()
end

function CrystalBomb()
	ATTACK = true
	Rooted = false
	for i=0, 9, 0.1 / Animation_Speed do
		Swait()
		turnto(Mouse.Hit.Position)
		WACKYEFFECT({Time = 1, EffectType = "Crystal", Size = VT(0,0,0), Size2 = VT(20,i*9,20), Transparency = 1, Transparency2 = 0, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = 3*i*24, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-36 - 2* SIN(SINE / 40)), RAD(0), RAD(0)), 0.8 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(130), RAD(-1), RAD(23+4* SIN(SINE / 73))) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(130), RAD(1), RAD(-23+4* SIN(SINE / 73))) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0+0.25* SIN(SINE / 71), -0.35+0.25* SIN(SINE / 68), 3+1* SIN(SINE / 51)) * ANGLES(RAD(-35-2.5* SIN(SINE / 46)), RAD(0), RAD(0)), 0.8 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1 , -1, 0) * ANGLES(RAD(-35), RAD(80+10* SIN(SINE / 78)), RAD(0)) * ANGLES(RAD(-3+1* SIN(SINE / 58)), RAD(0), RAD(2.5+8 * SIN(SINE / 43))), 0.8 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -0.5, -0.5) * ANGLES(RAD(-35), RAD(-80-10* SIN(SINE / 81)), RAD(0)) * ANGLES(RAD(-3+1* SIN(SINE / 63)), RAD(0), RAD(0+15 * SIN(SINE / 49))), 0.8 / Animation_Speed)		
	end
	local dis = CreateParta(char,1,1,"Neon",BrickColor.new("Really red"))
	dis.CFrame = CF(Mouse.Hit.Position)
	WACKYEFFECT({EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(140,140,140), Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = 6, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = 1259054947, SoundPitch = 1, SoundVolume = 10})
	WACKYEFFECT({EffectType = "Wave", Size = VT(300,90,300), Size2 = VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = 6, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
	WACKYEFFECT({EffectType = "Wave", Size = VT(0,0,0), Size2 = VT(450,160,450), Transparency = 0, Transparency2 = 1, CFrame = CF(Mouse.Hit.Position), MoveToPos = nil, RotationX = 0, RotationY = -23, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
	EffectsAoE(Mouse.Hit.Position,302,60,90000,190,"Move",false)
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-36 - 2* SIN(SINE / 40)), RAD(0), RAD(0)), 0.8 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(130), RAD(-1), RAD(23+4* SIN(SINE / 73))) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(130), RAD(1), RAD(-23+4* SIN(SINE / 73))) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0+0.25* SIN(SINE / 71), -0.35+0.25* SIN(SINE / 68), 3+1* SIN(SINE / 51)) * ANGLES(RAD(35-2.5* SIN(SINE / 46)), RAD(0), RAD(0)), 0.8 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1 , -1, 0) * ANGLES(RAD(0), RAD(80+10* SIN(SINE / 78)), RAD(0)) * ANGLES(RAD(-3+1* SIN(SINE / 58)), RAD(0), RAD(2.5+8 * SIN(SINE / 43))), 0.8 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -0.5, -0.5) * ANGLES(RAD(0), RAD(-80-10* SIN(SINE / 81)), RAD(0)) * ANGLES(RAD(-3+1* SIN(SINE / 63)), RAD(0), RAD(0+15 * SIN(SINE / 49))), 0.8 / Animation_Speed)		
	end
	ATTACK = false
	Rooted = false
end

function lerp(object, newCFrame, alpha)
	return object:lerp(newCFrame, alpha)
end

--put this before snap

function snap()
	local TARGET = Mouse.Target
	if TARGET ~= nil then
		if TARGET.Parent:FindFirstChildOfClass("Humanoid") then
			local HUM = TARGET.Parent:FindFirstChildOfClass("Humanoid")
			local ROOT = TARGET.Parent:FindFirstChild("HumanoidRootPart") or TARGET.Parent:FindFirstChild("Torso") or TARGET.Parent:FindFirstChild("UpperTorso")
			if ROOT and HUM.Health > 0 then
				local FOE = Mouse.Target.Parent
				local HEAD = FOE:FindFirstChild("Head")
				local ROOTFOE = FOE:FindFirstChild("HumanoidRootPart")
				if HEAD then
					ATTACK = true
					Rooted = true
					CharacterFade(C3(0,0,0),120)
					for _, c in pairs(FOE:GetChildren()) do
						if c.ClassName == "Part" then
							c.Anchored = true
						end
					end
					CreateSound(235097614, Torso, 1, 3, false)
					RootPart.CFrame = HEAD.CFrame * CFrame.new(0,-1.5,2)
					CharacterFade(C3(0,0,0),120)
					Cam.CameraType = "Scriptable"
					for i=0, 1.25, 0.1 / Animation_Speed do
						Swait()
						Cam.CFrame = lerp(cam.CFrame, Character.HumanoidRootPart.CFrame*CFrame.new(4.5, 0.5, -3)*CFrame.Angles(math.rad(-20),math.rad(110),math.rad(0)), 0.1)
						RootPart.CFrame = HEAD.CFrame * CFrame.new(0,-1.5,2)
						RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, .4, 0) * ANGLES(RAD(10), RAD(0), RAD(0)), 1 / Animation_Speed)
						Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(15), RAD(0), RAD(0)), 1 / Animation_Speed)
						RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(20)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
						LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(-20)) * LEFTSHOULDERC0, 1 / Animation_Speed)
						RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(10), RAD(85), RAD(0)) * ANGLES(RAD(-1), RAD(0), RAD(0)), 1 / Animation_Speed)
						LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(10), RAD(-85), RAD(0)) * ANGLES(RAD(-1), RAD(0), RAD(0)), 1 / Animation_Speed)
					end
					--Cam.CameraType = Enum.CameraType.Scriptable
					for i = 0, 21 do

					end
					for i=0, 1.25, 0.1 / Animation_Speed do
						Swait()
						Cam.CFrame = lerp(cam.CFrame, Character.HumanoidRootPart.CFrame*CFrame.new(4.5, 0.5, -3)*CFrame.Angles(math.rad(-20),math.rad(110),math.rad(0)), 0.1)
						RootPart.CFrame = HEAD.CFrame * CFrame.new(0,-1.5,2)
						RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
						Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
						RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(30)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
						LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.1, 0.5, -0.6) * ANGLES(RAD(130), RAD(0), RAD(0)) * LEFTSHOULDERC0, 1 / Animation_Speed)
						RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(0), RAD(85), RAD(0)) * ANGLES(RAD(-1), RAD(0), RAD(0)), 1 / Animation_Speed)
						LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(0), RAD(-85), RAD(0)) * ANGLES(RAD(-1), RAD(0), RAD(0)), 1 / Animation_Speed)
					end
					--[[for i = 0, 4 do
					MagicSphere(VT(3,3,3),5,HEAD.CFrame,"Really black",VT(0,10,0))
					end]]
					for i=0, 0.5, 0.1 / Animation_Speed do
						Swait()
						RootPart.CFrame = HEAD.CFrame * CFrame.new(0,-1.5,2)
						RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
						Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
						RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(30)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
						LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.1, 0.5, -0.6) * ANGLES(RAD(130), RAD(0), RAD(0)) * LEFTSHOULDERC0, 1 / Animation_Speed)
						RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(0), RAD(85), RAD(0)) * ANGLES(RAD(-1), RAD(0), RAD(0)), 1 / Animation_Speed)
						LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(0), RAD(-85), RAD(0)) * ANGLES(RAD(-1), RAD(0), RAD(0)), 1 / Animation_Speed)
					end
					CreateSound(363808674, HEAD, 15, 1, false)
					CreateSound(1548538202, HEAD, 7, 1, false)
					CreateSound(2237297994, HEAD, 8, 1, false)
					CreateSound(675172759, HEAD, 6, 0.7, false)
					CreateSound(763718160, HEAD, 7, 1, false)
					WACKYEFFECT({Time = 50 ,EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(150,150,150), Transparency = 0, Transparency2 = 1, CFrame = HEAD.CFrame*CF(0,0,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(27/255,42/255,53/255), SoundID = nil, SoundPitch = nil, SoundVolume = nil})	
					WACKYEFFECT({Time = 150 ,EffectType = "Block", Size = VT(0,0,0), Size2 = VT(180,180,180), Transparency = 0, Transparency2 = 1, CFrame = HEAD.CFrame*CF(0,0,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(27/255,42/255,53/255), SoundID = nil, SoundPitch = nil, SoundVolume = nil})
					local Sizey = 1
					FOE:BreakJoints()
					ApplyDamage(HUM,0,true)
					for i = 0, 12 do
						Chunks(HEAD)
					end
					ROOT.Anchored = false
					ROOT:remove()
					for i=0, 3, 0.1 / Animation_Speed do
						Swait()
						Sizey = Sizey + 0.1
						for i = 0, 2 do
							WACKYEFFECT({Time = math.random(10,60) ,EffectType = "Crystal", Size = VT(5,0,5), Size2 = VT(0,125,0), Transparency = 0, Transparency2 = 1, CFrame = ROOT.CFrame*CF(0,0,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(27/255,42/255,53/255), SoundID = nil, SoundPitch = nil, SoundVolume = nil})
						end
						WACKYEFFECT({Time = 20 ,EffectType = "Sphere", Size = VT(0,9999,0), Size2 = VT(5+Sizey,9999,5+Sizey), Transparency = 0, Transparency2 = 1, CFrame = ROOT.CFrame*CF(0,0,0), MoveToPos = nil, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(27/255,42/255,53/255), SoundID = nil, SoundPitch = nil, SoundVolume = nil})
						CameraEnshaking(0.5,8)
						RootPart.CFrame = HEAD.CFrame * CFrame.new(0,-1.5,2)
						RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
						Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
						RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(130), RAD(0), RAD(30)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
						LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.1, 0.5, -0.6) * ANGLES(RAD(130), RAD(0), RAD(0)) * LEFTSHOULDERC0, 1 / Animation_Speed)
						RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(0), RAD(85), RAD(0)) * ANGLES(RAD(-1), RAD(0), RAD(0)), 1 / Animation_Speed)
						LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(0), RAD(-85), RAD(0)) * ANGLES(RAD(-1), RAD(0), RAD(0)), 1 / Animation_Speed)
					end
					for i=0, 0.5, 0.1 / Animation_Speed do
						Swait()
						RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0 + 0.05 * COS(SINE / 12)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
						Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0 - 2.5 * COS(SINE / 12)), RAD(0), RAD(0)), 1 / Animation_Speed)
						RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.1, 0.65, -1.5) * ANGLES(RAD(130), RAD(0), RAD(-35)) * RIGHTSHOULDERC0, 2 / Animation_Speed)
						LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.1, 0.5, 0) * ANGLES(RAD(130), RAD(0), RAD(0)) * LEFTSHOULDERC0, 2 / Animation_Speed)
						RightHip.C0 = Clerp(RightHip.C0, CF(1, -1 - 0.05 * COS(SINE / 12), -0.01) * ANGLES(RAD(0), RAD(85), RAD(0)) * ANGLES(RAD(-1), RAD(0), RAD(0)), 1 / Animation_Speed)
						LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1 - 0.05 * COS(SINE / 12), -0.01) * ANGLES(RAD(0), RAD(-85), RAD(0)) * ANGLES(RAD(-1), RAD(0), RAD(0)), 1 / Animation_Speed)
					end
					Cam.CameraType = Enum.CameraType.Custom
					for _, c in pairs(FOE:GetChildren()) do
						if c.ClassName == "Part" then
							c.Anchored = false
						end
					end
					ATTACK = false
					Rooted = false
				end
			end
		end
	end
end

function HellPillarsSummoning()
	local HITFLOOR,HITPOS = Raycast(RootPart.Position, (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 12, Character)
	if HITFLOOR then
		ATTACK = true
		Rooted = true
		CreateSound(255679384, RootPart, 10, MRANDOM(13,15)/10, false)
		CreateSound(163619849, RootPart, 1, 1.4, false)
		for i=0, 3, 0.1 / Animation_Speed do
			Swait()
			if MRANDOM(1,4)==1 then
				WACKYEFFECT({EffectType = "Box", Size = VT(1,0,1), Size2 = VT(0,3,0)*MRANDOM(1,2), Transparency = 0, Transparency2 = 1, CFrame = RightArm.CFrame*CF(0,-1,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
				WACKYEFFECT({EffectType = "Box", Size = VT(1,0,1), Size2 = VT(0,3,0)*MRANDOM(1,2), Transparency = 0, Transparency2 = 1, CFrame = LeftArm.CFrame*CF(0,-1,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			end
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0+math.random(-15,15)), RAD(0+math.random(-15,15)), RAD(0+math.random(-15,15))), 3 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.25, 0.5, -0.25) * ANGLES(RAD(75+math.random(-10,10)), RAD(-4+math.random(-10,10)), RAD(-34+math.random(-10,10))) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.25, 0.5, -0.25) * ANGLES(RAD(75+math.random(-10,10)), RAD(4+math.random(-10,10)), RAD(34+math.random(-10,10))) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(75), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-75), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
		CreateSound(689743416, RootPart, 8, MRANDOM(13,15)/10, false)
		coroutine.resume(coroutine.create(function()
			local CFRAME = RootPart.CFrame*CF(0,-3,0)
			for i = 1, 15 do
				local SPIKE = GetNeonPillar(Vector3.new(10,0,10))
				SPIKE.CFrame = RootPart.CFrame*CF(math.random(-100,100),-5,math.random(-100,100))
				local FLOORD = GetNeonPillar(Vector3.new(0,0,0))
				FLOORD.CFrame = SPIKE.CFrame*CF(0,2,0)
				EffectsAoE(FLOORD.Position,15,50,95,150,"Up",false,nil)
				CreateSound(203691785, FLOORD, 3, 1.1, false)
				SPIKE.CFrame = SPIKE.CFrame*ANGLES(RAD(math.random(-5,5)),RAD(math.random(-5,5)),RAD(math.random(-5,5)))
				coroutine.resume(coroutine.create(function()
					local sizey = 0
					local sizey2 = 0
					for i = 1, 30 do
						Swait()
						sizey = sizey + 1
						sizey2 = sizey2 + 3
						FLOORD.Size = Vector3.new(sizey,sizey/25,sizey)
						--EffectsAoE(SPIKE.Position,15,10,35,0,"Normal",false,nil)
						SPIKE.Size = Vector3.new(10,sizey2,10)
					end
					wait(1.5)
					for i = 1, 100 do
						Swait()
						sizey2 = sizey2 - 1
						sizey = sizey - 0.2
						FLOORD.Size = Vector3.new(sizey,sizey/25,sizey)
						FLOORD.Transparency = FLOORD.Transparency + 1/85
						SPIKE.Transparency = SPIKE.Transparency + 1/85
						SPIKE.Size = Vector3.new(10,sizey2,10)
					end
					FLOORD.CanCollide = false
					SPIKE.CanCollide = false
					SPIKE.Firey.Enabled = false
					FLOORD.Firey.Enabled = false
					wait(2.5)
					SPIKE:remove()
					FLOORD:remove()
				end))
				swait(4)
			end
		end))
		for i=0, 0.5, 0.1 / Animation_Speed do
			Swait()
			if MRANDOM(1,4)==1 then
				WACKYEFFECT({EffectType = "Box", Size = VT(1,0,1), Size2 = VT(0,3,0)*MRANDOM(1,2), Transparency = 0, Transparency2 = 1, CFrame = RightArm.CFrame*CF(0,-1,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
				WACKYEFFECT({EffectType = "Box", Size = VT(1,0,1), Size2 = VT(0,3,0)*MRANDOM(1,2), Transparency = 0, Transparency2 = 1, CFrame = LeftArm.CFrame*CF(0,-1,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			end
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(10), RAD(0), RAD(0)), 1 / Animation_Speed)
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(20+math.random(-15,15)), RAD(0+math.random(-15,15)), RAD(0+math.random(-15,15))), 3 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(10+math.random(-10,10)), RAD(-4+math.random(-10,10)), RAD(40+math.random(-10,10))) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(10+math.random(-10,10)), RAD(4+math.random(-10,10)), RAD(-40+math.random(-10,10))) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(10), RAD(75), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(10), RAD(-75), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
		ATTACK = false
		Rooted = false
	end
end

function Chaotic_Rain()
	local HITFLOOR,HITPOS = Raycast(RootPart.Position, (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 12, Character)
	ATTACK = true
	Rooted = true
	local Release = false
	coroutine.resume(coroutine.create(function()
		CreateSound(1398471768, RootPart, 3, 1.1, false)
		CreateSound(2349519342, RootPart, 3, 0.8, false)
		local S=false
		for i = 1, 25 do
			local STAR = GetStars()
			--EffectsAoE(CFRAME*CF(0,0,-i*8).p,7,10,35,75,"Up",false)
			coroutine.resume(coroutine.create(function()
				repeat
					STAR.CFrame = Torso.CFrame*CF(0,0,-2.5)*ANGLES(RAD(-90),RAD(0),RAD(0))
					STAR.Transparency = STAR.Transparency - 0.025
					swait()
				until STAR.Transparency < 0.5
			end))
			coroutine.resume(coroutine.create(function()
				repeat
					swait()
				until Release==true
				local POS = RootPart.CFrame*CF(MRANDOM(-50,50),MRANDOM(30,50),MRANDOM(-50,50))*ANGLES(RAD(-90+MRANDOM(-10,10)),RAD(MRANDOM(-10,10)),RAD(MRANDOM(-10,10)))
				for i = 1, 60 do
					Swait()
					STAR.CFrame = Clerp(STAR.CFrame, POS, 0.3 / Animation_Speed)
				end
				STAR.Anchored = false
				local bv = Instance.new("BodyVelocity")
				bv.maxForce = Vector3.new(1e9, 1e9, 1e9)
				bv.velocity = STAR.CFrame.lookVector*100
				bv.Parent = STAR
				game:GetService("Debris"):AddItem(STAR, 5)
				local hitted = false
				coroutine.resume(coroutine.create(function()
					--swait(2.5)
					STAR.Touched:connect(function(hit) 
						if hitted == false and hit.Parent ~= char then
							hitted = true
							STAR.Anchored=true
							STAR.Transparency = 1
							CameraEnshaking(3,7)
							CreateSound(695165857, STAR, 2, 1.1, false)
							CreateSound(1127797047, STAR, 1, 1, false)
							CreateSound(203691785, STAR, 1.5, 1.25, false)
							EffectsAoE(STAR.Position,12.5,15,30,50,"Fling",false,nil)
							WACKYEFFECT({Time = 90 ,EffectType = "Block", Size = VT(0,0,0), Size2 = VT(15,15,15), Transparency = 0, Transparency2 = 1, CFrame = STAR.CFrame*CF(0,0,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(8/255,0,255/255), SoundID = nil, SoundPitch = nil, SoundVolume = nil})
							WACKYEFFECT({Time = 30 ,EffectType = "Block", Size = VT(0,0,0), Size2 = VT(25,25,25), Transparency = 0, Transparency2 = 1, CFrame = STAR.CFrame*CF(0,0,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(8/255,0,255/255), SoundID = nil, SoundPitch = nil, SoundVolume = nil})
							for i = 0, 3 do
								WACKYEFFECT({Time = MRANDOM(20,60) ,EffectType = "Slash", Size = VT(0,0,0), Size2 = VT(0.125,0.0125,0.125)*MRANDOM(1,2), Transparency = 0, Transparency2 = 1, CFrame = STAR.CFrame*CF(0,0,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(8/255,0,255/255), SoundID = nil, SoundPitch = nil, SoundVolume = nil})
							end
							symbolizeBlink(STAR,0,259201913,Color3.new(8/255,0,1),1.5,0,0,0,STAR,true,math.random(-10,10),1)
							wait(5)
							STAR:Destroy()
						end
					end)
				end))
			end))
			--Swait()
		end
	end))
	for i=0, 0.75, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(10)), 0.9 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(15), RAD(0), RAD(0)), 0.9 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.25, 0.5, -0.25) * ANGLES(RAD(65), RAD(-4), RAD(-34)) * RIGHTSHOULDERC0, 0.9 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.25, 0.5, -0.25) * ANGLES(RAD(65), RAD(4), RAD(34)) * LEFTSHOULDERC0, 0.9 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(75), RAD(0)) * ANGLES(RAD(-4), RAD(0), RAD(0)), 0.9 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-75), RAD(0)) * ANGLES(RAD(-4), RAD(0), RAD(0)), 0.9 / Animation_Speed)
	end
	CreateSound(596439421, RootPart, 25, 1, false)
	Release=true
	for i=0, 0.25, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(-10), RAD(0), RAD(0)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-20), RAD(0), RAD(0)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.8, 0) * ANGLES(RAD(155), RAD(-4), RAD(40)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.8, 0) * ANGLES(RAD(155), RAD(4), RAD(-40)) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(-10), RAD(75), RAD(0)) * ANGLES(RAD(-4), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(-10), RAD(-75), RAD(0)) * ANGLES(RAD(-4), RAD(0), RAD(0)), 1 / Animation_Speed)
	end
	wait(0.3)
	ATTACK = false
	Rooted = false
end

function SETUPWINDWAVE(CFRAME,MINDMG,DMG,EFFECTS,MINDMG2,DMG2,EFFECTS2,STARTUP,DISTANCE)
	local TORNADO = GetTestBlock()
	TORNADO.CFrame = CFRAME
	local DISTANCES=0
	coroutine.resume(coroutine.create(function()
		for i = 1, STARTUP do
			DISTANCES = DISTANCES + 0.1 + DISTANCES/20
			WACKYEFFECT({Time = 30 ,EffectType = "Swirl", Size = VT(5,5,5), Size2 = VT(17.5,20,17.5), Transparency = 0, Transparency2 = 1, CFrame = TORNADO.CFrame*CF(0,0,0)*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-15,15), RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			WACKYEFFECT({Time = 25 ,EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(10,10,10), Transparency = 0, Transparency2 = 1, CFrame = TORNADO.CFrame*CF(0,0,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.new("Bright green").Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			EffectsAoE(TORNADO.Position,12.5,MINDMG,DMG,EFFECTS,"Poison",false)
			TORNADO.CFrame = TORNADO.CFrame * CF(0,0,-DISTANCES)
			CreateSound(362395087,TORNADO,1,1,false)
			CreateSound(199149137,TORNADO,0.25,1,false)
			Swait(7.5)
		end					
		for i = 1, DISTANCE do
			DISTANCES = DISTANCES + 0.1 + DISTANCES/15
			WACKYEFFECT({Time = 30 ,EffectType = "Swirl", Size = VT(10,12.5,10), Size2 = VT(17.5,20,17.5), Transparency = 0, Transparency2 = 1, CFrame = TORNADO.CFrame*CF(0,0,0)*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-15,15), RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			WACKYEFFECT({Time = 25 ,EffectType = "Sphere", Size = VT(8,8,8), Size2 = VT(10,10,10), Transparency = 0, Transparency2 = 1, CFrame = TORNADO.CFrame*CF(0,0,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.new("Bright green").Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			if math.random(1,5)==1 then
				WACKYEFFECT({Time = 30 ,EffectType = "Sphere", Size = VT(4,0,4), Size2 = VT(0,80,0), Transparency = 0, Transparency2 = 1, CFrame = TORNADO.CFrame*CF(0,0,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.new("Bright green").Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			end
			EffectsAoE(TORNADO.Position,13.5,MINDMG2,DMG2,EFFECTS2,"Poison",false)
			TORNADO.CFrame = TORNADO.CFrame * CF(0,0,-DISTANCES)
			CreateSound(362395087,TORNADO,0.75,1,false)
			CreateSound(199149137,TORNADO,1,1,false)
			Swait(7.5)
		end
		for i = 1, 45 do
			Swait()
			TORNADO.Transparency = TORNADO.Transparency + 1/45
		end
		TORNADO:remove()
	end))
end

function Toxic_WindWaves()
	ATTACK = true
	Rooted = true
	local GYRO = IT("BodyGyro",RootPart)
	GYRO.D = 2
	GYRO.P = 2000
	GYRO.MaxTorque = VT(0,4000000,0)
	coroutine.resume(coroutine.create(function()
		repeat
			Swait()
			GYRO.CFrame = CF(RootPart.Position,Mouse.Hit.p)
		until ATTACK == false
		GYRO:Remove()
	end))
	CreateSound(199145659,RightArm,1,1.3,false)
	for i=0, 1.5, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(80)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(10), RAD(0), RAD(-65)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(90), RAD(0), RAD(-25)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(-15)) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 1 / Animation_Speed)
	end
	SETUPWINDWAVE(RootPart.CFrame*CF(0,0,-3),1,1,80,1,4,40,15,15)
	for i=0, 1.25, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(65)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(10), RAD(0), RAD(-65)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(90), RAD(0), RAD(55)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(-5), RAD(0), RAD(-10)) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 1 / Animation_Speed)
	end
	CreateSound(199145659,LeftArm,1,1.3,false)
	for i=0, 0.75, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0, ROOTC0 * CF(0, 0, -1) * ANGLES(RAD(0), RAD(5), RAD(65)), 0.5 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0) * ANGLES(RAD(10), RAD(0), RAD(-65)), 0.15 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(-20)) * RIGHTSHOULDERC0, 0.5 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1, 0.5, -0.5) * ANGLES(RAD(90), RAD(0), RAD(90)) * LEFTSHOULDERC0, 0.5 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -0.5, -0.5) * ANGLES(RAD(0), RAD(80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(-5)), 0.5 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -0.75, -0.25) * ANGLES(RAD(0), RAD(-80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(50)), 0.5 / Animation_Speed)
	end
	for i=0, 0.75, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(60)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1.2)) * ANGLES(RAD(15), RAD(0), RAD(-60)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(10), RAD(0), RAD(-20)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(50), RAD(0), RAD(-50)) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 1 / Animation_Speed)
	end
	SETUPWINDWAVE(RootPart.CFrame*ANGLES(RAD(0),RAD(22.5),RAD(0))*CF(0,0,-3),1,1,35,1,2,20,20,5)
	SETUPWINDWAVE(RootPart.CFrame*ANGLES(RAD(0),RAD(-22.5),RAD(0))*CF(0,0,-3),1,1,35,1,2,20,20,5)
	for i=0, 1.25, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(-45)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(15), RAD(0), RAD(45)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(-10), RAD(0), RAD(-10)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1, 0.5, -0.5) * ANGLES(RAD(80), RAD(0), RAD(65)) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 1 / Animation_Speed)
	end
	CreateSound(199145659,LeftArm,1,1.3,false)
	CreateSound(199145659,RightArm,1,1.3,false)
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.8 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(10), RAD(0), RAD(0)), 0.8 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(90), RAD(0), RAD(80)) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(90), RAD(0), RAD(-80)) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.8 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.8 / Animation_Speed)
	end
	SETUPWINDWAVE(RootPart.CFrame*ANGLES(RAD(0),RAD(22.5),RAD(0))*CF(0,0,-3),1,1,10,1,7,10,7,16)
	SETUPWINDWAVE(RootPart.CFrame*ANGLES(RAD(0),RAD(0),RAD(0))*CF(0,0,-3),1,1,10,1,7,10,7,17)
	SETUPWINDWAVE(RootPart.CFrame*ANGLES(RAD(0),RAD(-22.5),RAD(0))*CF(0,0,-3),1,1,10,1,7,10,7,16)
	CreateSound(199145659,LeftArm,1,1.3,false)
	CreateSound(199145659,RightArm,1,1.3,false)
	for i=0, 2.25, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, -0.05) * ANGLES(RAD(5), RAD(0), RAD(0)), 0.6 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(15), RAD(0), RAD(0)), 0.6 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1, 0.5, -0.5) * ANGLES(RAD(85), RAD(0), RAD(-80)) * RIGHTSHOULDERC0, 0.6 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1, 0.5, -0.5) * ANGLES(RAD(95), RAD(0), RAD(80)) * LEFTSHOULDERC0, 0.6 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(5), RAD(80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.6 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(5), RAD(-80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.6 / Animation_Speed)
	end
	SETUPWINDWAVE(RootPart.CFrame*ANGLES(RAD(0),RAD(45),RAD(0))*CF(0,0,-3),1,1,5,1,5,10,8,19)
	SETUPWINDWAVE(RootPart.CFrame*ANGLES(RAD(0),RAD(22.5),RAD(0))*CF(0,0,-3),1,1,5,1,5,10,8,19)
	SETUPWINDWAVE(RootPart.CFrame*ANGLES(RAD(0),RAD(0),RAD(0))*CF(0,0,-3),1,1,5,1,5,10,8,19)
	SETUPWINDWAVE(RootPart.CFrame*ANGLES(RAD(0),RAD(-22.5),RAD(0))*CF(0,0,-3),1,1,5,1,5,10,8,19)
	SETUPWINDWAVE(RootPart.CFrame*ANGLES(RAD(0),RAD(-45),RAD(0))*CF(0,0,-3),1,1,5,1,5,10,8,19)
	for i=0, 1.5, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(-5), RAD(0), RAD(0)), 0.8 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(10), RAD(0), RAD(0)), 0.8 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(90), RAD(0), RAD(65)) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(90), RAD(0), RAD(-65)) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(-5), RAD(80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.8 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(-5), RAD(-80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.8 / Animation_Speed)
	end
	ATTACK = false
	Rooted = false
end

function Spins_Toxicified()
	ATTACK = true
	Rooted = true
	--CreateSound(199145659,RightArm,1,1.3,false)
	local spin=MRANDOM(-360,360)
	local spin2=0
	for i=0, 2, 0.1 / Animation_Speed do
		Swait()
		spin = spin + 5
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(spin)), 0.7 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(10), RAD(0), RAD(0)), 0.7 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(80)) * RIGHTSHOULDERC0, 0.7 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(-80)) * LEFTSHOULDERC0, 0.7 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.7 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.7 / Animation_Speed)
	end
	spin2 = MRANDOM(-360,360)
	for i=0, 8 do
		for i=0, 0.6, 0.1 / Animation_Speed do
			Swait()
			spin = spin + 15
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(spin)), 1 / Animation_Speed)
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(10), RAD(0), RAD(0)), 1 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(80)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(-80)) * LEFTSHOULDERC0, 1 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 1 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
		spin2 = spin2 + 45
		SETUPWINDWAVE(RootPart.CFrame*ANGLES(RAD(0),RAD(spin2),RAD(0))*CF(0,0,-3),1,1,40,1,2,20,10,MRANDOM(10,15))
	end
	ATTACK = false
	Rooted = false
end

function Crystal_MarkDown()
	ATTACK = true
	Rooted = true
	local GYRO = IT("BodyGyro",RootPart)
	GYRO.D = 2
	GYRO.P = 2000
	GYRO.MaxTorque = VT(0,4000000,0)
	coroutine.resume(coroutine.create(function()
		repeat
			Swait()
			GYRO.CFrame = CF(RootPart.Position,Mouse.Hit.p)
		until ATTACK == false
		GYRO:Remove()
	end))
	for i=0, 0.85, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(-15)), 0.7 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(10), RAD(0), RAD(15)), 0.7 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(-15), RAD(5), RAD(10)) * RIGHTSHOULDERC0, 0.7 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.3, 0.5, 0) * ANGLES(RAD(15), RAD(-10), RAD(10)) * LEFTSHOULDERC0, 0.7 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(80), RAD(0)) * ANGLES(RAD(-4), RAD(0), RAD(-7.5)), 0.7 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-80), RAD(0)) * ANGLES(RAD(-4), RAD(0), RAD(-5)), 0.7 / Animation_Speed)
	end


	CreateSound(147722227,RightArm,10,0.9,false)
	local SPIKE = GetTestBlock()
	SPIKE.CFrame = RootPart.CFrame*CF(0,0,0)*ANGLES(RAD(180),RAD(0),RAD(0))
	local CFRAMES=RootPart.CFrame*CF(0,-2,-15)
	coroutine.resume(coroutine.create(function()
		wait(0.25)
		repeat
			ohgod=ohgod+1
			WACKYEFFECT({Time = 1, EffectType = "Crystal", Size = VT(10,15,10), Size2 = VT(10,15,10), Transparency = 0.6, Transparency2 = 0.6, CFrame = SPIKE.CFrame*CF(0,-10+0.5* SIN(SINE / 70),0)*ANGLES(RAD(0),RAD(ohgod),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			WACKYEFFECT({Time = 1, EffectType = "Wave", Size = VT(12,5,12), Size2 =VT(12,5,12), Transparency = 0.6, Transparency2 = 0.6, CFrame = SPIKE.CFrame*CF(0,-1.5,0)*ANGLES(RAD(180),RAD(0+ohgod*2),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			WACKYEFFECT({Time = 1, EffectType = "Wave", Size = VT(4.5,1.35,4.5), Size2 =VT(4.5,1.35,4.5), Transparency = 0.6, Transparency2 = 0.6, CFrame = SPIKE.CFrame*CF(0,0.25,0)*ANGLES(RAD(180),RAD(0-ohgod*2),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			swait()
		until ToxicCrystalEquipped==true
		WACKYEFFECT({Time = 90, EffectType = "Crystal", Size = VT(10,15,10), Size2 = VT(10,15,10), Transparency = 0.6, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(0,-10+0.5* SIN(SINE / 70),0)*ANGLES(RAD(0),RAD(ohgod),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 2, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		WACKYEFFECT({Time = 90, EffectType = "Wave", Size = VT(12,5,12), Size2 =VT(12,5,12), Transparency = 0.6, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(0,-1.5,0)*ANGLES(RAD(180),RAD(0+ohgod*2),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 2, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		WACKYEFFECT({Time = 90, EffectType = "Wave", Size = VT(4.5,1.35,4.5), Size2 =VT(4.5,1.35,4.5), Transparency = 0.6, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(0,0.25,0)*ANGLES(RAD(180),RAD(0-ohgod*2),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = -2, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
	end))
	coroutine.resume(coroutine.create(function()
		ToxicCrystalEquipped=false
		local POS = CFRAMES*ANGLES(RAD(180),RAD(MRANDOM(-360,360)),RAD(0))
		for i = 1, 75 do
			Swait()
			SPIKE.CFrame = Clerp(SPIKE.CFrame, POS, 0.2 / Animation_Speed)
		end
		for i = 1, 7 do
			CreateSound(315743331,SPIKE,5,1,false)
			CreateSound(743521450,SPIKE,2,0.8,false)
			EffectsAoE(SPIKE.Position,65,5,10,40,"Poison",false)
			WACKYEFFECT({Time = 40, EffectType = "Sphere", Size = VT(0,1.5,0), Size2 =VT(125,1.5,125), Transparency = 0, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(0,-1,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			for i = 0, 35 do
				WACKYEFFECT({Time = MRANDOM(40,90), EffectType = "Sphere", Size = VT(2.5,0,2.5), Size2 =VT(0,15,0), Transparency = 0, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(MRANDOM(-50,50),0.3,MRANDOM(-50,50))*ANGLES(RAD(MRANDOM(-10,10)),RAD(MRANDOM(-10,10)),RAD(MRANDOM(-10,10))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			end
			for i = 0, 3 do
				WACKYEFFECT({Time = 60, EffectType = "Sphere", Size = VT(0,0,0), Size2 =VT(35,35,35), Transparency = 0, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(0,-10,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
				WACKYEFFECT({Time = 60, EffectType = "Sphere", Size = VT(0,0,0), Size2 =VT(25,25,25), Transparency = 0.5, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(0,-10,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
				--WACKYEFFECT({Time = MRANDOM(30,60), EffectType = "Sphere", Size = VT(0,0,0), Size2 =VT(10,10,10), Transparency = 0, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(MRANDOM(-10,10),MRANDOM(-22.5,0),MRANDOM(-10,10))*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
				swait(6)
			end
			swait(100)
		end
		CreateSound(235097614,SPIKE,9,1.15,false)
		for i = 0, 60 do
			WACKYEFFECT({Time = MRANDOM(45,95), EffectType = "Sphere", Size = VT(5,5,5), Size2 =VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(MRANDOM(-10,10),MRANDOM(-22.5,0),MRANDOM(-10,10))*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			swait()
		end
		CreateSound(315743331,SPIKE,15,1.25,false)
		CreateSound(743521450,SPIKE,7,1.4,false)
		WACKYEFFECT({Time = 20, EffectType = "Block", Size = VT(0,0,0), Size2 =VT(150,150,150), Transparency = 0, Transparency2 = 1, CFrame = SPIKE.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 11.25, RotationY = 11.25, RotationZ = 11.25, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		CameraEnshaking(5,3)
		EffectsAoE(SPIKE.Position,95,5,5,150,"Poison",false)
		WACKYEFFECT({Time = 40, EffectType = "Ring", Size = VT(0,0,0.1), Size2 =VT(15,15,0.1), Transparency = 0, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(0,-1,0)*ANGLES(RAD(90),RAD(0),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		ToxicCrystalEquipped=true
		coroutine.resume(coroutine.create(function()
			for i = 0, 60 do
				WACKYEFFECT({Time = MRANDOM(45,95), EffectType = "Sphere", Size = VT(5,5,5), Size2 =VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(MRANDOM(-60,60),MRANDOM(-40,0.25),MRANDOM(-60,60))*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
				swait()
			end
		end))
		for i = 0, 3 do
			WACKYEFFECT({Time = 60, EffectType = "Sphere", Size = VT(0,0,0), Size2 =VT(35,35,35)*3, Transparency = 0, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(0,-10,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			WACKYEFFECT({Time = 60, EffectType = "Sphere", Size = VT(0,0,0), Size2 =VT(25,25,25)*3, Transparency = 0.5, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(0,-10,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			--WACKYEFFECT({Time = MRANDOM(30,60), EffectType = "Sphere", Size = VT(0,0,0), Size2 =VT(10,10,10), Transparency = 0, Transparency2 = 1, CFrame = SPIKE.CFrame*CF(MRANDOM(-10,10),MRANDOM(-22.5,0),MRANDOM(-10,10))*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			swait(6)
		end
		wait(2.5)
		SPIKE:Destroy()
	end))
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(15)), 0.7 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(10), RAD(0), RAD(-15)), 0.7 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(90), RAD(5), RAD(15)) * RIGHTSHOULDERC0, 0.7 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.3, 0.5, 0) * ANGLES(RAD(15), RAD(-10), RAD(10)) * LEFTSHOULDERC0, 0.7 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(80), RAD(0)) * ANGLES(RAD(-4), RAD(0), RAD(-7.5)), 0.7 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-80), RAD(0)) * ANGLES(RAD(-4), RAD(0), RAD(-5)), 0.7 / Animation_Speed)
	end
	ATTACK = false
	Rooted = false
end

local HEADMARK = nil
local ROOTFOEMARK = nil

function CHRONICAL_PAUSE()
	local TARGET = Mouse.Target
	if TARGET ~= nil then
		if TARGET.Parent:FindFirstChildOfClass("Humanoid") then
			local HUM = TARGET.Parent:FindFirstChildOfClass("Humanoid")
			local ROOT = TARGET.Parent:FindFirstChild("HumanoidRootPart") or TARGET.Parent:FindFirstChild("Torso") or TARGET.Parent:FindFirstChild("UpperTorso")
			local FOE = Mouse.Target.Parent
			HEADMARK = FOE:FindFirstChild("Head")
			ROOTFOEMARK = FOE:FindFirstChild("HumanoidRootPart")
			ATTACK = true
			Rooted = true
			local GYRO = IT("BodyGyro",RootPart)
			GYRO.D = 2
			GYRO.P = 2000
			GYRO.MaxTorque = VT(0,4000000,0)
			coroutine.resume(coroutine.create(function()
				repeat
					Swait()
					GYRO.CFrame = CF(RootPart.Position,ROOTFOEMARK.Position)
				until ATTACK == false
				GYRO:Remove()
			end))
			local spin1=MRANDOM(-360,360)
			local spin2=MRANDOM(-360,360)
			local spin3=MRANDOM(-360,360)
			local circle = CLOCKCIRCLE:Clone()
			circle.Parent = Effects
			local arrow1 = Instance.new("Part",Effects)
			arrow1.Transparency = 1
			arrow1.Size = Vector3.new(0.6,0.2,1.75)
			arrow1.Color = BrickColor.new("Lime green").Color
			arrow1.Material = "ForceField"
			arrow1.Anchored = true
			arrow1.CanCollide = false
			local arrow2 = Instance.new("Part",Effects)
			arrow2.Transparency = 1
			arrow2.Size = Vector3.new(0.6,0.2,2.25)
			arrow2.Color = BrickColor.new("Lime green").Color
			arrow2.Material = "ForceField"
			arrow2.Anchored = true
			arrow2.CanCollide = false
			local circle2 = CLOCKCIRCLE:Clone()
			circle2.Parent = Effects
			circle2.Size = Vector3.new(2,2,0.017)
			local arrow3 = Instance.new("Part",Effects)
			arrow3.Transparency = 1
			arrow3.Size = Vector3.new(0.2,0.07,0.584)
			arrow3.Color = BrickColor.new("Lime green").Color
			arrow3.Material = "ForceField"
			arrow3.Anchored = true
			arrow3.CanCollide = false
			local arrow4 = Instance.new("Part",Effects)
			arrow4.Transparency = 1
			arrow4.Size = Vector3.new(0.2,0.07,0.75)
			arrow4.Color = BrickColor.new("Lime green").Color
			arrow4.Material = "ForceField"
			arrow4.Anchored = true
			arrow4.CanCollide = false
			CreateSound(447682521, RightArm, 5, 1, false)
			CreateSound(447682521, ROOTFOEMARK, 5, 1, false)
			for i=0, 3, 0.1 / Animation_Speed do
				spin1 = spin1 + 2.1
				spin2 = spin2 + 4.5
				spin3 = spin3 + 9
				circle.Transparency = circle.Transparency - 0.06
				arrow1.Transparency = arrow1.Transparency - 0.03
				arrow2.Transparency = arrow2.Transparency - 0.03
				circle2.Transparency = circle2.Transparency - 0.06
				arrow3.Transparency = arrow3.Transparency - 0.03
				arrow4.Transparency = arrow4.Transparency - 0.03
				circle.CFrame = ROOTFOEMARK.CFrame*CF(0,-3,0)*ANGLES(RAD(90),RAD(0),RAD(spin1))
				arrow1.CFrame = ROOTFOEMARK.CFrame*ANGLES(RAD(0),RAD(spin2),RAD(0))*CF(0,-3,-0.8)
				arrow2.CFrame = ROOTFOEMARK.CFrame*ANGLES(RAD(0),RAD(spin3),RAD(0))*CF(0,-3,-1.125)
				circle2.CFrame = RightArm.CFrame*CF(0,-1,0)*ANGLES(RAD(90),RAD(0),RAD(spin1))
				arrow3.CFrame = RightArm.CFrame*ANGLES(RAD(0),RAD(spin2),RAD(0))*CF(0,-1,-0.27)
				arrow4.CFrame = RightArm.CFrame*ANGLES(RAD(0),RAD(spin3),RAD(0))*CF(0,-1,-0.375)
				Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(10 - 2.5 * SIN(SINE / 15)), RAD(6), RAD(-30)), 0.8 / Animation_Speed)
				RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(90), RAD(5), RAD(30)) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
				RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(30)), 0.8 / Animation_Speed)
				LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(-12)) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
				RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(0), RAD(90), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 0.8 / Animation_Speed)
				LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(0), RAD(-90), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 0.8 / Animation_Speed)
				Swait()
			end
			coroutine.resume(coroutine.create(function()
				repeat
					spin1 = spin1 + 2.1
					spin2 = spin2 + 4.5
					spin3 = spin3 + 9
					circle2.Color = MAINRUINCOLOR.Color
					arrow3.Color = MAINRUINCOLOR.Color
					arrow4.Color = MAINRUINCOLOR.Color
					circle2.CFrame = Torso.CFrame*CF(1,-0.75,0)*ANGLES(RAD(0),RAD(-90),RAD(spin1))
					arrow3.CFrame = Torso.CFrame*CF(1,-0.75,0)*ANGLES(RAD(spin2),RAD(0),RAD(-90))*CF(0,0,-0.27)
					arrow4.CFrame = Torso.CFrame*CF(1,-0.75,0)*ANGLES(RAD(spin3),RAD(0),RAD(-90))*CF(0,0,-0.375)
					swait()
				until ROOTFOEMARK==nil
				CreateSound(792162431,ROOT,1,0.9,false)
				WACKYEFFECT({Time = 30, EffectType = "Sphere", Size = VT(3,3,3), Size2 =VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = circle2.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.new("Lime green").Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})

				wait(0.1)
				circle2:Destroy()
				arrow3:Destroy()
				arrow4:Destroy()
			end))
			WACKYEFFECT({Time = 30, EffectType = "Sphere", Size = VT(7.5,7.5,7.5), Size2 =VT(12.5,12.5,12.5), Transparency = 0, Transparency2 = 1, CFrame = ROOTFOEMARK.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.new("Lime green").Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			for _, c in pairs(FOE:GetChildren()) do
				if c.ClassName == "Part" then
					c.Anchored = true
				end
			end	
			ATTACK = false
			Rooted = false
			for i = 0, 80 do
				WACKYEFFECT({Time = 25, EffectType = "Sphere", Size = VT(1.25,0,1.25), Size2 =VT(0,7.5,0), Transparency = 0, Transparency2 = 1, CFrame = ROOTFOEMARK.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = BrickColor.new("Lime green").Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
				swait(5)
			end
			CreateSound(792162431,ROOT,3,0.9,false)
			WACKYEFFECT({Time = 30, EffectType = "Sphere", Size = VT(9,9,9), Size2 =VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = ROOTFOEMARK.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.new("Lime green").Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			circle:Destroy()
			arrow1:Destroy()
			arrow2:Destroy()
			HEADMARK = nil
			ROOTFOEMARK = nil
			for _, c in pairs(FOE:GetChildren()) do
				if c.ClassName == "Part" then
					c.Anchored = false
				end
			end		
		end
	end
end

function RESUME_TRAVELS()
	if ROOTFOEMARK~=nil then
		ATTACK=true
		Rooted=true
		local TARGETED = ROOTFOEMARK.CFrame*CF(0,0,3)
		for i=0, 0.8, 0.1 / Animation_Speed do
			Swait()
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(10 - 2.5 * SIN(SINE / 15)), RAD(0), RAD(0)), 0.6 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1, 0.5, -0.5) * ANGLES(RAD(90), RAD(5), RAD(-80)) * RIGHTSHOULDERC0, 0.6 / Animation_Speed)
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.6 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1, 0.5, -0.5) * ANGLES(RAD(80), RAD(0), RAD(90)) * LEFTSHOULDERC0, 0.6 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(0), RAD(90), RAD(0)) * ANGLES(RAD(-1), RAD(0), RAD(0)), 0.6 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(0), RAD(-90), RAD(0)) * ANGLES(RAD(-1), RAD(0), RAD(0)), 0.6 / Animation_Speed)
		end
		for i=0, 0.75, 0.1 / Animation_Speed do
			Swait()
			WACKYEFFECT({Time = 130, EffectType = "Round Slash", Size = VT(0,0,0), Size2 =VT(0.5,0,0.5), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.new("Lime green").Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})

			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(30 - 2.5 * SIN(SINE / 15)), RAD(0), RAD(0)), 0.6 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(20), RAD(5), RAD(70)) * RIGHTSHOULDERC0, 0.6 / Animation_Speed)
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0.4, -0.2) * ANGLES(RAD(-40), RAD(0), RAD(0)), 0.6 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(20), RAD(0), RAD(-70)) * LEFTSHOULDERC0, 0.6 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(-40), RAD(90), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 0.6 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(-40), RAD(-90), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 0.6 / Animation_Speed)
		end
		WACKYEFFECT({Time = 30, EffectType = "Sphere", Size = VT(10,10,10), Size2 =VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.new("Lime green").Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		RootPart.CFrame = TARGETED
		ATTACK=false
		Rooted=false
	end
end

function BANSHEE_DESTRUCTS()
	ATTACK=true
	Rooted=true
	coroutine.resume(coroutine.create(function()
		for i = 0,6 do
			WACKYEFFECT({Time = 200, EffectType = "Sphere", Size = VT(0,0,0), Size2 =VT(750,750,750), Transparency = 0.5, Transparency2 = 1, CFrame = RootPart.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			swait(10)
		end
	end))
	for i=0, 3, 0.1 / Animation_Speed do
		Swait()
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-15 - 2.5 * SIN(SINE / 15)), RAD(0), RAD(0)), 0.2 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(-20), RAD(5), RAD(40)) * RIGHTSHOULDERC0, 0.2 / Animation_Speed)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0.15, 2 - 0.25 * SIN(SINE / 32)) * ANGLES(RAD(-15), RAD(0), RAD(0)), 0.2 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(-20), RAD(0), RAD(-40)) * LEFTSHOULDERC0, 0.2 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(-25), RAD(90), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 0.2 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(-25), RAD(-90), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 0.2 / Animation_Speed)
	end
	for i=0, 2, 0.1 / Animation_Speed do
		Swait()
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(15 - 2.5 * SIN(SINE / 15)), RAD(0), RAD(0)), 0.2 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1, 0.5, -0.5) * ANGLES(RAD(20), RAD(5), RAD(-50)) * RIGHTSHOULDERC0, 0.2 / Animation_Speed)
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, -0.15, 2 - 0.25 * SIN(SINE / 32)) * ANGLES(RAD(15), RAD(0), RAD(0)), 0.2 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1, 0.5, -0.5) * ANGLES(RAD(20), RAD(0), RAD(50)) * LEFTSHOULDERC0, 0.2 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(5), RAD(90), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 0.2 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -0.5, -0.5) * ANGLES(RAD(5), RAD(-90), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 0.2 / Animation_Speed)
	end
	CameraEnshaking(7,25)
	CreateSound(315743331,Character,5,1,false)
	CreateSound(743521450,Character,3,1,false)
	CreateSound(142070127,Character,3.5,1,false)
	CreateSound(262562442,Character,4,0.8,false)
	for i = 0,15 do
		EffectsAoE(RootPart.Position,380,20,40,200,"Up",false)
		WACKYEFFECT({Time = MRANDOM(200/10,750/10), EffectType = "Block", Size = VT(0,0,0), Size2 =VT(600,600,600), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame, MoveToPos = nil, RotationX = 11.25, RotationY = 11.25, RotationZ = 11.25, Material = "Neon", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		WACKYEFFECT({Time = MRANDOM(150/10,650/10), EffectType = "Sphere", Size = VT(0,99999999,0), Size2 =VT(400,99999999,400), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		WACKYEFFECT({Time = MRANDOM(300/10,800/10), EffectType = "Ring", Size = VT(0,0,0.1), Size2 =VT(60,60,0.3), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,0,0)*ANGLES(RAD(90),RAD(0),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		for i = 0,2 do
			WACKYEFFECT({Time = MRANDOM(300/10,750/10), EffectType = "Sphere", Size = VT(2,0,2), Size2 =VT(0,100,0)*MRANDOM(10/10,50/10), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(MRANDOM(-300,300),MRANDOM(-300,300),MRANDOM(-300,300))*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			WACKYEFFECT({Time = MRANDOM(150/10,500/10), EffectType = "Slash", Size = VT(0,0,0), Size2 =VT(5,0,5)*MRANDOM(10/10,30/10), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-100/10,100/10), RotationZ = 0, Material = "Neon", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		end
	end
	swait(15)
	WACKYEFFECT({Time = 85, EffectType = "Sphere", Size = VT(0,99999999,0), Size2 =VT(400,99999999,400), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
	WACKYEFFECT({Time = 120, EffectType = "Block", Size = VT(0,0,0), Size2 =VT(600,600,600), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
	WACKYEFFECT({Time = 100, EffectType = "Ring", Size = VT(0,0,0.1), Size2 =VT(60,60,0.3), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(0,0,0)*ANGLES(RAD(90),RAD(0),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "ForceField", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
	ATTACK=false
	Rooted=false
end

function Unforgivable_Sins()
	ATTACK = true
	Rooted = true
	for i=0, 1.5, 0.1 / Animation_Speed do
		Swait()
		WACKYEFFECT({EffectType = "Block", Size = VT(1,1,1), Size2 = VT(1,1,1), Transparency = 0.5, Transparency2 = 1, CFrame = RightArm.CFrame*CF(0,-1.3,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		WACKYEFFECT({EffectType = "Block", Size = VT(1,1,1), Size2 = VT(1,1,1), Transparency = 0.5, Transparency2 = 1, CFrame = LeftArm.CFrame*CF(0,-1.3,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0.35, 3.5+i) * ANGLES(RAD(-10-i*7), RAD(0), RAD(0)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-25), RAD(0), RAD(0)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(135), RAD(0), RAD(12)) * RIGHTSHOULDERC0, 0.6 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(135), RAD(0), RAD(-12)) * LEFTSHOULDERC0, 0.6 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -0.5, -0.5) * ANGLES(RAD(0), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.6 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1.1, 0) * ANGLES(RAD(0), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.6 / Animation_Speed)
	end
	coroutine.resume(coroutine.create(function()
		wait(0.1)
		CreateSound(240429466,RootPart,20,1,false)
		CreateSound(240517975,RootPart,2,1.3,false)
		local RANCF1=MRANDOM(-75/10,75/10)
		local RANCF2=MRANDOM(-75/10,75/10)
		local RANCF3=MRANDOM(-75/10,75/10)
		local RANCF4=MRANDOM(-75/10,75/10)
		CameraEnshaking(9,10)
		for i=0, 10 do
			RANCF1=MRANDOM(-100/10,100/10)
			RANCF2=MRANDOM(-100/10,100/10)
			RANCF3=MRANDOM(-100/10,100/10)
			RANCF4=MRANDOM(-100/10,100/10)
			coroutine.resume(coroutine.create(function()
				local hitb = Instance.new("Part", char)
				hitb.Anchored = true
				hitb.CanCollide = false
				hitb.FormFactor = 3
				hitb.Name = "Ring"
				hitb.Size = Vector3.new(1, 1, 1)
				hitb.Transparency = 1
				hitb.CFrame = RootPart.CFrame*CF(RANCF1,-3,-2*i*12)
				ManualAoE(hitb.Position,25,15,45,30,false)
				CreateSound(240429466,hitb,0.6,1,false)
				WACKYEFFECT({EffectType = "Crystal", Size = VT(0,0,0), Size2 = VT(10,100,10), Transparency = 0.5, Transparency2 = 1, CFrame = hitb.CFrame*CF(0,-3.5,0)*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-5/10,5/10), RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
				WACKYEFFECT({EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(60,10,60), Transparency = 0.5, Transparency2 = 1, CFrame = hitb.CFrame*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-100/10,100/10), RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
				wait(3)
				hitb:Destroy()
			end))
			coroutine.resume(coroutine.create(function()
				local hitc = Instance.new("Part", char)
				hitc.Anchored = true
				hitc.CanCollide = false
				hitc.FormFactor = 3
				hitc.Name = "Ring"
				hitc.Size = Vector3.new(1, 1, 1)
				hitc.Transparency = 1
				hitc.CFrame = RootPart.CFrame*CF(RANCF2,-3,2*i*12)
				ManualAoE(hitc.Position,25,15,45,30,false)
				CreateSound(240429466,hitc,0.6,1,false)
				WACKYEFFECT({EffectType = "Crystal", Size = VT(0,0,0), Size2 = VT(10,100,10), Transparency = 0.5, Transparency2 = 1, CFrame = hitc.CFrame*CF(0,-3.5,0)*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-5/10,5/10), RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
				WACKYEFFECT({EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(60,10,60), Transparency = 0.5, Transparency2 = 1, CFrame = hitc.CFrame*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-100/10,100/10), RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
				wait(3)
				hitc:Destroy()
			end))
			coroutine.resume(coroutine.create(function()
				local hitd = Instance.new("Part", char)
				hitd.Anchored = true
				hitd.CanCollide = false
				hitd.FormFactor = 3
				hitd.Name = "Ring"
				hitd.Size = Vector3.new(1, 1, 1)
				hitd.Transparency = 1
				hitd.CFrame = RootPart.CFrame*CF(-2*i*12,-3,RANCF1)
				CreateSound(240429466,hitd,0.6,1,false)
				ManualAoE(hitd.Position,25,15,45,30,false)
				WACKYEFFECT({EffectType = "Crystal", Size = VT(0,0,0), Size2 = VT(10,100,10), Transparency = 0.5, Transparency2 = 1, CFrame = hitd.CFrame*CF(0,-3.5,0)*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-5/10,5/10), RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
				WACKYEFFECT({EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(60,10,60), Transparency = 0.5, Transparency2 = 1, CFrame = hitd.CFrame*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-100/10,100/10), RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
				wait(3)
				hitd:Destroy()
			end))
			coroutine.resume(coroutine.create(function()
				local hite = Instance.new("Part", char)
				hite.Anchored = true
				hite.CanCollide = false
				hite.FormFactor = 3
				hite.Name = "Ring"
				hite.Size = Vector3.new(1, 1, 1)
				hite.Transparency = 1
				hite.CFrame = RootPart.CFrame*CF(2*i*12,-3,RANCF1)
				CreateSound(240429466,hite,0.6,1,false)
				ManualAoE(hite.Position,25,15,45,30,false)
				WACKYEFFECT({EffectType = "Crystal", Size = VT(0,0,0), Size2 = VT(10,100,10), Transparency = 0.5, Transparency2 = 1, CFrame = hite.CFrame*CF(0,-3.5,0)*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-5/10,5/10), RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
				WACKYEFFECT({EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(60,10,60), Transparency = 0.5, Transparency2 = 1, CFrame = hite.CFrame*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-100/10,100/10), RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
				wait(3)
				hite:Destroy()
			end))
			swait()
		end
	end))
	for i=0, 1, 0.1 / Animation_Speed do
		Swait()
		WACKYEFFECT({EffectType = "Block", Size = VT(1,1,1), Size2 = VT(1,1,1), Transparency = 0.5, Transparency2 = 1, CFrame = RightArm.CFrame*CF(0,-1.3,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		WACKYEFFECT({EffectType = "Block", Size = VT(1,1,1), Size2 = VT(1,1,1), Transparency = 0.5, Transparency2 = 1, CFrame = LeftArm.CFrame*CF(0,-1.3,0), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = Color3.fromRGB(59,0,0), SoundID = nil, SoundPitch = 1, SoundVolume = 5})
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, -0.35, -0.1) * ANGLES(RAD(35), RAD(0), RAD(0)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(25), RAD(0), RAD(0)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(35), RAD(0), RAD(12)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(35), RAD(0), RAD(-12)) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1.1, 0) * ANGLES(RAD(35), RAD(90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1.1, 0) * ANGLES(RAD(35), RAD(-90), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
	end
	ATTACK = false
	Rooted = false
end

function GLITCH_BOX()
	ATTACK=true
	Rooted=true
	coroutine.resume(coroutine.create(function()
		for i=0, 5 do
			swait()
			coroutine.resume(coroutine.create(function()
				local hitb = Instance.new("Part", char)
				hitb.Anchored = true
				hitb.CanCollide = false
				hitb.FormFactor = 3
				hitb.Name = "Ring"
				hitb.Material = "Neon"
				hitb.Size = Vector3.new(1, 1, 1)
				hitb.Transparency = 1
				hitb.TopSurface = 0
				hitb.BottomSurface = 0
				hitb.CFrame = RootPart.CFrame*CF(MRANDOM(-9,9),MRANDOM(-9,9),MRANDOM(-9,9))
				EffectsAoE(hitb.Position,3,1,15,0,"Fling",false)
				WACKYEFFECT({Time = MRANDOM(80,100), EffectType = "Box", Size = VT(2.75,2.75,2.75), Size2 =VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = hitb.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = MRANDOM(-100/100,100/100), RotationY = MRANDOM(-100/100,100/100), RotationZ = MRANDOM(-100/100,100/100), Material = "Neon", Color = TEXTCOLOR, SoundID = 2101658119, SoundPitch = MRANDOM(12,15)/10, SoundVolume = 0.2})
				wait(5)
				hitb:Destroy()
			end))
		end
	end))
	ATTACK=false
	Rooted=false
end

function TELEBOX()
	ATTACK=true
	Rooted=true
	local CFR=Mouse.Hit
	coroutine.resume(coroutine.create(function()
		repeat
			coroutine.resume(coroutine.create(function()
				local hitb = Instance.new("Part", char)
				hitb.Anchored = true
				hitb.CanCollide = false
				hitb.FormFactor = 3
				hitb.Name = "Ring"
				hitb.Material = "Neon"
				hitb.Size = Vector3.new(1, 1, 1)
				hitb.Transparency = 1
				hitb.TopSurface = 0
				hitb.BottomSurface = 0
				hitb.CFrame = CFR*CF(MRANDOM(-9,9),MRANDOM(-9,9),MRANDOM(-9,9))
				hitb.Orientation = VT(0,0,0)
				EffectsAoE(hitb.Position,3,1,15,0,"Fling",false)
				WACKYEFFECT({Time = MRANDOM(80,100), EffectType = "Box", Size = VT(2.75,2.75,2.75), Size2 =VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = hitb.CFrame*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = MRANDOM(-100/100,100/100), RotationY = MRANDOM(-100/100,100/100), RotationZ = MRANDOM(-100/100,100/100), Material = "Neon", Color = TEXTCOLOR, SoundID = 2101658119, SoundPitch = MRANDOM(12,15)/10, SoundVolume = 0.2})
				wait(5)
				hitb:Destroy()
			end))
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-15 - 2.5 * SIN(SINE / 15) + MRANDOM(-4,4)), RAD(0 + MRANDOM(-4,4)), RAD(-10 + MRANDOM(-4,4))), 3 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(-15+ 5 * COS(SINE / 30) + MRANDOM(-4,4)), RAD(0 + MRANDOM(-4,4)), RAD(12 + MRANDOM(-4,4))) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0.15 + 0.06 * COS(SINE / 45), -0.05 + 0.1 * COS(SINE / 45)) * ANGLES(RAD(-15 - 6 * COS(SINE / 45)), RAD(0), RAD(0)), 0.8 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.4, 0.8, 0) * ANGLES(RAD(165 + MRANDOM(-4,4)), RAD(0 + MRANDOM(-4,4)), RAD(25 + MRANDOM(-4,4))) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1 - 0.1 * COS(SINE / 45), -0.01) * ANGLES(RAD(-15- 6 * COS(SINE / 45)), RAD(80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(-15)), 0.8 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1 - 0.1 * COS(SINE / 45), -0.01) * ANGLES(RAD(-15- 6 * COS(SINE / 45)), RAD(-80), RAD(0)) * ANGLES(RAD(-2), RAD(0), RAD(0)), 0.8 / Animation_Speed)
			swait()
		until ATTACK==false
	end))
	for i=0, 30 do
		WACKYEFFECT({Time = MRANDOM(80,100), EffectType = "Box", Size = VT(3.5,3.5,3.5), Size2 = VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*CF(MRANDOM(-15,15),MRANDOM(-15,15),MRANDOM(-15,15))*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = RootPart.CFrame*CF(0,0,0)*VT(0,0,0), RotationX = MRANDOM(-100/100,100/100), RotationY = MRANDOM(-100/100,100/100), RotationZ = MRANDOM(-100/100,100/100), Material = "Neon", Color = TEXTCOLOR, SoundID = 2101658119, SoundPitch = MRANDOM(12,15)/10, SoundVolume = 0.125})
		swait()
	end
	swait(60)
	WACKYEFFECT({Time = 25, EffectType = "Box", Size = VT(25,25,25), Size2 = VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-100/100,100/100), RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = 588738712, SoundPitch = 1.2, SoundVolume = 1})
	swait(1)
	CameraEnshaking(7,10)
	RootPart.CFrame = CFR*CF(0,2.5,0)
	WACKYEFFECT({Time = 25, EffectType = "Box", Size = VT(25,25,25), Size2 = VT(0,0,0), Transparency = 0, Transparency2 = 1, CFrame = RootPart.CFrame*ANGLES(RAD(0),RAD(MRANDOM(-360,360)),RAD(0)), MoveToPos = nil, RotationX = 0, RotationY = MRANDOM(-100/100,100/100), RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = 588738712, SoundPitch = 1.2, SoundVolume = 1})
	ATTACK=false
	Rooted=false
end

function GIANT_BLOCKY()
	local HITFLOOR,HITPOS = Raycast(RootPart.Position, (CF(RootPart.Position, RootPart.Position + VT(0, -1, 0))).lookVector, 12, Character)
	if HITFLOOR then
		ATTACK = true
		Rooted = true
		CreateSound(255679384, RootPart, 10, MRANDOM(13,15)/10, false)
		for i=0, 1, 0.1 / Animation_Speed do
			Swait()
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(0), RAD(0), RAD(0)), 1 / Animation_Speed)
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(0), RAD(0), RAD(0)), 3 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.25, 0.5, -0.25) * ANGLES(RAD(75), RAD(-4), RAD(-34)) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.25, 0.5, -0.25) * ANGLES(RAD(75), RAD(4), RAD(34)) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(0), RAD(75), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 1 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(0), RAD(-75), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
		CreateSound(1019848561, Character, 2, 1.1, false)
		coroutine.resume(coroutine.create(function()
			local CFRAME = RootPart.CFrame*CF(0,-3,0)
			local SPIKE = Getblocky(HITFLOOR)
			SPIKE.CanCollide = true
			SPIKE.Size = VT(100,100,100)
			SPIKE.CFrame = RootPart.CFrame*CF(0,-103,0)
			CreateSound(203691785, RootPart, 3, 1.1, false)
			coroutine.resume(coroutine.create(function()
				local POS = SPIKE.CFrame*CF(0,120,0)
				wait(0.25)
				CreateSound(165970126, Torso, 0.75, 0.8, false)
				local posit=0
				Humanoid.HipHeight = 5
				local HIT = function()
					local HITS = {}
					local TOUCH = SPIKE.Touched:Connect(function(hit)
						if hit.Parent:FindFirstChildOfClass("Humanoid") and hit.Parent ~= Character then
							local HUM = hit.Parent:FindFirstChildOfClass("Humanoid")
							local TORSO = hit.Parent:FindFirstChild("Torso") or hit.Parent:FindFirstChild("UpperTorso")
							if TORSO and HUM.Health > 0 then
								local PASS = true
								for i = 1, #HITS do
									if HITS[i] == hit.Parent then
										PASS = false
									end
								end
								table.insert(HITS,hit.Parent)
								if PASS == true then
									ManualDamage(HUM,MRANDOM(50,90),HUM.Torso)
								end
							end
						end
					end)
					return TOUCH
				end
				local TOUCHED = HIT()
				for i = 1, 300 do
					Swait()
					CameraEnshaking(0.2,5)
					if MRANDOM(1,4)==1 then
						WaveParticles(SPIKE.Position,300,C3(1,1,1))
					end
					SPIKE.CFrame = Clerp(SPIKE.CFrame, POS, 0.025 / Animation_Speed)
				end
				Humanoid.HipHeight = 0
				TOUCHED:Disconnect()
				wait(5)
				for i = 1, 250 do
					Swait()
					SPIKE.Transparency = SPIKE.Transparency + 1/125
					SPIKE.CFrame = SPIKE.CFrame * CF(0,-1,0)
				end
				SPIKE:remove()
			end))
		end))
		for i=0, 1, 0.1 / Animation_Speed do
			Swait()
			WACKYEFFECT({EffectType = "Box", Size = VT(1,0,1), Size2 = VT(0,3,0)*MRANDOM(1,2), Transparency = 0, Transparency2 = 1, CFrame = RightArm.CFrame*CF(0,-1,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			WACKYEFFECT({EffectType = "Box", Size = VT(1,0,1), Size2 = VT(0,3,0)*MRANDOM(1,2), Transparency = 0, Transparency2 = 1, CFrame = LeftArm.CFrame*CF(0,-1,0)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = TEXTCOLOR, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0.1, 0) * ANGLES(RAD(-10), RAD(0), RAD(0)), 1 / Animation_Speed)
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-20), RAD(0), RAD(0)), 3 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(150), RAD(-4), RAD(40)) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(150), RAD(4), RAD(-40)) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, 0) * ANGLES(RAD(-10), RAD(80), RAD(0)) * ANGLES(RAD(-4), RAD(0), RAD(0)), 1 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, 0) * ANGLES(RAD(-10), RAD(-80), RAD(0)) * ANGLES(RAD(-4), RAD(0), RAD(0)), 1 / Animation_Speed)
		end
		wait(8)
		ATTACK = false
		Rooted = false
	end
end

function INSANITY_STRIKES()
	ATTACK=true
	Rooted=true
	local CFR=Mouse.Hit
	coroutine.resume(coroutine.create(function()
		repeat
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-12), RAD(0), RAD(0)), 3 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.85, 0) * ANGLES(RAD(155), RAD(0), RAD(12)) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(-6), RAD(7), RAD(0)), 0.8 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(-12)) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1.25, -0.01) * ANGLES(RAD(-6), RAD(80), RAD(0)) * ANGLES(RAD(8), RAD(0), RAD(-5)), 0.8 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(-6), RAD(-80), RAD(0)) * ANGLES(RAD(-10), RAD(0), RAD(0)), 0.8 / Animation_Speed)
			swait()
		until ATTACK==false
	end))
	coroutine.resume(coroutine.create(function()
		local hitb = Instance.new("Part", char)
		hitb.Anchored = true
		hitb.CanCollide = false
		hitb.FormFactor = 3
		hitb.Name = "Ring"
		hitb.Material = "Neon"
		hitb.Size = Vector3.new(1, 1, 1)
		hitb.Transparency = 1
		hitb.TopSurface = 0
		hitb.BottomSurface = 0
		hitb.CFrame = CFR
		hitb.Orientation = VT(0,0,0)
		hitb.Anchored = true
		WACKYEFFECT({Time = 5, EffectType = "Sphere", Size = VT(0,0,0), Size2 =VT(275,5,275), Transparency = 0, Transparency2 = 1, CFrame = hitb.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = MRANDOM(12,15)/10, SoundVolume = 0.2})
		WACKYEFFECT({Time = 25, EffectType = "Sphere", Size = VT(0,0,0), Size2 =VT(275,5,275), Transparency = 0, Transparency2 = 1, CFrame = hitb.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = MRANDOM(12,15)/10, SoundVolume = 0.2})
		WACKYEFFECT({Time = 60, EffectType = "Sphere", Size = VT(0,0,0), Size2 =VT(275,5,275), Transparency = 0, Transparency2 = 1, CFrame = hitb.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = MRANDOM(12,15)/10, SoundVolume = 0.2})
		WACKYEFFECT({Time = 500, EffectType = "Sphere", Size = VT(200,0,200), Size2 =VT(250,1,250), Transparency = 0.7, Transparency2 = 1, CFrame = hitb.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = MRANDOM(12,15)/10, SoundVolume = 0.2})
		wait(0.25)
		coroutine.resume(coroutine.create(function()
			for i=0,180 do
				WACKYEFFECT({Time = MRANDOM(300/10,750/10), EffectType = "Sphere", Size = VT(2,0,2), Size2 =VT(0,100,0)*MRANDOM(10/10,50/10), Transparency = 0, Transparency2 = 1, CFrame = hitb.CFrame*CF(MRANDOM(-70,70),MRANDOM(0,5),MRANDOM(-70,70))*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = nil, SoundVolume = nil})
				swait(2)
			end
		end))
		for i=0,100 do
			coroutine.resume(coroutine.create(function()
				local hita = Instance.new("Part", char)
				hita.Anchored = true
				hita.CanCollide = false
				hita.FormFactor = 3
				hita.Name = "Ring"
				hita.Material = "Neon"
				hita.Size = Vector3.new(1, 1, 1)
				hita.Transparency = 1
				hita.TopSurface = 0
				hita.BottomSurface = 0
				hita.CFrame = hitb.CFrame*CF(MRANDOM(-60,60),1.5,MRANDOM(-60,60))
				hita.Orientation = VT(0,0,0)
				ManualAoE(hita.Position,45,25,50,50,false)
				CreateSound(444667844,hita,1,MRANDOM(105/100,130/100),false)
				WACKYEFFECT({Time = 25, EffectType = "Sphere", Size = VT(0,1,0), Size2 =VT(75,15,75), Transparency = 0, Transparency2 = 1, CFrame = hita.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = 1, SoundVolume = 0})
				WACKYEFFECT({Time = 25, EffectType = "Sphere", Size = VT(30,0,30), Size2 =VT(0,100,0), Transparency = 0, Transparency2 = 1, CFrame = hita.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = BrickColor.Random().Color, SoundID = nil, SoundPitch = 1, SoundVolume = 0})
				wait(5)
				hita:Destroy()
			end))
			swait(MRANDOM(100/100,750/100))
		end
		wait(5)
		hitb:Destroy()
	end))
	wait(0.5)
	ATTACK=false
	Rooted=false
end


function RadiatingPuddles()
	ATTACK = true
	Rooted = true
	local BOMB = CreatePart(3, Effects, "Neon", 0, 1, "Crimson", "Bomb", VT(0,0,0))
	BOMB.CFrame = RightArm.CFrame
	MakeForm(BOMB,"Ball")
	for i = 1, 10 do
		Swait()
		turnto(Mouse.Hit.p)
		BOMB.CFrame = RightArm.CFrame
		BOMB.Transparency = BOMB.Transparency - 0.1
		BOMB.Size = BOMB.Size + VT(4/10,4/10,4/10)
	end
	CreateSound("588693156", BOMB, 7, 1)
	coroutine.resume(coroutine.create(function()
		LobbingObject(BOMB,Mouse.Hit.p,30,50,true)
		local LOOP = 0
		local TRAILCOLORS = {"Dark red","Crimson","Red"}
		for i = 1, 30 do
			Swait()
			LOOP = LOOP + 1
			if LOOP == 8 then
				LOOP = 0
				MagicSphere(BOMB.Size/1.4,15,BOMB.CFrame,"Really black",VT(0.3,0.3,0.3))
			end
			MagicSphere(VT(1.5,1.5,1.5),35,BOMB.CFrame*CF(MRANDOM(-3,3)/3,MRANDOM(-3,3)/3,MRANDOM(-3,3)/3),TRAILCOLORS[MRANDOM(1,#TRAILCOLORS)],VT(-(1.5/35),-(1.5/35),-(1.5/35)))
		end
		ManualAoE(BOMB.Position,30,55,85,12,false)
		BOMB.Transparency = 1
		Debris:AddItem(BOMB,10)
		MagicSphere(BOMB.Size/1.4,25,BOMB.CFrame,"Really black",VT(2,2,2))
		MagicSphere(BOMB.Size/1.4,65,BOMB.CFrame,"Really black",VT(0.3,0.3,0.3))
		MagicSphere(BOMB.Size/1.7,65,BOMB.CFrame,"Crimson",VT(0.3,0.3,0.3))
		MagicSphere(BOMB.Size,65,BOMB.CFrame,"Crimson",VT(0.3,0.3,0.3))
		CreateSound("231917750", BOMB, 10, 1)
		CreateSound("289842971", BOMB, 5, 1.3)
		local HITFLOOR = Raycast(BOMB.Position, (CF(BOMB.Position, BOMB.Position + VT(0, -1, 0))).lookVector, 2, Character)
		CreateDebreeRing(HITFLOOR,BOMB.Position+VT(0,-2,0),2,VT(3,3,3),75)
		for i = 1, 35 do
			MagicSphere(VT(0.2,0.2,0.2),65,CF(BOMB.CFrame*CF(MRANDOM(-5,5),MRANDOM(-5,5),MRANDOM(-5,5)).p,BOMB.Position),"Crimson",VT(0.001,0.001,1),0)
		end
	end))
	ATTACK = false
	Rooted = false
end

function CHRONIC_DIMENSION()
	ATTACK=true
	Rooted=true
	local CFR=Mouse.Hit
	coroutine.resume(coroutine.create(function()
		repeat
			Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-12), RAD(0), RAD(0)), 3 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.85, 0) * ANGLES(RAD(155), RAD(0), RAD(12)) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
			RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(-6), RAD(7), RAD(0)), 0.8 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(0), RAD(0), RAD(-12)) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(1, -1.25, -0.01) * ANGLES(RAD(-6), RAD(80), RAD(0)) * ANGLES(RAD(8), RAD(0), RAD(-5)), 0.8 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(-6), RAD(-80), RAD(0)) * ANGLES(RAD(-10), RAD(0), RAD(0)), 0.8 / Animation_Speed)
			swait()
		until ATTACK==false
	end))
	wait(0.25)
	for i,v in pairs(game:GetService("Players"):GetChildren()) do
		local var = script.LocalScripts.Visioning:Clone()
		var.Parent = v.PlayerGui
		var.Disabled = false
	end	
	wait(5)
	for i,v in pairs(game:GetService("Players"):GetChildren()) do
		local var = script.LocalScripts.RemoveVision:Clone()
		var.Parent = v.PlayerGui
		var.Disabled = false
	end	
	ATTACK=false
	Rooted=false
end

function Nightys()
	ATTACK = true
	Rooted = false
	local Col = script["90s"]:Clone()
	Col.Parent = game.Lighting
	local z=0
	local BWR=MRANDOM(0/1000,1000/1000)
	--local Part = CreatePart(3,Character,"Neon",1,1,"Really red","a",VT(90, 90, 90),false)
	CreateSound(233856115,Character,3,1,false)
	CreateSound(233856115,RootPart,10,1,false)
	for i=0, 2, 0.1 / Animation_Speed do
		Swait()
		z=z+1
		if z < 1 then
			Col.Enabled = true
		elseif z > 1 then
			Col.Enabled = false
		end
		if z>5 then
			z=0
		end
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, -0.3, 0) * ANGLES(RAD(10), RAD(0), RAD(0)), 0.25 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(25), RAD(0), RAD(0)), 0.25 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5, 0) * ANGLES(RAD(15), RAD(0), RAD(15)) * RIGHTSHOULDERC0, 0.25 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5, 0) * ANGLES(RAD(15), RAD(0), RAD(-15)) * LEFTSHOULDERC0, 0.25 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(10), RAD(80), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.25 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(10), RAD(-80), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.25 / Animation_Speed)
	end
	for i=0, 0.6, 0.1 / Animation_Speed do
		Swait()
		z=z+1
		if z < 1 then
			Col.Enabled = true
		elseif z > 1 then
			Col.Enabled = false
		end
		if z>5 then
			z=0
		end		
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, -0.3, 0) * ANGLES(RAD(15), RAD(0), RAD(0)), 0.5 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(5), RAD(0), RAD(0)), 0.5 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.4, -0.6) * ANGLES(RAD(55), RAD(0), RAD(-90)) * RIGHTSHOULDERC0, 0.8 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.4, -0.6) * ANGLES(RAD(55), RAD(0), RAD(90)) * LEFTSHOULDERC0, 0.8 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(15), RAD(80), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.8 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(15), RAD(-80), RAD(0)) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.8 / Animation_Speed)
	end
	CreateSound(233856079,Character,4,1,false)
	CreateSound(233856079,RootPart,15,1,false)
	Col.Enabled = true
	for i=0,10 do
		swait()
		BWR=MRANDOM(0/1000,1000/1000)
		WACKYEFFECT({Time = 90,EffectType = "Block", Size = VT(0,0,0), Size2 = VT(1000,1000,1000), Transparency = 0.6, Transparency2 = 0.5, CFrame = RootPart.CFrame*CF(0,0,-0.5)*ANGLES(RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360)),RAD(MRANDOM(-360,360))), MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(BWR,BWR,BWR), SoundID = 260435136, SoundPitch = 0.9, SoundVolume = 10, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 50})
		--WACKYEFFECT({Time = 60, EffectType = "Sphere", Size = VT(0,0,0), Size2 = VT(1000,1000,1000), Transparency = 0.6, Transparency2 = 0.2, CFrame = RootPart.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(BWR,BWR,BWR), SoundID = 0, SoundPitch = MRANDOM(8,15)/10, SoundVolume = 4, UseBoomerangMath = true, Boomerang = 0, SizeBoomerang = 950})
	end
	ManualAoE(Torso.Position,500,40,80,0,false)
	for i=0, 0.8, 0.1 / Animation_Speed do
		Swait()
		local BWR = MRANDOM(0,1)
		--Part.CFrame = RootPart.CFrame
		--WACKYEFFECT({EffectType = "Block", Size = VT(10,10,10), Size2 = VT(90,90,90), Transparency = 0.5, Transparency2 = 1, CFrame = Torso.CFrame, MoveToPos = nil, RotationX = 0, RotationY = 0, RotationZ = 0, Material = "Neon", Color = C3(BWR,BWR,BWR), SoundID = nil, SoundPitch = nil, SoundVolume = nil})
		RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0, 0.3, 0) * ANGLES(RAD(-15), RAD(0), RAD(0)), 1 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((1) - 1)) * ANGLES(RAD(-20), RAD(0), RAD(0)), 1 / Animation_Speed)
		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.6, 0.6, 0.2) * ANGLES(RAD(-25), RAD(0), RAD(55)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
		LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.6, 0.6, 0.2) * ANGLES(RAD(-25), RAD(0), RAD(-55)) * LEFTSHOULDERC0, 1 / Animation_Speed)
		RightHip.C0 = Clerp(RightHip.C0, CF(1, -1, -0.01) * ANGLES(RAD(-15), RAD(80), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 1 / Animation_Speed)
		LeftHip.C0 = Clerp(LeftHip.C0, CF(-1, -1, -0.01) * ANGLES(RAD(-15), RAD(-80), RAD(0)) * ANGLES(RAD(-3), RAD(0), RAD(0)), 1 / Animation_Speed)
	end
	ATTACK = false
	Rooted = false
	wait(2)
	--Part:Destroy()
	Col:Destroy()
end

--//=================================\\
--||	  ASSIGN THINGS TO KEYS
--\\=================================//

function MouseDown(Mouse)
	if ATTACK==false then
		if MODE == "Purged" then
			PurgeCombo()
		elseif  MODE == "Fairy" or MODE == "Black" or MODE=="Ace" or MODE=="Origins" or MODE=="Vanta" or MODE=="Crystal" then
			FlyingCombo()
		elseif MODE == "Sanity" or MODE == "Glitch" or MODE == "Singularity" then
			SpamableCombo()
		elseif MODE == "Pilgrim" then
			AT1()
		else
			BasicCombo()
		end
	end
end

function MouseUp(Mouse)
end

function KeyDown(Key)
	if Key ~= "w" and Key ~= "a" and Key ~= "s" and Key ~= "d" then
		KEYHOLD = true
		-- mode thing --
		if Key == "n" and ATTACK == false and MODE == "Lunar" then
			SolarForm()

		elseif Key == "n" and ATTACK == false and MODE == "Solar" then
			ResetMode()
		end
		if Key == "m" and ATTACK == false and MODE == "Lunar" then
			OriginsForm()

		elseif Key == "m" and ATTACK == false and MODE == "Origins" then
			ResetMode()
		end
		if Key == "q" and ATTACK == false and MODE ~= "Time" then
			ChronoForm()

		elseif Key == "m" and ATTACK == false and MODE == "Time" then
			OverForm()

		elseif Key == "n" and ATTACK == false and MODE == "Time" then
			RadForm()

		elseif Key == "q" and ATTACK == false and MODE == "Time" then
			ResetMode()
		end
		if Key == "f" and ATTACK == false and MODE ~= "Superior" then
			SuperiorForm()

		elseif Key == "f" and ATTACK == false and MODE == "Superior" then
			ResetMode()

		elseif Key == "m" and ATTACK == false and MODE == "Superior" then
			YandereForm()

		elseif Key == "n" and ATTACK == false and MODE == "Superior" then
			SingularityForm()
		end
		if Key == "h" and ATTACK == false and MODE ~= "Pilgrim" then
			PilgrimForm()

		elseif Key == "h" and ATTACK == false and MODE == "Pilgrim" then
			ResetMode()
		end
		if Key == "e" and ATTACK == false and MODE ~= "Hexa" then
			TerraForm()

		elseif Key == "n" and ATTACK == false and MODE == "Hexa" then
			AceForm()

		elseif Key == "e" and ATTACK == false and MODE == "Hexa" then
			ResetMode()
		end
		if Key == "p" and ATTACK == false and MODE ~= "Random" then
			RandomForm()
			RCHOICE = MRANDOM(1,6)

		elseif Key == "n" and ATTACK == false and MODE == "Random" and MODE ~= "Visual" then
			VisualForm()
			RCHOICE = MRANDOM(1,6)

		elseif Key == "m" and ATTACK == false and MODE == "Random" then
			RCHOICE = MRANDOM(1,6)


		elseif Key == "p" and ATTACK == false and MODE == "Random" then
			ResetMode()
		end
		if Key == "m" and ATTACK == false and MODE == "Hexa" then
			VantaForm()
		end
	elseif Key == "m" and ATTACK == false and MODE == "Vanta" then
		TerraForm()
	end
	if Key == "g" and ATTACK == false and MODE ~= "Glitch" then
		GlitchForm()

	elseif Key == "g" and ATTACK == false and MODE == "Glitch" then
		ResetMode()
	end
	if Key == "j" and ATTACK == false and MODE ~= "Peculiar" then
		PeculiarForm()

	elseif Key == "m" and ATTACK == false and MODE == "Peculiar" then
		TrashForm()

	elseif Key == "n" and ATTACK == false and MODE == "Peculiar" then
		BerForm()

	elseif Key == "j" and ATTACK == false and MODE == "Peculiar" then
		ResetMode()
	end
	if Key == "r" and ATTACK == false and MODE ~= "Sense" then
		SenseForm()

	elseif Key == "r" and ATTACK == false and MODE == "Sense" then
		ResetMode()
	end
	if Key == "n" and ATTACK == false and MODE == "Superior" and MODE~="Heart" then
		--HeartForm()

	elseif Key == "n" and ATTACK == false and MODE == "Heart" then
		SuperiorForm()
	end
	if Key == "n" and ATTACK == false and MODE == "Sense" then
		CrystalForm()

	elseif Key == "m" and ATTACK == false and MODE == "Crystal" then
		SenseForm()
	end
	if Key == "m" and ATTACK == false and MODE == "Sense" then
		SanityForm()

	elseif Key == "m" and ATTACK == false and MODE == "Sanity" then
		SenseForm()
	end
