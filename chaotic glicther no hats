		----rwing1leftweld.C1 = Clerp(----rwing1leftweld.C1, CF(1.1-0.15*SIN(SINE / 46.75), 0, 0), 0.4 / Animation_Speed)
		handleweld.C1 = Clerp(handleweld.C1, CF(0, -1, -1.5) * ANGLES(RAD(0), RAD(0), RAD(0)), 0.4 / Animation_Speed)
	elseif MODE == "Devil" or MODE == "90s" or MODE == "Lunar" or MODE == "Hexa" then
		rwing1weld.C1 = Clerp(rwing1weld.C1, CF(-2.4, -1, 0+0.75*SIN(SINE / 32)) * ANGLES(RAD(0), RAD(0 + 12 * SIN(SINE / 32)), RAD(0)), 0.4 / Animation_Speed)
