	for i = 0, 8, 0.1 do
		Swait()
               swordweld.C0 = Clerp(swordweld.C0, CF(-0.2*SIZE-0 * COS(SINE / 15), -5*SIZE-0 * COS(SINE / 8), 0.0*SIZE+0 * COS(SINE / 20)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-0+0 * COS(SINE / 20)), RAD(45-0 * COS(SINE / 11)-(sick.PlaybackLoudness * 0))), 1 / Animation_Speed)
