-- Scripts:

local function SXABF_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local phrase = CHAT
	local reference = NAMEMODE
	
	while task.wait() do

		script.Parent.Trapazoid1.ImageColor3 = reference.TextColor3
		script.Parent.Trapazoid2.ImageColor3 = reference.TextColor3
		script.Parent.Gradiant.ImageColor3 = reference.TextStrokeColor3
		
		script.Parent.GLICH1.TextColor3 = reference.TextStrokeColor3
		script.Parent.GLICH2.TextColor3 = reference.TextStrokeColor3
	
		script.Parent.PHRASE.TextColor3 = reference.TextStrokeColor3
		script.Parent.PHRASE.Text = phrase.Text
		
		script.Parent.NAAME.TextColor3 = reference.TextStrokeColor3
		script.Parent.NAAME.Text = reference.Text
		
		script.Parent.PHRASE1.Rotation = phrase.Rotation
		script.Parent.PHRASE1.TextColor3 = reference.TextColor3
		script.Parent.PHRASE1.Text = phrase.Text
		
		script.Parent.PHRASE.Rotation = phrase.Rotation
		script.Parent.NAAME.Rotation = phrase.Rotation

	end
	
	
end
coroutine.wrap(SXABF_fake_script)()

  
    

