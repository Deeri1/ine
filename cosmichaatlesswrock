function transition()
	ATTACK = true
	Rooted = false
swordweld.Part0 = GUN
sword2weld.Part0 = GUN
LeftShoulder.Part0 = Torso
RightShoulder.Part0 = Torso
Auraweld.Part0 = Torso
sword3weld.Part0 = GUN
sword4weld.Part0 = GUN
	for i=0, .01, 0.1 / Animation_Speed do
		Swait()
		Neck.C0 = Clerp(Neck.C0, CF(0*SIZE+0 * COS(SINE / 1), 1.0*SIZE+0 * COS(SINE / 1), -.0*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(90.3+3*math.cos(SINE/30)),math.rad(180+3*math.cos(SINE/28)),math.rad(-10.1+6*math.cos(SINE/25))), 1 / Animation_Speed)

            RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0*SIZE- 0 * COS(SINE / 1), 0*SIZE+0 * COS(SINE / 1), 0*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(-18.8+0 * COS(SINE / 1)), RAD(-1.45+0 * COS(SINE / 1)), RAD(-15.9+0 * COS(SINE / 1))), 1 / Animation_Speed)
			RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1*SIZE-0 * COS(SINE / 1), 0*SIZE+0 * COS(SINE / 1), -0.8*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(166.27+0 * COS(SINE / 1)), RAD(-4.34+0 * COS(SINE / 1)), RAD(-33.25+0 * COS(SINE / 1))) * RIGHTSHOULDERC0, 1 / Animation_Speed)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1*SIZE-0 * COS(SINE / 1), 0*SIZE+0 * COS(SINE / 1), -0.8*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(168+0 * COS(SINE / 1)), RAD(4+0 * COS(SINE / 1)), RAD(30+0 * COS(SINE / 1))) * LEFTSHOULDERC0, 1 / Animation_Speed)
			RightHip.C0 = Clerp(RightHip.C0, CF(-0.5*SIZE+0 * COS(SINE / 1), 0*SIZE+0 * COS(SINE / 1), -1*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(339.76+0 * COS(SINE / 1)), RAD(21.69+0 * COS(SINE / 1)), RAD(354.22+0 * COS(SINE / 1))), 1 / Animation_Speed)
			LeftHip.C0 = Clerp(LeftHip.C0, CF(0.5*SIZE+0 * COS(SINE / 1), -1*SIZE+0 * COS(SINE / 1), 0.3*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(331.08+0 * COS(SINE / 1)), RAD(333.98+0 * COS(SINE / 1)), RAD(0+0 * COS(SINE / 1))), 1 / Animation_Speed)
	end
	ATTACK = false
	Rooted = false

end
function negromancy()
ATTACK = true
sword2weld.Part0 = Torso
swordweld.Part0 = Torso
sword3weld.Part0 = Torso
sword4weld.Part0 = Torso

Auraweld.Part0 = Torso
scytheweld.Part0 = Torso

		CreateSound(10319073970, Torso, 100, 1, false)

		CreateSound(10319073970, Torso, 100, 1, false)
	sick.SoundId = "rbxassetid://1"
CHAT.Text = "You know this is just a stupid block game,"

for i=0, 5.5, 0.1 / Animation_Speed do
local rng = math.random(1,5)

if rng == 5 then
NAMEMODE.TextColor3 = Color3.fromRGB(5,0,10)
end

if rng == 4 then
NAMEMODE.TextColor3 = Color3.fromRGB(15,0,30)
end

if rng == 3 then
NAMEMODE.TextColor3 = Color3.fromRGB(15,0,30)
end

if rng == 2 then
NAMEMODE.TextColor3 = Color3.fromRGB(25,0,50)
end

if rng == 1 then
NAMEMODE.TextColor3 = Color3.fromRGB(0,0,0)
end
Swait()
Neck.C0 = Clerp(Neck.C0, CF(0*SIZE+0 * COS(SINE / 1), 1.0*SIZE+0 * COS(SINE / 1), -.0*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(60.3+3*math.cos(SINE/30)),math.rad(180+3*math.cos(SINE/28)),math.rad(-10.1+6*math.cos(SINE/25))), 1 / Animation_Speed)

	   sword2weld.C0 = Clerp(sword2weld.C0, CF(-4*SIZE-0 * COS(SINE / 15), 2*SIZE+.7 * math.cos(SINE / 21), 3*SIZE+0 * COS(SINE / 20)) * ANGLES(RAD(0+0 * COS(SINE / 505)), RAD(20+0* COS(SINE / 20)), RAD(-65+-13 * math.sin(SINE / 21))), 1 / Animation_Speed)
         swordweld.C0 = Clerp(swordweld.C0, CF(4*SIZE-0 * COS(SINE / 15), -0*SIZE+.7 * math.cos(SINE / 21), 3.0*SIZE+0 * COS(SINE / 20)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-20+0 * COS(SINE / 20)), RAD(155+13 * math.sin(SINE / 21))), 1 / Animation_Speed)

              sword3weld.C0 = Clerp(sword3weld.C0, CF(4*SIZE-0 * COS(SINE / 15), 2*SIZE+.7 * math.cos(SINE / 21), 3*SIZE+0 * COS(SINE / 20)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-20+0 * COS(SINE / 20)), RAD(155+13 * math.sin(SINE / 21))), 1 / Animation_Speed)
		    sword4weld.C0 = Clerp(sword4weld.C0, CF(-4*SIZE-0 * COS(SINE / 15), -0*SIZE+.7 * math.cos(SINE / 21), 3*SIZE+0* COS(SINE / 20)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(20+-0 * COS(SINE / 20)), RAD(-65+-13 * math.sin(SINE / 21))), 1 / Animation_Speed)

		   rockweld.C0 = Clerp(rockweld.C0, CF(-0*SIZE+0 * COS(SINE / 17), 3*SIZE+0* COS(SINE / 15), 4*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(0+0 * COS(SINE / 15)), RAD(-0+0 * COS(SINE / 1200)), RAD(0+2220 * COS(SINE / 110))), 1 / Animation_Speed)
		    Auraweld.C0 = Clerp(Auraweld.C0, CF(-0*SIZE+0 * COS(SINE / 17), -0*SIZE+0* COS(SINE / 15), 2*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(0+600 * COS(SINE / 105)), RAD(-0+700 * COS(SINE / 120)), RAD(0+2220 * COS(SINE / 110))), 1 / Animation_Speed)
	 scytheweld.C0 = Clerp(scytheweld.C0, CF(-0*SIZE+0 * COS(SINE / 17), -0*SIZE+0* COS(SINE / 15), 2*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(0+-600 * COS(SINE / 105)), RAD(-0+-700 * COS(SINE / 120)), RAD(0+-2220 * COS(SINE / 110))), 1 / Animation_Speed)

RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0*CFrame.new(0,0,0-.15*math.sin(SINE/25))*CFrame.Angles(math.rad(20),0,0),.25)

            RightShoulder.C0 = Clerp(RightShoulder.C0,CFrame.new(1.1,.5,-.5)*CFrame.Angles(math.rad(-180),.4,-19.3+.1*math.cos(SINE/25))*RIGHTSHOULDERC0,.25)
            LeftShoulder.C0 = Clerp(LeftShoulder.C0,CFrame.new(-1.1,.1,-1)*CFrame.Angles(math.rad(20),0,math.rad(90-10*math.cos(SINE/25)))*LEFTSHOULDERC0,.25)
            RightHip.C0 = Clerp(RightHip.C0,CFrame.new(1,-1.2+.15*math.sin(SINE/25),0)*CFrame.Angles(math.rad(-10),math.rad(80),math.rad(10+0*math.cos(SINE/25))),.25)
            LeftHip.C0 = Clerp(LeftHip.C0,CFrame.new(-1,-1.1+.15*math.sin(SINE/25),-0.5)*CFrame.Angles(math.rad(20),math.rad(-80),math.rad(-10-0*math.cos(SINE/25))),.25)


end
CHAT.Text = "Right??"

for i=0, 1.5, 0.1 / Animation_Speed do
    
Swait()

Neck.C0 = Clerp(Neck.C0, CF(0*SIZE+0 * COS(SINE / 1), 1.0*SIZE+0 * COS(SINE / 1), -.0*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(100.3+3*math.cos(SINE/30)),math.rad(180+3*math.cos(SINE/28)),math.rad(-0.1+6*math.cos(SINE/25))), 1 / Animation_Speed)
	   sword2weld.C0 = Clerp(sword2weld.C0, CF(-4*SIZE-0 * COS(SINE / 15), 2*SIZE+.7 * math.cos(SINE / 21), 3*SIZE+0 * COS(SINE / 20)) * ANGLES(RAD(0+0 * COS(SINE / 505)), RAD(20+0* COS(SINE / 20)), RAD(-65+-13 * math.sin(SINE / 21))), 1 / Animation_Speed)
         swordweld.C0 = Clerp(swordweld.C0, CF(4*SIZE-0 * COS(SINE / 15), -0*SIZE+.7 * math.cos(SINE / 21), 3.0*SIZE+0 * COS(SINE / 20)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-20+0 * COS(SINE / 20)), RAD(155+13 * math.sin(SINE / 21))), 1 / Animation_Speed)

              sword3weld.C0 = Clerp(sword3weld.C0, CF(4*SIZE-0 * COS(SINE / 15), 2*SIZE+.7 * math.cos(SINE / 21), 3*SIZE+0 * COS(SINE / 20)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-20+0 * COS(SINE / 20)), RAD(155+13 * math.sin(SINE / 21))), 1 / Animation_Speed)
		    sword4weld.C0 = Clerp(sword4weld.C0, CF(-4*SIZE-0 * COS(SINE / 15), -0*SIZE+.7 * math.cos(SINE / 21), 3*SIZE+0* COS(SINE / 20)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(20+-0 * COS(SINE / 20)), RAD(-65+-13 * math.sin(SINE / 21))), 1 / Animation_Speed)

		   rockweld.C0 = Clerp(rockweld.C0, CF(-0*SIZE+0 * COS(SINE / 17), 3*SIZE+0* COS(SINE / 15), 4*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(0+0 * COS(SINE / 15)), RAD(-0+0 * COS(SINE / 1200)), RAD(0+2220 * COS(SINE / 110))), 1 / Animation_Speed)
		    Auraweld.C0 = Clerp(Auraweld.C0, CF(-0*SIZE+0 * COS(SINE / 17), -0*SIZE+0* COS(SINE / 15), 2*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(0+600 * COS(SINE / 105)), RAD(-0+700 * COS(SINE / 120)), RAD(0+2220 * COS(SINE / 110))), 1 / Animation_Speed)
	 scytheweld.C0 = Clerp(scytheweld.C0, CF(-0*SIZE+0 * COS(SINE / 17), -0*SIZE+0* COS(SINE / 15), 2*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(0+-600 * COS(SINE / 105)), RAD(-0+-700 * COS(SINE / 120)), RAD(0+-2220 * COS(SINE / 110))), 1 / Animation_Speed)

RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0*CFrame.new(0,0,0-.15*math.sin(SINE/25))*CFrame.Angles(math.rad(20),0,0),.25)

            RightShoulder.C0 = Clerp(RightShoulder.C0,CFrame.new(1.5,.1,-0)*CFrame.Angles(math.rad(-220),.4,-18+.1*math.cos(SINE/25))*RIGHTSHOULDERC0,.25)
            LeftShoulder.C0 = Clerp(LeftShoulder.C0,CFrame.new(-1.1,.1,-1)*CFrame.Angles(math.rad(20),0,math.rad(90-10*math.cos(SINE/25)))*LEFTSHOULDERC0,.25)
            RightHip.C0 = Clerp(RightHip.C0,CFrame.new(1,-1.2+.15*math.sin(SINE/25),0)*CFrame.Angles(math.rad(-10),math.rad(80),math.rad(10+0*math.cos(SINE/25))),.25)
            LeftHip.C0 = Clerp(LeftHip.C0,CFrame.new(-1,-1.1+.15*math.sin(SINE/25),-0.5)*CFrame.Angles(math.rad(20),math.rad(-80),math.rad(-10-0*math.cos(SINE/25))),.25)


