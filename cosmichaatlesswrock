		   rockweld.C0 = Clerp(rockweld.C0, CF(3*SIZE+1.5 * COS(SINE / 55), 2*SIZE+1* COS(SINE / 55), -.0*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(0+0 * COS(SINE / 15)), RAD(-0+0 * COS(SINE / 1)), RAD(0+0 * COS(SINE / 1))), 1 / Animation_Speed)
