function ILattack()
ATTACK = true
for i=0, .55, 0.1 / Animation_Speed do
Swait()
               swordweld.C0 = Clerp(swordweld.C0, CF(-0*SIZE-0 * COS(SINE / 15), -5*SIZE-0 * COS(SINE / 8)+-(sick.PlaybackLoudness * 0.03), 3+.5*math.sin(SINE/25)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-0+0 * COS(SINE / 20)), RAD(45-0 * COS(SINE / 11)-(sick.PlaybackLoudness * 0))), 1 / Animation_Speed)
		    sword2weld.C0 = Clerp(sword2weld.C0, CF(-5*SIZE-0 * COS(SINE / 15)+-(sick.PlaybackLoudness * 0.03), 0*SIZE-0 * COS(SINE / 8), 3+.5*math.sin(SINE/25)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-0+-0 * COS(SINE / 20)), RAD(-45-0 * COS(SINE / 11)+(sick.PlaybackLoudness * 0))), 1 / Animation_Speed)

		   rockweld.C0 = Clerp(rockweld.C0, CF(-0*SIZE+1.5 * COS(SINE / 55), 2*SIZE+1* COS(SINE / 55), -.0*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(0+0 * COS(SINE / 15)), RAD(-0+0 * COS(SINE / 1)), RAD(0+0 * COS(SINE / 1))), 1 / Animation_Speed)
		    Auraweld.C0 = Clerp(Auraweld.C0, CF(-0*SIZE+0 * COS(SINE / 55), 2*SIZE+0 * COS(SINE / 55), 0*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(-90+0 * COS(SINE / 13)), RAD(-0+0 * COS(SINE / 90)), RAD(90+1000 * COS(SINE / 90))), 1 / Animation_Speed)
		     scytheweld.C0 = Clerp(scytheweld.C0, CF(0*SIZE+0 * COS(SINE / 35), -7.7*SIZE+0.5 * math.sin(SINE/50),-4*SIZE+0 * COS(SINE / 12)) * ANGLES(RAD(-70), RAD(-0+0* COS(SINE / 70)), RAD(90+365*math.rad(SINE/2))), 1 / Animation_Speed)

               sword3weld.C0 = Clerp(sword3weld.C0, CF(5*SIZE-0 * COS(SINE / 15)+(sick.PlaybackLoudness * 0.03), 0*SIZE-0 * COS(SINE / 8), 3+.5*math.sin(SINE/25)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-0+0 * COS(SINE / 20)), RAD(135-0 * COS(SINE / 11)-(sick.PlaybackLoudness * 0))), 1 / Animation_Speed)
		    sword4weld.C0 = Clerp(sword4weld.C0, CF(0*SIZE-0 * COS(SINE / 15), 5*SIZE-0 * COS(SINE / 8)+(sick.PlaybackLoudness * 0.03), 3+.5*math.sin(SINE/25)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-0+-0 * COS(SINE / 20)), RAD(-135-0 * COS(SINE / 11)+(sick.PlaybackLoudness * 0))), 1 / Animation_Speed)

RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0*CFrame.new(0,0,3-.5*math.sin(SINE/25))*CFrame.Angles(math.rad(20),0,0),.25)

            RightShoulder.C0 = Clerp(RightShoulder.C0,CFrame.new(1.5,.5,0)*CFrame.Angles(math.rad(-10),0,90)*RIGHTSHOULDERC0,.25)
            LeftShoulder.C0 = Clerp(LeftShoulder.C0,CFrame.new(-1.5,.5,0)*CFrame.Angles(math.rad(20),0,math.rad(-10-10*math.cos(SINE/25)))*LEFTSHOULDERC0,.25)
            RightHip.C0 = Clerp(RightHip.C0,CFrame.new(1,-1,0)*CFrame.Angles(math.rad(10),math.rad(80),math.rad(10+10*math.cos(SINE/25))),.25)
            LeftHip.C0 = Clerp(LeftHip.C0,CFrame.new(-1,-1,0)*CFrame.Angles(math.rad(20),math.rad(-80),math.rad(-10-10*math.cos(SINE/25))),.25)


end

for i=0, .55, 0.1 / Animation_Speed do
Swait()
Neck.C0 = Clerp(Neck.C0, CF(0*SIZE+0 * COS(SINE / 1), 1.0*SIZE+0 * COS(SINE / 1), -.0*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(90.3+3*math.cos(SINE/30)),math.rad(180+3*math.cos(SINE/28)),math.rad(-0.1+6*math.cos(SINE/25))), 1 / Animation_Speed)
               swordweld.C0 = Clerp(swordweld.C0, CF(-0*SIZE-0 * COS(SINE / 15), -5*SIZE-0 * COS(SINE / 8)+-(sick.PlaybackLoudness * 0.03), 3+.5*math.sin(SINE/25)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-0+0 * COS(SINE / 20)), RAD(45-0 * COS(SINE / 11)-(sick.PlaybackLoudness * 0))), 1 / Animation_Speed)
		    sword2weld.C0 = Clerp(sword2weld.C0, CF(-5*SIZE-0 * COS(SINE / 15)+-(sick.PlaybackLoudness * 0.03), 0*SIZE-0 * COS(SINE / 8), 3+.5*math.sin(SINE/25)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-0+-0 * COS(SINE / 20)), RAD(-45-0 * COS(SINE / 11)+(sick.PlaybackLoudness * 0))), 1 / Animation_Speed)

		   rockweld.C0 = Clerp(rockweld.C0, CF(-0*SIZE+1.5 * COS(SINE / 55), 2*SIZE+1* COS(SINE / 55), -.0*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(0+0 * COS(SINE / 15)), RAD(-0+0 * COS(SINE / 1)), RAD(0+0 * COS(SINE / 1))), 1 / Animation_Speed)
		    Auraweld.C0 = Clerp(Auraweld.C0, CF(-0*SIZE+0 * COS(SINE / 55), 2*SIZE+0 * COS(SINE / 55), 0*SIZE+0 * COS(SINE / 1)) * ANGLES(RAD(-90+0 * COS(SINE / 13)), RAD(-0+0 * COS(SINE / 90)), RAD(90+1000 * COS(SINE / 90))), 1 / Animation_Speed)
		     scytheweld.C0 = Clerp(scytheweld.C0, CF(0*SIZE+0 * COS(SINE / 35), -7.7*SIZE+0.5 * math.sin(SINE/50),-4*SIZE+0 * COS(SINE / 12)) * ANGLES(RAD(-70), RAD(-0+0* COS(SINE / 70)), RAD(90+365*math.rad(SINE/2))), 1 / Animation_Speed)

               sword3weld.C0 = Clerp(sword3weld.C0, CF(5*SIZE-0 * COS(SINE / 15)+(sick.PlaybackLoudness * 0.03), 0*SIZE-0 * COS(SINE / 8), 3+.5*math.sin(SINE/25)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-0+0 * COS(SINE / 20)), RAD(135-0 * COS(SINE / 11)-(sick.PlaybackLoudness * 0))), 1 / Animation_Speed)
		    sword4weld.C0 = Clerp(sword4weld.C0, CF(0*SIZE-0 * COS(SINE / 15), 5*SIZE-0 * COS(SINE / 8)+(sick.PlaybackLoudness * 0.03), 3+.5*math.sin(SINE/25)) * ANGLES(RAD(0-0 * COS(SINE / 11)), RAD(-0+-0 * COS(SINE / 20)), RAD(-135-0 * COS(SINE / 11)+(sick.PlaybackLoudness * 0))), 1 / Animation_Speed)

RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0*CFrame.new(0,0,3-.5*math.sin(SINE/25))*CFrame.Angles(math.rad(20),0,0),.25)
            RightShoulder.C0 = Clerp(RightShoulder.C0,CFrame.new(1.5,.5,0)*CFrame.Angles(math.rad(-20),40,90)*RIGHTSHOULDERC0,.25)
            LeftShoulder.C0 = Clerp(LeftShoulder.C0,CFrame.new(-1.5,.5,0)*CFrame.Angles(math.rad(20),0,math.rad(-10-10*math.cos(SINE/25)))*LEFTSHOULDERC0,.25)
            RightHip.C0 = Clerp(RightHip.C0,CFrame.new(1,-1,0)*CFrame.Angles(math.rad(10),math.rad(80),math.rad(10+10*math.cos(SINE/25))),.25)
            LeftHip.C0 = Clerp(LeftHip.C0,CFrame.new(-1,-1,0)*CFrame.Angles(math.rad(20),math.rad(-80),math.rad(-10-10*math.cos(SINE/25))),.25)


end
ATTACK = false
end

