            RightHip.C0 = Clerp(RightHip.C0, CF(1.0*SIZE+.1 * SIN(SINE / 32), -0*SIZE-.1 * COS(SINE / 32)-.05*math.sin(SINE/32), -0.5*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(0.8),math.rad(-10.1),math.rad(11.0+5*math.sin(SINE/32)))*ANGLES(math.rad(0),math.rad(90),math.rad(0)), 1 / Animation_Speed)
            LeftHip.C0 = Clerp(LeftHip.C0, CF(-1.0*SIZE+.1 * SIN(SINE / 32), -1.5*SIZE-.1 * COS(SINE / 32)+.05*math.sin(SINE/32), -0.5*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(60.6),math.rad(10.0),math.rad(-11.0-5*math.sin(SINE/32)))*ANGLES(math.rad(0),math.rad(-90),math.rad(0)), 1 / Animation_Speed)
	
        
     end 
    
     if randomnumber == 6 then 
            RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(-0*SIZE+0.3 * math.sin(SINE / 19), 0*SIZE+1.5 * COS(SINE / 26), 7*SIZE+4 * math.cos(SINE / 27)) * ANGLES(math.rad(42.6+15 * math.sin(SINE / 19.5)), RAD(-4.7+10 * math.sin(SINE / 18)), RAD(-0.2+0 * COS(SINE / 1))), 1 / Animation_Speed)
     	     RightHip.C0 = Clerp(RightHip.C0, CF(1.0*SIZE+0 * SIN(SINE / 12), -.4*SIZE-.1 * COS(SINE / 25)-.05*math.sin(SINE/12), -.6*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(0.8-32*math.sin(SINE/27)),math.rad(-15.1),math.rad(7.0+2*math.sin(SINE/26)))*ANGLES(math.rad(0),math.rad(90),math.rad(0)), 1 / Animation_Speed)
            LeftHip.C0 = Clerp(LeftHip.C0, CF(-1.0*SIZE+0 * SIN(SINE / 12), -1*SIZE-.1 * COS(SINE / 25)+.05*math.sin(SINE/12), 0.0*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(0.6-32*math.sin(SINE/27)),math.rad(35.0),math.rad(-7.0-2*math.sin(SINE/26)))*ANGLES(math.rad(0),math.rad(-90),math.rad(0)), 1 / Animation_Speed) 		
	
        
     end 
     if randomnumber == 7 then 
        
              RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(-0*SIZE+.2 * math.sin(SINE / 32), 0*SIZE+0 * COS(SINE / 1), -.4*SIZE+.1 * math.cos(SINE / 32)) * ANGLES(math.rad(25.6), RAD(-0.7+5 * math.sin(SINE / 32)), RAD(-45.2+0 * COS(SINE / 1))), 1 / Animation_Speed)
     	
            RightHip.C0 = Clerp(RightHip.C0, CF(0.9*SIZE+.1 * SIN(SINE / 32), -1*SIZE-.1 * COS(SINE / 32)-.05*math.sin(SINE/32)-rightlegheight, -.3*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(-1.8),math.rad(-20.1),math.rad(5.4-5*math.sin(SINE/32)))*ANGLES(math.rad(0),math.rad(90),math.rad(0)), 1 / Animation_Speed)
            LeftHip.C0 = Clerp(LeftHip.C0, CF(-1.4*SIZE+.1 * SIN(SINE / 32), -1*SIZE-.1 * COS(SINE / 32)+.05*math.sin(SINE/32)-leftlegheight, -0.5*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(-25.6),math.rad(23.2),math.rad(-15.5-5*math.sin(SINE/32)))*ANGLES(math.rad(0),math.rad(-90),math.rad(0)), 1 / Animation_Speed) 
		
     end 
     if randomnumber == 8 then 
        
         RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(-0*SIZE+0 * math.sin(SINE / 32), 0*SIZE+0 * COS(SINE / 1), -.2*SIZE+-.2 * math.cos(SINE / 32)) * ANGLES(math.rad(10.6+5 * math.sin(SINE / 32)), RAD(0.7), RAD(-90.2+0 * COS(SINE / 1))), 1 / Animation_Speed)

            RightHip.C0 = Clerp(RightHip.C0, CF(0.9*SIZE+0 * SIN(SINE / 32), -1*SIZE-.1 * COS(SINE / 32)-.05*math.sin(SINE/32)-rightlegheight, -.2*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(-1.8),math.rad(-20.1),math.rad(25.4-5*math.sin(SINE/32)))*ANGLES(math.rad(0),math.rad(90),math.rad(0)), 1 / Animation_Speed)
            LeftHip.C0 = Clerp(LeftHip.C0, CF(-1.3*SIZE+0 * SIN(SINE / 32), -.8*SIZE-.1 * COS(SINE / 32)+.05*math.sin(SINE/32)-leftlegheight, -0.1*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(-0.6),math.rad(10.2),math.rad(-17-5*math.sin(SINE/32)))*ANGLES(math.rad(0),math.rad(-90),math.rad(0)), 1 / Animation_Speed) 

     end 
     if randomnumber == 9 then 
        
          RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(0*SIZE+0 * math.sin(SINE / 32), 0*SIZE+0 * COS(SINE / 1), 1.5*SIZE+.6 * math.cos(SINE / 24)) * ANGLES(math.rad(-80.6+-15 * math.sin(SINE / 22)), RAD(-4.7+0 * math.sin(SINE / 32)), RAD(0.2+0 * COS(SINE / 1))), 1 / Animation_Speed)
  RightHip.C0 = Clerp(RightHip.C0, CF(1.2*SIZE+0 * SIN(SINE / 32), -.5*SIZE-.1 * COS(SINE / 32)-.05*math.sin(SINE/32), -1*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(-57.8+ 14 * math.sin(SINE / 31)),math.rad(-20.1),math.rad(-25.4-0*math.sin(SINE/32)))*ANGLES(math.rad(0),math.rad(90),math.rad(0)), 1 / Animation_Speed)
            LeftHip.C0 = Clerp(LeftHip.C0, CF(-1.2*SIZE+0 * SIN(SINE / 32), -1.4*SIZE-.1 * COS(SINE / 32)+.05*math.sin(SINE/32), 0.1*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(-10.6+ 14 * math.sin(SINE / 31)),math.rad(23.2),math.rad(25.5-0*math.sin(SINE/32)))*ANGLES(math.rad(0),math.rad(-90),math.rad(0)), 1 / Animation_Speed) 

     end 
      if randomnumber == 10 then 
         RootJoint.C0 = Clerp(RootJoint.C0,ROOTC0 * CF(-0*SIZE+0.3 * math.sin(SINE / 19), 0*SIZE+0 * COS(SINE / 1), 3*SIZE+1 * math.cos(SINE / 20)) * ANGLES(math.rad(-25.6+15 * math.sin(SINE / 27.5)), RAD(34.7+10 * math.sin(SINE / 25)), RAD(-40.2+0 * COS(SINE / 1))), 1 / Animation_Speed)
     	
            RightHip.C0 = Clerp(RightHip.C0, CF(1.6*SIZE+0 * SIN(SINE / 12), -.25*SIZE-.1 * COS(SINE / 12)-.05*math.sin(SINE/12), -1.2*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(-50.8),math.rad(-15.1),math.rad(-25.0+2*math.sin(SINE/26)))*ANGLES(math.rad(0),math.rad(90),math.rad(0)), 1 / Animation_Speed)
            LeftHip.C0 = Clerp(LeftHip.C0, CF(-1.0*SIZE+0 * SIN(SINE / 12), -1*SIZE-.1 * COS(SINE / 12)+.05*math.sin(SINE/12), 0.0*SIZE+0 * COS(SINE / 1)) * ANGLES(math.rad(0.6),math.rad(35.0),math.rad(-7.0-2*math.sin(SINE/26)))*ANGLES(math.rad(0),math.rad(-90),math.rad(0)), 1 / Animation_Speed) 		

        
     end 
    
    return randomnumber
    
    end 

function RandomArmsAnimation(randomnumber) 
    
    if randomnumber == 1 then 
        RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5, 0.5 + 0.12 * lookvel ^ 2 - movement / 40 * math.cos(SINE /4) / 3, movement / 150 + movement / 40 * math.cos(SINE /4)) * CFrame.Angles((math.rad(-5 - movement * 2 * math.cos(SINE /4)) + -(movement / 10) * math.sin(SINE /4)) * lookvel, math.rad(0 - movement * 2 * math.cos(SINE /4)), -math.rad(movement * 1 * 0.2 - movement * 1 * math.cos(SINE /2) * lookvel) + math.rad(-5 * rightvel)) * RIGHTSHOULDERC0, 1 / Animation_Speed)
LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.5 + 0.12 * lookvel ^ 2 + movement / 40 * math.cos(SINE /4) / 3, movement / 150 - movement / 40 * math.cos(SINE /4)) * CFrame.Angles((math.rad(-5 + movement * 2 * math.cos(SINE /4)) + movement / 10 * math.sin(SINE /4)) * lookvel, math.rad(0 - movement * 2 * math.cos(SINE /4)), -math.rad(-(movement * 1) * 0.2 + movement * 1 * math.cos(SINE /2) * lookvel) + math.rad(-5 * rightvel)) * LEFTSHOULDERC0, 1 / Animation_Speed)

        
        end 
     if randomnumber == 2 then 
        		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(.7, -.2 - 0.1 * math.cos(SINE / 48) / 3, -0.6 - 0.05 * math.cos(SINE / 16)) * CFrame.Angles(math.rad(0 + 0 * math.sin(SINE / 16)), math.rad(-25 + 6 * math.sin(SINE / 16)), math.rad(-109 + 0 * math.cos(SINE / 32))) * RIGHTSHOULDERC0, .2)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1, 0 - 0.1 * math.cos(SINE / 48) / 3, -0.6 - 0.05 * math.cos(SINE / 16)) * CFrame.Angles(math.rad(90 + 0 * math.sin(SINE / 16)), math.rad(-0.2 - 7 * math.sin(SINE / 32)), math.rad(55 + 0 * math.cos(SINE / 32))) * LEFTSHOULDERC0, .2)			
          
        
     end 
     if randomnumber == 3 then 
        		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.6, 0.-0 - 0.1 * math.cos(SINE / 16) / 3, 0.5 - 0.05 * math.cos(SINE / 16)) * CFrame.Angles(math.rad(135 + 4 * math.sin(SINE / 16)), math.rad(-0 + 5 * math.sin(SINE / 16)), math.rad(15 - 7.6 * math.cos(SINE / 16))) * RIGHTSHOULDERC0, .2)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.5, 0.4 - 0.1 * math.cos(SINE / 16) / 3, 0.0 - 0.05 * math.cos(SINE / 16)) * CFrame.Angles(math.rad(25 + 4 * math.sin(SINE / 16)), math.rad(-15 - 5 * math.sin(SINE / 16)), math.rad(-45 + 7.6 * math.cos(SINE / 48))) * LEFTSHOULDERC0, .2)
	
        
     end 
     if randomnumber == 4 then 
              	RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.3, 0.1 - 0.1 * math.cos(SINE / 16) / 3, -0.5- 0.05 * math.cos(SINE / 16)) * CFrame.Angles(math.rad(70 + 4 * math.sin(SINE / 16)), math.rad(25 + 5 * math.sin(SINE / 16)), math.rad(-45 - 3.0 * math.cos(SINE / 16))) * RIGHTSHOULDERC0, .2)
			LeftShoulder.C0 = Clerp(LeftShoulder.C0, CF(-1.3, 0.2 - 0.1 * math.cos(SINE / 16) / 3, -0.5 - 0.05 * math.cos(SINE / 16)) * CFrame.Angles(math.rad(100 + 4 * math.sin(SINE / 16)), math.rad(-25 - 0 * math.sin(SINE / 16)), math.rad(25 + 0.0 * math.cos(SINE / 16))) * LEFTSHOULDERC0, .2)		

        
     end 
     if randomnumber == 5 then 
