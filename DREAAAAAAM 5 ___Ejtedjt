function Parents()
	RootJoint.Parent = RootPart
	Neck.Parent = Torso
	RightShoulder.Parent = Torso
	LeftShoulder.Parent = Torso
	RightHip.Parent = Torso
	LeftHip.Parent = Torso
	RootPart.Parent = Character
	LeftArm.Parent = Character
	RightArm.Parent = Character
	RightLeg.Parent = Character
	LeftLeg.Parent = Character
	Torso.Parent = Character
	Head.Parent = Character
	Effects.Parent = Character
end
local States = {
	"FallingDown";
	"PlatformStanding";
	"Physics";
	"Swimming";
	"Dead";
	"Ragdoll";
	"Seated";
}
for i,v in pairs(States) do
	Humanoid:SetStateEnabled(v,false)
end

--//=================================\\
--||        CHAT FUNCTION
--\\=================================//

local FONTS = {
	Enum.Font.Antique,
	Enum.Font.Arcade,
	Enum.Font.Arial,
	Enum.Font.ArialBold,
	Enum.Font.Bodoni,
	Enum.Font.Cartoon,
	Enum.Font.Code,
	Enum.Font.Fantasy,
	Enum.Font.Garamond,
	Enum.Font.Highway,
	Enum.Font.Legacy,
	Enum.Font.SciFi,
	Enum.Font.SourceSans,
	Enum.Font.SourceSansBold,
	Enum.Font.SourceSansItalic,
	Enum.Font.SourceSansLight,
	Enum.Font.SourceSansSemibold
}
local chatting = false
function chatfunc(text)
	local chat = coroutine.wrap(function()
		chatting = true
		if Character:FindFirstChild("TalkingBillBoard")~= nil then
			Character:FindFirstChild("TalkingBillBoard"):destroy()
		end
		local Bill = Instance.new("BillboardGui",Character)
		Bill.Size = UDim2.new(0,100,0,40)
		Bill.StudsOffset = VT(0,5,0)
		Bill.Adornee = Character.Head
		Bill.Name = "TalkingBillBoard"
		local Hehe = Instance.new("TextLabel",Bill)
		Hehe.BackgroundTransparency = 1
		Hehe.BorderSizePixel = 0
		Hehe.Text = ""
		Hehe.Font = "Bodoni"
		Hehe.TextSize = 40
		Hehe.TextStrokeTransparency = 0
		Hehe.Size = UDim2.new(1,0,0.5,0)
		coroutine.resume(coroutine.create(function()
			while Hehe ~= nil do
				Swait(2.5)	
				Hehe.Font = FONTS[MRANDOM(1, #FONTS)]
				Hehe.Position = UDim2.new(0, 0, .05, 0)
				local val = MRANDOM(1,255)
				Hehe.TextColor3 = color
				Hehe.TextStrokeColor3 = color
			end
		end))
		for i = 1,string.len(text),1 do
			Swait()
			Hehe.Text = string.sub(text,1,i)
		end
		Swait(90)
		for i = 0, 1, .025 do
			Swait()
			Hehe.TextStrokeTransparency = i
			Hehe.TextTransparency = i
		end
		Bill:Destroy()
		chatting = false
	end)
	chat()
end
ModeName1 = Instance.new("BillboardGui",Character)
ModeName1.AlwaysOnTop = true
ModeName1.Size = UDim2.new(7,35,3,15)
ModeName1.StudsOffset = Vector3.new(0,2,0)
ModeName1.MaxDistance = 10000
ModeName1.Adornee = Head
ModeName1.Name = "Name2"
ModeName2 = Instance.new("TextLabel",ModeName1)
ModeName2.BackgroundTransparency = 1
ModeName2.TextScaled = true
ModeName2.BorderSizePixel = 0
ModeName2.Text = "DR34M5 - "..madenam
ModeName2.Font = "Arcade"
ModeName2.TextSize = 35
ModeName2.TextStrokeTransparency = 0
ModeName2.Size = UDim2.new(1,0,0.5,0)
ModeName2.Parent = ModeName1
ModeName2.TextColor3 = Color3.fromRGB()

function onChatted(msg)
	chatfunc(msg)
end

Player.Chatted:Connect(onChatted)

