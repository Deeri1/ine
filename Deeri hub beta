local function KILHDFO_fake_script() -- silentrejointoggle.LocalScript 
	local script = Instance.new('LocalScript', silentrejointoggle)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent["silent rejoin toggle"].BackgroundColor3 == Color3.fromHSV(0, 0, 1)== true then
			script.Parent.Parent["silent rejoin toggle"].BackgroundColor3 = Color3.fromHSV(0, 0, 0)
		elseif script.Parent.Parent["silent rejoin toggle"].BackgroundColor3 == Color3.fromHSV(0, 0, 0)== true then
			script.Parent.Parent["silent rejoin toggle"].BackgroundColor3 = Color3.fromHSV(0, 0, 1)
			end
	
	end)
end
coroutine.wrap(KILHDFO_fake_script)()
