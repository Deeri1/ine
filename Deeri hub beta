			Color3.fromHSV(0, 0, 0)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.openup.BackgroundColor3 =
			Color3.fromHSV(0.111111, 1, 1)
	end)
	
	script.Parent.MouseButton1Click:Connect(function()
		mainui.openup.TextColor3 =
			Color3.fromHSV(0, 0, 0)
	end)
	
end
coroutine.wrap(NXCNIR_fake_script)()
local function HVLIE_fake_script() -- closeforcolorsettings.closecolorsettings 
	local script = Instance.new('LocalScript', closeforcolorsettings)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(HVLIE_fake_script)()
