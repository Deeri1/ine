local function KPJJHQO_fake_script() -- animator.closepage.Script 
	local script = Instance.new('Script', animator.closepage)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.leftarm.Visible == true then
			script.Parent.Parent.leftarm.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.rightleg.Visible == true then
			script.Parent.Parent.rightleg.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.torso.Visible == true then
			script.Parent.Parent.torso.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.leftleg.Visible == true then
			script.Parent.Parent.leftleg.Visible = false
			script.Parent.Parent.mainmian.Visible = true	
		elseif script.Parent.Parent.rightarm.Visible == true then
			script.Parent.Parent.rightarm.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.hat1.Visible == true then
			script.Parent.Parent.hat1.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		else
			script.Parent.Parent.Parent.main:Destroy()
		end
	end)
	
end
coroutine.wrap(KPJJHQO_fake_script)()
