	local script = Instance.new('Script', leftarm_2)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.mainmian.Visible = false
		script.Parent.Parent.Parent.leftarm.Visible = true
	end)
end
coroutine.wrap(NZBPX_fake_script)()
local function VAPDRRZ_fake_script() -- animation.LocalScript 
	local script = Instance.new('LocalScript', animation)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.layouts.Visible = true
	end)
end
coroutine.wrap(VAPDRRZ_fake_script)()
local function BVTU_fake_script() -- CLOSE.LocalScript 
	local script = Instance.new('LocalScript', CLOSE)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(BVTU_fake_script)()
