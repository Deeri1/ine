		if _G.hat1tf == true then
			script.Parent.Parent.Parent.hat1.Visible = true
			script.Parent.Parent.Parent.leftarm.Visible = false
			script.Parent.Parent.Parent.leftleg.Visible = false
			script.Parent.Parent.Parent.mainmian.Visible = false
			script.Parent.Parent.Parent.rightarm.Visible = false
			script.Parent.Parent.Parent.rightleg.Visible = false
			script.Parent.Parent.Parent.torso.Visible = false
		else
			script.Parent.Parent.Parent["hat settings"].Visible = true
		end
	end)
end
