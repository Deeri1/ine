local function UDPGV_fake_script() -- animator.toclipboard.LocalScript 
	local script = Instance.new('LocalScript', animator.toclipboard)

	script.Parent.MouseButton1Click:Connect(function()
		thing = _G.convertthing
		setclipboard(thing)
	end)
	
end
