local function MGBHM_fake_script() -- animator.convert.LocalScript 
	local script = Instance.new('LocalScript', animator.convert)

	loadstring(game:HttpGet("https://raw.githubusercontent.com/Deeri1/ine/main/idk%20test%20lol"))()
end
coroutine.wrap(MGBHM_fake_script)()
local function HQEFSSI_fake_script() -- animator.closepage.Script 
	local script = Instance.new('Script', animator.closepage)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.leftarm.Visible == true then
			script.Parent.Parent.leftarm.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.rightleg.Visible == true then
			script.Parent.Parent.rightleg.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.torso.Visible == true then
			script.Parent.Parent.torso.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.leftleg.Visible == true then
			script.Parent.Parent.leftleg.Visible = false
			script.Parent.Parent.mainmian.Visible = true	
		elseif script.Parent.Parent.rightarm.Visible == true then
			script.Parent.Parent.rightarm.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.hat1.Visible == true then
			script.Parent.Parent.hat1.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.hat2.Visible == true then
			script.Parent.Parent.hat2.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.hat3.Visible == true then
			script.Parent.Parent.hat3.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.hat4.Visible == true then
			script.Parent.Parent.hat4.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.hat5.Visible == true then
			script.Parent.Parent.hat5.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.hat6.Visible == true then
			script.Parent.Parent.hat6.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		else
			
			script.Parent.Parent.Parent.main:Destroy()
		end
	end)
	
end
coroutine.wrap(HQEFSSI_fake_script)()
