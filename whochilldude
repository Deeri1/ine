-- Scripts:

local function NIERCN_fake_script() -- toclipboard.LocalScript 
	local script = Instance.new('LocalScript', toclipboard)

	script.Parent.MouseButton1Click:Connect(function()
		thing = _G.convertthing
		setclipboard(thing)
	end)
	
end
coroutine.wrap(NIERCN_fake_script)()
local function KYIFJX_fake_script() -- close.LocalScript 
	local script = Instance.new('LocalScript', close)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(KYIFJX_fake_script)()
local function MYXQJB_fake_script() -- convert_2.convert script 
	local script = Instance.new('Script', convert_2)

	player = game.Players.LocalPlayer
