-- Scripts:

local function NKJFJV_fake_script() -- animator.toclipboard.LocalScript 
	local script = Instance.new('LocalScript', animator.toclipboard)

	script.Parent.MouseButton1Click:Connect(function()
		thing = _G.convertthing
		setclipboard(thing)
	end)
	
end
coroutine.wrap(NKJFJV_fake_script)()
local function GMOHI_fake_script() -- animator.close.LocalScript 
	local script = Instance.new('LocalScript', animator.close)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(GMOHI_fake_script)()
local function LXVSAJW_fake_script() -- animator.TextButton.LocalScript 
	local script = Instance.new('LocalScript', animator.TextButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent["convert text"].Text = _G.hate1
	end)
end
coroutine.wrap(LXVSAJW_fake_script)()
local function REFDFD_fake_script() -- animator.TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', animator.TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent["convert text"].Text = _G.hate2
	end)
end
coroutine.wrap(REFDFD_fake_script)()
local function KFDZ_fake_script() -- animator.convert_2.LocalScript 
	local script = Instance.new('LocalScript', animator.convert_2)

	loadstring(game:HttpGet("https://raw.githubusercontent.com/Deeri1/ine/main/idk%20test%20lol"))()
end
coroutine.wrap(KFDZ_fake_script)()
local function WTPCY_fake_script() -- animator.closepage.Script 
	local script = Instance.new('Script', animator.closepage)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.leftarm.Visible == true then
			script.Parent.Parent.leftarm.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.rightleg.Visible == true then
			script.Parent.Parent.rightleg.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.torso.Visible == true then
			script.Parent.Parent.torso.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.leftleg.Visible == true then
			script.Parent.Parent.leftleg.Visible = false
			script.Parent.Parent.mainmian.Visible = true	
		elseif script.Parent.Parent.rightarm.Visible == true then
			script.Parent.Parent.rightarm.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.hat1.Visible == true then
			script.Parent.Parent.hat1.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.hat2.Visible == true then
			script.Parent.Parent.hat2.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		else
			script.Parent.Parent.Parent.main:Destroy()
		end
	end)
	
end
coroutine.wrap(WTPCY_fake_script)()
local function LZCALIF_fake_script() -- animator.leftkeg.Script 
