-- Scripts:

local function ZFEO_fake_script() -- convert.convert script 
	local script = Instance.new('Script', convert)

	
	
	
	_G.SelexityNet = true
	_G.StrongerNet = false-- you hats will not lag other that if your moving super fast.
	_G.Net = false -- your regular net. hats may lag behind if moving fast.
	_G.Bot = false -- bot reanimate
	_G.WeakNet = false --a weak net with little jitter
	loadstring(game:HttpGet("https://raw.githubusercontent.com/DeeriHub/Deeri-Hub/main/Deeri%20net"))()
	wait(2.5)
	player = game.Players.LocalPlayer
	Character = player.Character
	player = game.Players.LocalPlayer
	Player = player
	Character = Player.Character
	LeftArm=Character["Left Arm"] 
	LeftLeg=Character["Left Leg"] 
	RightArm=Character["Right Arm"] 
	RightLeg=Character["Right Leg"] 
	PlayerGui=Player.PlayerGui 
	Backpack=Player.Backpack 
	Torso=Character.Torso 
	Head=Character.Head 
	LW=Torso["Left Shoulder"] 
	LH=Torso["Left Hip"] 
	RW=Torso["Right Shoulder"] 
	RH=Torso["Right Hip"] 
	LH=Torso["Left Hip"]
	RH=Torso["Right Hip"]
	ch = Character
	Humanoid = ch.Humanoid
	Humanoid.Animator.Parent = nil
	RootPart=Character.HumanoidRootPart
	RootJoint=RootPart.RootJoint
	cf = CFrame.new
	angles=CFrame.Angles
	euler=CFrame.fromEulerAnglesXYZ
	RootCF=euler(-1.57,0,3.14)
	necko=cf(0, 1, 0, -1, -0, -0, 0, 0, 1, 0, 1, 0)
	necko2=cf(0, -0.5, 0, -1, -0, -0, 0, 0, 1, 0, 1, 0)
	LHC0=cf(-1,-1,0,-0,-0,-1,0,1,0,1,0,0)
	LHC1=cf(-0.5,1,0,-0,-0,-1,0,1,0,1,0,0)
	RHC0=cf(1,-1,0,0,0,1,0,1,0,-1,-0,-0)
	RHC1=cf(0.5,1,0,0,0,1,0,1,0,-1,-0,-0)
	function clerp(a,b,t) 
		local qa = {QuaternionFromCFrame(a)}
		local qb = {QuaternionFromCFrame(b)} 
		local ax, ay, az = a.x, a.y, a.z 
		local bx, by, bz = b.x, b.y, b.z
		local _t = 1-t
		return QuaternionToCFrame(_t*ax + t*bx, _t*ay + t*by, _t*az + t*bz,QuaternionSlerp(qa, qb, t)) 
	end 
	function QuaternionFromCFrame(cf) 
		local mx, my, mz, m00, m01, m02, m10, m11, m12, m20, m21, m22 = cf:components() 
		local trace = m00 + m11 + m22 
		if trace > 0 then 
			local s = math.sqrt(1 + trace) 
			local recip = 0.5/s 
			return (m21-m12)*recip, (m02-m20)*recip, (m10-m01)*recip, s*0.5 
		else 
			local i = 0 
			if m11 > m00 then
				i = 1
			end
			if m22 > (i == 0 and m00 or m11) then 
				i = 2 
			end 
			if i == 0 then 
				local s = math.sqrt(m00-m11-m22+1) 
				local recip = 0.5/s 
				return 0.5*s, (m10+m01)*recip, (m20+m02)*recip, (m21-m12)*recip 
			elseif i == 1 then 
				local s = math.sqrt(m11-m22-m00+1) 
				local recip = 0.5/s 
				return (m01+m10)*recip, 0.5*s, (m21+m12)*recip, (m02-m20)*recip 
			elseif i == 2 then 
				local s = math.sqrt(m22-m00-m11+1) 
				local recip = 0.5/s return (m02+m20)*recip, (m12+m21)*recip, 0.5*s, (m10-m01)*recip 
			end 
		end 
	end
	
	function QuaternionToCFrame(px, py, pz, x, y, z, w) 
		local xs, ys, zs = x + x, y + y, z + z 
		local wx, wy, wz = w*xs, w*ys, w*zs 
		local xx = x*xs 
		local xy = x*ys 
		local xz = x*zs 
		local yy = y*ys 
		local yz = y*zs 
		local zz = z*zs 
		return CFrame.new(px, py, pz,1-(yy+zz), xy - wz, xz + wy,xy + wz, 1-(xx+zz), yz - wx, xz - wy, yz + wx, 1-(xx+yy)) 
	end
	
	function QuaternionSlerp(a, b, t) 
		local cosTheta = a[1]*b[1] + a[2]*b[2] + a[3]*b[3] + a[4]*b[4] 
		local startInterp, finishInterp; 
		if cosTheta >= 0.0001 then 
			if (1 - cosTheta) > 0.0001 then 
				local theta = math.acos(cosTheta) 
				local invSinTheta = 1/math.sin(theta) 
				startInterp = math.sin((1-t)*theta)*invSinTheta 
				finishInterp = math.sin(t*theta)*invSinTheta  
			else 
				startInterp = 1-t 
				finishInterp = t 
			end 
		else 
			if (1+cosTheta) > 0.0001 then 
				local theta = math.acos(-cosTheta) 
				local invSinTheta = 1/math.sin(theta) 
				startInterp = math.sin((t-1)*theta)*invSinTheta 
				finishInterp = math.sin(t*theta)*invSinTheta 
			else 
				startInterp = t-1 
				finishInterp = t 
			end 
		end 
		return a[1]*startInterp + b[1]*finishInterp, a[2]*startInterp + b[2]*finishInterp, a[3]*startInterp + b[3]*finishInterp, a[4]*startInterp + b[4]*finishInterp 
	end
	
	player = game.Players.LocalPlayer
	Character = player.Character
	
	script.Parent.MouseButton1Click:Connect(function()
		player = game.Players.LocalPlayer
		Character = player.Character
		rhx = script.Parent.Parent.rightleg.rhxt.Text
		rhy = script.Parent.Parent.rightleg.rhyt.Text
		rhz = script.Parent.Parent.rightleg.rhzt.Text
		rrhx = script.Parent.Parent.rightleg.rrhxt.Text
		rrhy = script.Parent.Parent.rightleg.rrhyt.Text
		rrhz = script.Parent.Parent.rightleg.rrhzt.Text
		lhx = script.Parent.Parent.leftleg.rhxt.Text
		lhy = script.Parent.Parent.leftleg.rhyt.Text
		lhz = script.Parent.Parent.leftleg.rhzt.Text
		lrhx = script.Parent.Parent.leftleg.rrhxt.Text
		lrhy = script.Parent.Parent.leftleg.rrhyt.Text
		lrhz = script.Parent.Parent.leftleg.rrhzt.Text
		rahx = script.Parent.Parent.rightarm.rhxt.Text
		rahy = script.Parent.Parent.rightarm.rhyt.Text
		rahz = script.Parent.Parent.rightarm.rhzt.Text
		rarhx = script.Parent.Parent.rightarm.rrhxt.Text
		rarhy = script.Parent.Parent.rightarm.rrhyt.Text
		rarhz = script.Parent.Parent.rightarm.rrhzt.Text
		lahx = script.Parent.Parent.leftarm.rhxt.Text
		lahy = script.Parent.Parent.leftarm.rhyt.Text
		lahz = script.Parent.Parent.leftarm.rhzt.Text
		larhx = script.Parent.Parent.leftarm.rrhxt.Text
		larhy = script.Parent.Parent.leftarm.rrhyt.Text
		larhz = script.Parent.Parent.leftarm.rrhzt.Text
		tahx = script.Parent.Parent.torso.rhxt.Text
		tahy = script.Parent.Parent.torso.rhyt.Text
		tahz = script.Parent.Parent.torso.rhzt.Text
		tarhx = script.Parent.Parent.torso.rrhxt.Text
		tarhy = script.Parent.Parent.torso.rrhyt.Text
		tarhz = script.Parent.Parent.torso.rrhzt.Text
		_G.convertthing = ("RH.C0=clerp(RH.C0,cf("..rhx..","..rhy..","..rhz..")*angles(math.rad("..rrhx.."),math.rad("..rrhy.."),math.rad("..rrhz..")),.2)\nLH.C0=clerp(LH.C0,cf("..lhx..","..lhy..","..lhz..")*angles(math.rad("..lrhx.."),math.rad("..lrhy.."),math.rad("..lrhz..")),.2)\nRootJoint.C0=clerp(RootJoint.C0,RootCF*cf("..tahx..","..tahy..","..tahz..")*angles(math.rad("..tarhx.."),math.rad("..tarhy.."),math.rad("..tarhz..")),.3)\nTorso.Neck.C0=clerp(Torso.Neck.C0,necko*angles(math.rad(0),math.rad(0),math.rad(0)),.3)\nRW.C0=clerp(RW.C0,cf("..rahx..","..rahy..","..rahz..")*angles(math.rad("..rarhx.."),math.rad("..rarhy.."),math.rad("..rarhz..")),.3)\nLW.C0=clerp(LW.C0,cf("..lahx..","..lahy..","..lahz..")*angles(math.rad("..larhx.."),math.rad("..larhy..")),math.rad("..larhz..")),.3)")
	
	
	
		
		print("converting")
		_G.convertthin2 = [[
		RH.C0=clerp(RH.C0,cf(rhx,rhy,rhz)*angles(math.rad(rrhx),math.rad(rrhy),math.rad(rrhz)),.2)
		LH.C0=clerp(LH.C0,cf(lhx,lhy,lhz)*angles(math.rad(lrhx),math.rad(lrhy),math.rad(lrhz)),.2)
		RootJoint.C0=clerp(RootJoint.C0,RootCF*cf(tahx,tahy,tahz)*angles(math.rad(tarhx),math.rad(tarhy),math.rad(tarhz)),.3)
		Torso.Neck.C0=clerp(Torso.Neck.C0,necko*angles(math.rad(0),math.rad(0),math.rad(0)),.3)
		RW.C0=clerp(RW.C0,cf(rahx,rahy,rahz)*angles(math.rad(rarhx),math.rad(rarhy),math.rad(rarhz)),.3)
		LW.C0=clerp(LW.C0,cf(lahx,lahy,lahz)*angles(math.rad(larhx),math.rad(larhy),math.rad(larhz)),.3)"]]
		print(_G.convertthing)
		script.Parent.Parent.Parent.convert["convert text"].Text = _G.convertthing
		script.Parent.Parent.Parent.convert.Visible = true
	end)
	while true do 
		wait()
	wait()
	rhx = script.Parent.Parent.rightleg.rhxt.Text
	rhy = script.Parent.Parent.rightleg.rhyt.Text
	rhz = script.Parent.Parent.rightleg.rhzt.Text
	rrhx = script.Parent.Parent.rightleg.rrhxt.Text
	rrhy = script.Parent.Parent.rightleg.rrhyt.Text
	rrhz = script.Parent.Parent.rightleg.rrhzt.Text
		lhx = script.Parent.Parent.leftleg.rhxt.Text
		lhy = script.Parent.Parent.leftleg.rhyt.Text
		lhz = script.Parent.Parent.leftleg.rhzt.Text
		lrhx = script.Parent.Parent.leftleg.rrhxt.Text
		lrhy = script.Parent.Parent.leftleg.rrhyt.Text
		lrhz = script.Parent.Parent.leftleg.rrhzt.Text
			rahx = script.Parent.Parent.rightarm.rhxt.Text
			rahy = script.Parent.Parent.rightarm.rhyt.Text
			rahz = script.Parent.Parent.rightarm.rhzt.Text
			rarhx = script.Parent.Parent.rightarm.rrhxt.Text
			rarhy = script.Parent.Parent.rightarm.rrhyt.Text
			rarhz = script.Parent.Parent.rightarm.rrhzt.Text
				lahx = script.Parent.Parent.leftarm.rhxt.Text
				lahy = script.Parent.Parent.leftarm.rhyt.Text
				lahz = script.Parent.Parent.leftarm.rhzt.Text
				larhx = script.Parent.Parent.leftarm.rrhxt.Text
				larhy = script.Parent.Parent.leftarm.rrhyt.Text
				larhz = script.Parent.Parent.leftarm.rrhzt.Text
					tahx = script.Parent.Parent.torso.rhxt.Text
					tahy = script.Parent.Parent.torso.rhyt.Text
					tahz = script.Parent.Parent.torso.rhzt.Text
					tarhx = script.Parent.Parent.torso.rrhxt.Text
					tarhy = script.Parent.Parent.torso.rrhyt.Text
					tarhz = script.Parent.Parent.torso.rrhzt.Text
	wait()
	RH.C0=clerp(RH.C0,cf(rhx,rhy,rhz)*angles(math.rad(rrhx),math.rad(rrhy),math.rad(rrhz)),.2)
	LH.C0=clerp(LH.C0,cf(lhx,lhy,lhz)*angles(math.rad(lrhx),math.rad(lrhy),math.rad(lrhz)),.2)
	RootJoint.C0=clerp(RootJoint.C0,RootCF*cf(tahx,tahy,tahz)*angles(math.rad(tarhx),math.rad(tarhy),math.rad(tarhz)),.3)
	Torso.Neck.C0=clerp(Torso.Neck.C0,necko*angles(math.rad(0),math.rad(0),math.rad(0)),.3)
	RW.C0=clerp(RW.C0,cf(rahx,rahy,rahz)*angles(math.rad(rarhx),math.rad(rarhy),math.rad(rarhz)),.3)
	LW.C0=clerp(LW.C0,cf(lahx,lahy,lahz)*angles(math.rad(larhx),math.rad(larhy),math.rad(larhz)),.3)
	end
end
coroutine.wrap(ZFEO_fake_script)()
local function UKWDJB_fake_script() -- closepage.Script 
	local script = Instance.new('Script', closepage)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.leftarm.Visible == true then
			script.Parent.Parent.leftarm.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.rightleg.Visible == true then
			script.Parent.Parent.rightleg.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.torso.Visible == true then
			script.Parent.Parent.torso.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.leftleg.Visible == true then
			script.Parent.Parent.leftleg.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		elseif script.Parent.Parent.rightarm.Visible == true then
			script.Parent.Parent.rightarm.Visible = false
			script.Parent.Parent.mainmian.Visible = true
		else
			script.Parent.Parent.Parent.main:Destroy()
		end
	end)
end
coroutine.wrap(UKWDJB_fake_script)()
local function TWBFAEM_fake_script() -- leftkeg.Script 
	local script = Instance.new('Script', leftkeg)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.mainmian.Visible = false
		script.Parent.Parent.Parent.leftleg.Visible = true
	end)
end
coroutine.wrap(TWBFAEM_fake_script)()
