				while wait(1.225) and mode == 5 do
					if seizure2 then
						seizure2 = false
					else
						seizure2 = true
					end
				end
			end)()
		elseif mode == 6 then
			text.Text = "RaiNBoW"
			text.TextColor3 = Color3.new(1, 1, 1)
			text.TextStrokeColor3 = Color3.new(r/255, g/255, b/255)
