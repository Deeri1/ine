mouse.KeyDown:connect(function(k)
	if k == "q" and attack == false and ModeOfGlitch ~= 1 then
		switc(BrickColor.new("Really black"),BrickColor.new("Hurrican grey"))
		ModeOfGlitch = 1
		storehumanoidWS = 30
		hum.WalkSpeed = 30
		rainbowmode = false
		chaosmode = false
		newTheme("rbxassetid://1422064574",0,1,5)
		RecolorTextAndRename("VIGOR",BrickColor.new("Really black").Color,BrickColor.new("Hurrican grey").Color,"Arcade")
		MAINRUINCOLOR = BrickColor.new("Really black")
		RecolorThing(MAINRUINCOLOR,BrickColor.new("Hurricane grey"),MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,true)
	end
	if k == "e" and attack == false and ModeOfGlitch ~= 2 then
		switc(BrickColor.new("Institutional white"),BrickColor.new("Light blue"))
		ModeOfGlitch = 2
		storehumanoidWS = 160
		hum.WalkSpeed = 160
		rainbowmode = false
		chaosmode = false
		RecolorTextAndRename("PARADOX",BrickColor.new("Light blue").Color,BrickColor.new("Institutional white").Color,"Arcade")
		newTheme("rbxassetid://2090890309",0,1,5)
		MAINRUINCOLOR = BrickColor.new("Institutional white")
		RecolorThing(MAINRUINCOLOR,BrickColor.new("Light blue"),MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,true)
	end
	if k == "r" and attack == false and ModeOfGlitch ~= 3 then
		switc(BrickColor.new("Royal purple"),BrickColor.new("Hot pink"))
		ModeOfGlitch = 3
		storehumanoidWS = 130
		hum.WalkSpeed = 130
		rainbowmode = false
		chaosmode = false
		RecolorTextAndRename("TRIBULATION",BrickColor.new("Royal purple").Color,BrickColor.new("Hot pink").Color,"Arcade")
		newTheme("rbxassetid://635908787",0,1,5)
		MAINRUINCOLOR = BrickColor.new("Hot pink")
		RecolorThing(MAINRUINCOLOR,BrickColor.new("Royal purple"),MAINRUINCOLOR,MAINRUINCOLOR,MAINRUINCOLOR,1,MAINRUINCOLOR,1,MAINRUINCOLOR,true)
	end
	if k == "t" and attack == false and ModeOfGlitch ~= 4 then
		switc(BrickColor.new("Pastel violet"),BrickColor.new("Dark stone grey"))
		ModeOfGlitch = 4
		storehumanoidWS = 100
		hum.WalkSpeed = 100
		rainbowmode = false
		chaosmode = false
		RecolorTextAndRename("NEUROTIC",BrickColor.new("Dark stone grey").Color,BrickColor.new("Pastel violet").Color,"Arcade")
		newTheme("rbxassetid://4749054738",0,1,5)
		MAINRUINCOLOR = BrickColor.new("Pastel violet")
