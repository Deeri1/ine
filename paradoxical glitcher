local attacktype = 1
mouse.Button1Down:connect(function()
	if attack == false and attacktype == 1 then
		attacktype = 2
		attackone()
	elseif attack == false and attacktype == 2 then
		attacktype = 3
		--attacktwo()
	elseif attack == false and attacktype == 3 then
		attacktype = 1
		--attackthree()
	elseif attack == false and attacktype == 4 then
		attacktype = 1
		--attackfour()
	end
end)

switc(BrickColor.new("Really black"),BrickColor.new("Hurrican grey"))
ModeOfGlitch = 1
storehumanoidWS = 30
hum.WalkSpeed = 30
newTheme("rbxassetid://1422064574",0,1,5)
